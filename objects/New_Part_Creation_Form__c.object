IF(ISPICKVAL( NPC_Form_Status__c, &apos;Pending AM Approval&apos;),&apos;Pending AM Approval&apos;,
IF(AND(ISPICKVAL(NPC_Form_Status__c, &apos;In Execution&apos;),ISBLANK(Planned_Implementation_Date__c)), &apos;Pending Assessment&apos;,
IF(AND(ISPICKVAL(NPC_Form_Status__c, &apos;In Execution&apos;),NOT(ISBLANK(Planned_Implementation_Date__c))), &apos;In Execution&apos;,
IF(ISPICKVAL(NPC_Form_Status__c, &apos;Not Executed&apos;), &apos;Not Executed&apos;,
IF(ISPICKVAL(NPC_Form_Status__c, &apos;Executed&apos;), &apos;Executed&apos;,&apos;&apos;))
)))))))</formula>
