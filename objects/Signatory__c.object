    <validationRules>
        <fullName>SinglePartyAddingNewContacts</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT(ISPICKVAL(Agreement__r.Apttus__Status__c, &apos;Activated&apos;)), 
RecordType.DeveloperName == &apos;NDA_to_Account_Tagging_Only&apos;, 

(Agreement__r.RecordType.Name == &quot;DS Multi-Party NDA&quot; || Agreement__r.RecordType.Name == &quot;Multi Party NDA&quot; || Agreement__r.RecordType.Name == &quot;Other Party NDA&quot; || Agreement__r.RecordType.Name == &quot;DS Other Party NDA&quot;) 
)</errorConditionFormula>
        <errorMessage>The agreement with account tagging only after status activate or To add additional Accounts for this NDA, navigate to section 1.1 to add Other Party Accounts</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SinglePartyNonTagging</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
ISPICKVAL(Agreement__r.Apttus__Status__c, &apos;Activated&apos;),
RecordType.DeveloperName == &apos;Agreement_Signatory&apos;
)</errorConditionFormula>
        <errorMessage>The agreement signatory should not be created after activated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Single_Party_Agreement</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
  ISNEW(),
  RecordType.DeveloperName = &apos;Agreement_Signatory&apos;,    
OR(
  Agreement__r.RecordType.DeveloperName =  &apos;GLOBALFOUNDRIES_NDA&apos;,
 Agreement__r.RecordType.DeveloperName = &apos;DS_2_Party_NDA&apos;,
 Agreement__r.RecordType.DeveloperName = &apos;TD_2_Party_NDA&apos;,
 Agreement__r.RecordType.DeveloperName = &apos;GENERAL_2_Party_NDA&apos;),
 ISPICKVAL(Agreement__r.Apttus__Status_Category__c, &apos;In Effect&apos;))</errorConditionFormula>
        <errorMessage>You selected a 2-Party NDA, if you need a Multi-Party NDA, cancel this request and create a Multi-Party NDA</errorMessage>
    </validationRules>
