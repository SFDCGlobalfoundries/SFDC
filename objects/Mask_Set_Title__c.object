    <validationRules>
        <fullName>MPW_MST_classification_can_not_be_modifi</fullName>
        <active>true</active>
        <errorConditionFormula>AND(LEFT(Name, 3)==&apos;MPW&apos;, 
ISCHANGED( Mask_Set_Classification__c),
NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c), 
NOT($Setup.IntegrationUsers__c.Is_Integration_User__c), 
$Profile.Name &lt;&gt; &apos;GF Integration&apos;)</errorConditionFormula>
        <errorMessage>Mask Set Classification for Global shuttle MPW can not be modified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mask_Set_Title_should_not_start_with_MPW</fullName>
        <active>true</active>
        <errorConditionFormula>AND(LEFT(Name, 3)==&apos;MPW&apos;, Manual_Creation__c,
Not($Permission.GShuttle_team_access),
NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c), 
NOT($Setup.IntegrationUsers__c.Is_Integration_User__c), 
$Profile.Name &lt;&gt; &apos;GF Integration&apos;)</errorConditionFormula>
        <errorMessage>Mask Set Title Should not start with MPW</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Number_of_Prime_Die_not_equal_to_Zero</fullName>
        <active>true</active>
        <errorConditionFormula>AND((Number_of_Prime_Die__c  &lt;= 0),
NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c), 
NOT($Setup.IntegrationUsers__c.Is_Integration_User__c), 
$Profile.Name &lt;&gt; &apos;GF Integration&apos;)</errorConditionFormula>
        <errorDisplayField>Number_of_Prime_Die__c</errorDisplayField>
        <errorMessage>Number of Prime Die should be greater than Zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reticle_Type_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Reticle_Type__c, &apos;&apos;),
NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c), 
NOT($Setup.IntegrationUsers__c.Is_Integration_User__c), 
$Profile.Name &lt;&gt; &apos;GF Integration&apos;)</errorConditionFormula>
        <errorDisplayField>Reticle_Type__c</errorDisplayField>
        <errorMessage>Reticle Type is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRMaskSetTitleOnlyUppercaseAlphaNumeric</fullName>
        <active>true</active>
        <description>Mask Set Title must be alphanumeric (0-9, A-Z) and is capital letter and max 7 characters.</description>
        <errorConditionFormula>AND(NOT(ISBLANK(Name )), OR( NOT( REGEX( Name , &quot;[A-Z0-9]+&quot;) ), LEN( Name ) &gt; 7 ),
NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c), 
NOT($Setup.IntegrationUsers__c.Is_Integration_User__c), 
$Profile.Name &lt;&gt; &apos;GF Integration&apos;)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Mask Set Title must be alphanumeric (0-9, A-Z) and is capital letter and max 7 characters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidationOnMaskSetTitleOnlyUppercase</fullName>
        <active>false</active>
        <description>Mask set title should be all in upper case</description>
        <errorConditionFormula>NOT(REGEX( Name  , &quot;[A-Z]+&quot;))</errorConditionFormula>
        <errorMessage>Mask Set Title should be all in Upper Case</errorMessage>
    </validationRules>
