    <validationRules>
        <fullName>OutofOffice_Data_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>(User__c  =  Delegate_User__c)</errorConditionFormula>
        <errorDisplayField>Delegate_User__c</errorDisplayField>
        <errorMessage>Delegate User has to be different form User.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OutofOffice_EndDate_greater_StartDate</fullName>
        <active>true</active>
        <errorConditionFormula>End_Time__c &lt;=  Start_Time__c</errorConditionFormula>
        <errorDisplayField>End_Time__c</errorDisplayField>
        <errorMessage>End Time has to be after Start Time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OutofOffice_End_Date_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(DATEVALUE(End_Time__c ) &lt; DATEVALUE(NOW()), ISNEW() ),
AND(DATEVALUE(End_Time__c ) &lt; DATEVALUE(NOW()),ISCHANGED(End_Time__c ))
)</errorConditionFormula>
        <errorDisplayField>End_Time__c</errorDisplayField>
        <errorMessage>End Date Should Not Be An Older Date and Time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OutofOffice_Start_Date_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(DATEVALUE(Start_Time__c) &lt; DATEVALUE(NOW()), ISNEW() ),
AND(DATEVALUE(Start_Time__c) &lt; DATEVALUE(NOW()), ISCHANGED(Start_Time__c))
)</errorConditionFormula>
        <errorDisplayField>Start_Time__c</errorDisplayField>
        <errorMessage>Start Date Should Not Be An Older Date and Time</errorMessage>
    </validationRules>
