    <validationRules>
        <fullName>ExternalUserValidation</fullName>
        <active>true</active>
        <description>FV2:External user can not add internal user as Asssigned to</description>
        <errorConditionFormula>AND 
( 
NOT(ISPICKVAL($User.UserType,&apos;Standard&apos;)), 
ISPICKVAL(assignTo__r.UserType,&apos;Standard&apos;) 
)</errorConditionFormula>
        <errorDisplayField>assignTo__c</errorDisplayField>
        <errorMessage>External user can not add internal user as assigned to</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GFTaskDueDtNotInPast</fullName>
        <active>true</active>
        <description>FV2:Due date cannot be past date.</description>
        <errorConditionFormula>Due_Date__c  &lt; Today()</errorConditionFormula>
        <errorDisplayField>Due_Date__c</errorDisplayField>
        <errorMessage>Due date cannot be past date. Enter valid date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReasonForVoidValidation</fullName>
        <active>true</active>
        <description>FV2:Enter Reason for Void</description>
        <errorConditionFormula>AND 
( 
ISPICKVAL(Status__c,&apos;Void&apos;), 
($User.Id = OwnerId || $User.Id= assignTo__r.Id), 
ISBLANK(Reason_for_Void__c) 
)</errorConditionFormula>
        <errorDisplayField>Reason_for_Void__c</errorDisplayField>
        <errorMessage>Enter Reason for Void</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VoidOnlyForUserOrAssignee</fullName>
        <active>true</active>
        <description>FV2 : Used for checking to select the Void status only for User or Assignee.</description>
        <errorConditionFormula>AND 
( 
ISPICKVAL(Status__c,&apos;Void&apos;), 
NOT($User.Id = OwnerId || $User.Id= assignTo__r.Id) 
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Status Can be Void only for Owner and Assignee</errorMessage>
    </validationRules>
