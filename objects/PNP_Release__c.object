    <validationRules>
        <fullName>PNP_Release_allow_to_complete</fullName>
        <active>true</active>
        <description>Make sure all required fields are filled out before allowing Complete to be checked</description>
        <errorConditionFormula>/* alert when complete box is checked and any of the progress fields isn&apos;t filled out */
AND(
  Complete__c  = TRUE,
  OR(
    OR(
      ISBLANK( Lot_Selected__c ),
      ISNULL( Lot_Selected__c )
    ),
    Acknowledge__c = FALSE /*,
    LEN( Progress_Comments__c ) = 0 */
  )
)</errorConditionFormula>
        <errorMessage>The Acknowledge checkbox, and Selected Lot must be set before this can be marked Complete</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PNP_Release_no_OSAT_fields_before_Accept</fullName>
        <active>true</active>
        <description>Make sure OSAT fields aren&apos;t set before OSAT Accepted stage</description>
        <errorConditionFormula>AND(
NOT( $User.Email = &apos;tim.curtin@globalfoundries.com&apos; ),
NOT( $User.Email = &apos;dellboomi.support@globalfoundries.com&apos; ), 
OR(
   ISPICKVAL( PNP_Release_Stage__c , &quot;New&quot;),
   ISPICKVAL( PNP_Release_Stage__c , &quot;Sent&quot;),
   ISNEW()
),
OR(
  NOT(
    OR(
      ISBLANK( Lot_Selected__c ),
      ISNULL( Lot_Selected__c )
    )
  ),
  Acknowledge__c = TRUE,
  No_Lot_Avail_pretest_hold_set__c  = TRUE,
  Complete__c  = TRUE,
  NOT(
    LEN( OSAT_Comments__c ) = 0
  )
)
)</errorConditionFormula>
        <errorMessage>The No Lot Avail, Selected Lot, and OSAT Comments cannot be set before OSAT has Approved the PNP Release</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PNP_Release_no_taking_back_complete</fullName>
        <active>true</active>
        <description>Ensure complete field can&apos;t be uncompleted</description>
        <errorConditionFormula>AND(
  Complete__c = FALSE,
  NOT( $User.Email = &apos;tim.curtin@globalfoundries.com&apos;),
  NOT( $User.Email = &apos;dellboomi.support@globalfoundries.com&apos; ), 
  ISCHANGED(  Complete__c ),
  NOT ( $User.Email =  Approved_For_MFG_By__c )
)</errorConditionFormula>
        <errorDisplayField>Complete__c</errorDisplayField>
        <errorMessage>Completion can&apos;t be removed from a record by anyone other than the person who marked it complete.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PNP_Release_nochanges_after_OSAT_Done</fullName>
        <active>true</active>
        <description>Ensure no Info or OSAT fields are changed after OSAT sends back to TAE</description>
        <errorConditionFormula>AND(
  ISPICKVAL( PNP_Release_Stage__c , &quot;OSAT Done&quot;) ,
  NOT( $User.Email = &apos;tim.curtin@globalfoundries.com&apos; ),
  NOT( $User.Email = &apos;dellboomi.support@globalfoundries.com&apos; ), 
  OR(
    ISCHANGED(  Priority__c ),
    ISCHANGED(   ATdb_ProductInfo__c    ),
    ISCHANGED(  Name  ),
    ISCHANGED(  Release_Comment__c ),
    ISCHANGED(  Lot_Requested__c ),
    ISCHANGED(  Lot_Size__c  ),
    ISCHANGED( Characterization_Percentage__c ),
    ISCHANGED(  Product_Name__c  ),
    ISCHANGED(   Stop_Production__c   ),
    ISCHANGED( Acknowledge__c ),
    ISCHANGED( No_Lot_Avail_pretest_hold_set__c ),
    ISCHANGED( Lot_Selected__c),
    ISCHANGED( OSAT_Comments__c) /*,
    ISCHANGED( Completion_Comments__c) */
  )
)</errorConditionFormula>
        <errorMessage>Information and OSAT fields can&apos;t be changed once a record has been sent to TAE for final approval.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PNP_Release_nochanges_after_sent_to_osat</fullName>
        <active>true</active>
        <description>Ensure Basic Info fields aren&apos;t changed after record is sent to OSAT for acceptance</description>
        <errorConditionFormula>AND(
NOT(ISPICKVAL( PNP_Release_Stage__c , &quot;New&quot;)),
OR(
ISCHANGED(  Priority__c ),
ISCHANGED(   ATdb_ProductInfo__c    ),
ISCHANGED(  Name  ),
ISCHANGED(  Release_Comment__c ),
ISCHANGED(  Lot_Requested__c ),
ISCHANGED(  Lot_Size__c  ),
ISCHANGED( Characterization_Percentage__c ),
ISCHANGED(  Product_Name__c  ),
ISCHANGED(   Stop_Production__c   )
),
NOT( $User.Email = &apos;tim.curtin@globalfoundries.com&apos; ),
NOT( $User.Email = &apos;dellboomi.support@globalfoundries.com&apos; )
)</errorConditionFormula>
        <errorMessage>The Basic Information fields for a record can&apos;t be changed after it is submitted to OSAT for Approval.  In order to edit: Recall the Approval, or ask OSAT to Reject if they&apos;ve already accepted.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PNP_Release_valid_prodname</fullName>
        <active>true</active>
        <description>Ensure that product name is entered and valid</description>
        <errorConditionFormula>AND(
ISBLANK(  ATdb_ProductInfo__c  ),
NOT ( $User.Email = &apos;tim.curtin@globalfoundries.com&apos;),
NOT( $User.Email = &apos;dellboomi.support@globalfoundries.com&apos; )
)</errorConditionFormula>
        <errorMessage>Please choose a Product using the ATdb Product lookup field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PNP_Release_valid_releasename</fullName>
        <active>false</active>
        <description>Ensure that pnp release name entered is in format AGMS.71.00001DV163.RELBSDQFMA</description>
        <errorConditionFormula>AND(
NOT(REGEX( Name , &quot;[A-Z]{4}\\.[0-9]{2}\\.[A-Z0-9]{10}\\.[A-Z0-9]{10}&quot;) ),
NOT( $User.Email = &apos;tim.curtin@globalfoundries.com&apos;),
NOT( $User.Email = &apos;dellboomi.support@globalfoundries.com&apos; )
)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>PNP Release name must be in the format AGMS.71.00001DV163.RELBSDQFMA</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
