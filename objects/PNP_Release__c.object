    <fields>
        <fullName>Ready_to_Submit_to_TAE__c</fullName>
        <description>This PNP Release is ready to submit for TAE Approval once Lot Selected is complete, any comments have been added, and PNP is ready for correlation by TAE.</description>
        <externalId>false</externalId>
        <formula>/*
New (saved, not submitted by TAE) 
Sent (sent to, but not approved by, OSAT) 
OSAT Accepted (OSAT has approved incoming object) 
OSAT Done (OSAT submitted results for TAE approval) 
Complete (TAE approved, PNP Release ready for mfg)
*/
IMAGE( 
  IF (
   ispickval(  PNP_Release_Stage__c, &quot;New&quot; ),
   &quot;/img/samples/flag_red.gif&quot;,
   IF (
    ispickval(  PNP_Release_Stage__c, &quot;Sent&quot; ),
    &quot;/img/samples/flag_red.gif&quot;,
    IF (
     AND (
      ispickval(  PNP_Release_Stage__c, &quot;OSAT Accepted&quot; ),
      NOT(ISBLANK( Lot_Selected__c ))
     ), &quot;/img/samples/flag_green.gif&quot;,
     IF (
      AND (
       ispickval(  PNP_Release_Stage__c, &quot;OSAT Done&quot; ),
       NOT(ISBLANK( Lot_Selected__c ))
      ), &quot;/img/samples/flag_red.gif&quot;,
      IF (
       ispickval(  PNP_Release_Stage__c, &quot;Complete&quot; ),
       &quot;/s.gif&quot;,
       &quot;/img/samples/flag_red.gif&quot;
      )
     )
    )
   )
  ),
&quot;Submit Ready Flag&quot;) &amp; &quot; &quot; &amp;
IF (OR (
   ispickval(  PNP_Release_Stage__c, &quot;New&quot; ),
   ispickval(  PNP_Release_Stage__c, &quot;Sent&quot; ),
   ispickval(  PNP_Release_Stage__c, &quot;OSAT Done&quot; ),
   ispickval(  PNP_Release_Stage__c, &quot;Complete&quot; )
  ), &quot;(Only valid after OSAT Approves release from TAE, and before OSAT Submits completion back to TAE)&quot;,
  IF (ISBLANK( Lot_Selected__c ),&quot;(Lot Selected needs a value)&quot;,
  &quot;Ready to Submit for Approval to TAE!&quot;
)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>This PNP Release is ready to submit for TAE Approval once Lot Selected is complete, any comments have been added, and PNP is ready for correlation by TAE.</inlineHelpText>
        <label>Ready to Submit to TAE</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
