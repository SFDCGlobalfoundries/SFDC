<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Settings that determine the behavior of the AIA/Siebel integration retry logic:</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Log__c</fullName>
        <description>log of the last scheduled job run</description>
        <externalId>false</externalId>
        <label>Log</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>MaxRecordsPerObject__c</fullName>
        <defaultValue>5</defaultValue>
        <description>The max number of records per object (Account, Opportunity, Acct Team) that can be processed at a time.</description>
        <externalId>false</externalId>
        <label>MaxRecordsPerObject</label>
        <precision>3</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>MinutesToResendRecord__c</fullName>
        <defaultValue>30</defaultValue>
        <description>The minimum time (in minutes) before a record will be resent for integration.  Retry is triggered only there is an error or AIA does respond with a reply.</description>
        <externalId>false</externalId>
        <inlineHelpText>The minimum time (in minutes) before a record will be resent for integration.  Retry is triggered only there is an error or AIA does respond with a reply.</inlineHelpText>
        <label>MinutesToResendRecord</label>
        <precision>6</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Time_to_Recall__c</fullName>
        <defaultValue>60</defaultValue>
        <description>How often the scheduler will run (in minutes) to trigger the integration retry process.</description>
        <externalId>false</externalId>
        <inlineHelpText>How often the scheduler will run (in minutes) to trigger the integration retry process.</inlineHelpText>
        <label>Time to Recall</label>
        <precision>6</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>Retry Logic</label>
    <visibility>Public</visibility>
</CustomObject>
