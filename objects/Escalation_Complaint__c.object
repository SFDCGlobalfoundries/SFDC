        <fullName>Expected_Date_can_not_be_in_Past</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISNEW(), Expected_Close_Date__c &lt;  TODAY())</errorConditionFormula>
        <errorDisplayField>Expected_Close_Date__c</errorDisplayField>
        <errorMessage>Expected Close Date can not be in a past date for new Actions.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Resolution_required_when_Stage_is_Closed</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ISPICKVAL(Stage__c , &quot;Closed&quot;) ,  ( ISBLANK(Resolution__c) ||  ISBLANK( Assigned_Resolution_Owner__c ))  )</errorConditionFormula>
        <errorMessage>Please provide &quot;Resolution Owner&quot; and &quot;Resolution&quot; When closing the Action.</errorMessage>
