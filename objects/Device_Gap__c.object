        <fullName>Status_P_BgColor__c</fullName>
        <externalId>false</externalId>
        <formula>IF (CONTAINS (TEXT (Previous_Status__c), &quot;G -&quot;), &apos;background-color: green;&apos;, 
IF (CONTAINS (TEXT(Previous_Status__c), &quot;Y -&quot;), &apos;background-color: yellow;&apos;, 
IF (CONTAINS (TEXT (Previous_Status__c), &quot;R1 -&quot;), &apos;background-color: red;&apos;, 
IF (CONTAINS (TEXT (Previous_Status__c), &quot;R2 -&quot;), &apos;background-color: red;&apos;, 
IF (CONTAINS (TEXT (Previous_Status__c), &quot;R3 -&quot;), &apos;background-color: red;&apos;, 
IF (CONTAINS (TEXT (Previous_Status__c), &quot;B -&quot;), &apos;background-color: blue;&apos;,&quot;&quot;))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
