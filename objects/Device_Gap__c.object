        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <externalId>false</externalId>
        <formula>IF (CONTAINS (TEXT (Select_Status__c), &quot;Y -&quot;), IMAGE (&quot;/resource/Dot_Y&quot;,&quot;&quot;,16,18),
    IF (CONTAINS (TEXT(Select_Status__c), &quot;G -&quot;), IMAGE (&quot;/resource/Dot_G&quot;,&quot;&quot;,16,18),
       IF (CONTAINS (TEXT (Select_Status__c), &quot;R1 -&quot;), IMAGE (&quot;/resource/Dot_R1&quot;,&quot;&quot;,16,18),
           IF (CONTAINS (TEXT (Select_Status__c), &quot;R2 -&quot;), IMAGE (&quot;/resource/Dot_R2&quot;,&quot;&quot;,16,18),
              IF (CONTAINS (TEXT (Select_Status__c), &quot;R3 -&quot;), IMAGE (&quot;/resource/Dot_R3&quot;,&quot;&quot;,16,18),
				  IF (CONTAINS (TEXT (Select_Status__c), &quot;M -&quot;), IMAGE (&quot;/resource/Dot_M&quot;,&quot;&quot;,16,18),
					 IF (CONTAINS (TEXT (Select_Status__c), &quot;B -&quot;), IMAGE (&quot;/resource/Dot_B&quot;,&quot;&quot;,16,18),&quot;&quot;)))))))</formula>
        <inlineHelpText>Use &quot;Select Status&quot; field to change the color code. This is Current Status.</inlineHelpText>
