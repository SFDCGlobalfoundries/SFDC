    <webLinks>
        <fullName>New_Opportunity_Issue</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New Opportunity Issue</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/connection.js&quot;)}
if(&apos;{!Opportunity.StageName}&apos;==&apos;8. Lost&apos; || &apos;{!Opportunity.StageName}&apos;==&apos;9. Dropped&apos; || &apos;{!Opportunity.StageName}&apos;==&apos;10. Void&apos;){
   alert(&apos;Opportunity Issue record cannot be created if Opportunity stage is 8,9 or 10 as either Opportunity Program is not created or Opportunity is Void&apos;);
}else{
     sforce.connection.sessionId = &apos;{!$Api.Session_ID}&apos;;
    var result = sforce.connection.describeSObjects([&apos;Device_Gap__c&apos;]);
    var clientPrefix = result[0].keyPrefix;    
    window.open(&apos;/&apos;+clientPrefix+&apos;/e?CF{!$Label.Device_Gap_Opportunity_Fieldid}={!Opportunity.Name}&amp;CF{!$Label.Device_Gap_Opportunity_Fieldid}_lkid={!Opportunity.Id}&amp;retURL=%2F{!Opportunity.Id}&apos;);
}</url>
    </webLinks>
