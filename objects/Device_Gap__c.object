    <validationRules>
        <fullName>Improve_Probability_Percentage_Creation</fullName>
        <active>false</active>
        <description>Impr%(C) + Opp Probability (%) is not more than 100% on Create/Edit of &quot;Opportunity Issue&quot; record</description>
        <errorConditionFormula>AND(ISNEW(),
(Improve_Prob__c + 
BLANKVALUE(Opportunity__r.Total_Improvement_Percentage__c, 0) + 
Opportunity__r.Probability ) &gt; 1.00
)</errorConditionFormula>
        <errorDisplayField>Improve_Prob__c</errorDisplayField>
        <errorMessage>Sum of Impr%(C) and Opportunity Probability (%) Can&apos;t be more than 100%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Improve_Probability_Percentage_Update</fullName>
        <active>false</active>
        <errorConditionFormula>AND
(
ISCHANGED(Improve_Prob__c),
(Sum_of_Probability__c-( Copy_of_Impr_C__c - Improve_Prob__c)) &gt; 1.00
)</errorConditionFormula>
        <errorDisplayField>Improve_Prob__c</errorDisplayField>
        <errorMessage>Sum of Impr%(C) for all Opportunity Issue and Opportunity Probability (%) Can&apos;t be more than 100%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Issue_Creation</fullName>
        <active>false</active>
        <description>Opportunity_Issue_Creation Cannot be done if its in Discovery</description>
        <errorConditionFormula>ISNEW() &amp;&amp;  ISPICKVAL(  Opportunity__r.StageName , &quot;1. Discovery&quot;)</errorConditionFormula>
        <errorMessage>Opportunity Issues can only be create in Stage 2 or later</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_and_Opportunity_Program_Name</fullName>
        <active>true</active>
        <errorConditionFormula>Opportunity_Program__r.Opportunity__c  &lt;&gt;  Opportunity__c</errorConditionFormula>
        <errorMessage>Opportunity program should be part of selected opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Device_Gap_Comment</fullName>
        <active>false</active>
        <description>Requires device gap comment when device gap checkbox is selected.

Turned off as per TASK-199.</description>
        <errorConditionFormula>AND( Device_Gap__c, ISBLANK( Device_Gap_Comment__c ))</errorConditionFormula>
        <errorDisplayField>Device_Gap_Comment__c</errorDisplayField>
        <errorMessage>Device Gap Comment is required when Device Gap checkbox is selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stage_Not_NONE</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Stage__c, &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Stage__c</errorDisplayField>
        <errorMessage>Stage cannot be None</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Unrequire_Device_Gap_Comment</fullName>
        <active>false</active>
        <description>Do not require device gap comment when device gap checkbox is unselected.

Turned off as per TASK-199.</description>
        <errorConditionFormula>AND( NOT(Device_Gap__c), NOT(ISBLANK( Device_Gap_Comment__c )))</errorConditionFormula>
        <errorDisplayField>Device_Gap_Comment__c</errorDisplayField>
        <errorMessage>Device Gap Comment is only required when Device Gap checkbox is selected.</errorMessage>
    </validationRules>
