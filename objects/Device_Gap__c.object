IF (CONTAINS (TEXT(Previous_Status__c), &quot;Y -&quot;), &quot;B&quot;, 
IF (CONTAINS (TEXT (Previous_Status__c), &quot;R1 -&quot;), &quot;A3&quot;, 
IF (CONTAINS (TEXT (Previous_Status__c), &quot;R2 -&quot;), &quot;A2&quot;, 
IF (CONTAINS (TEXT (Previous_Status__c), &quot;R3 -&quot;), &quot;A1&quot;, 
IF (CONTAINS (TEXT (Previous_Status__c), &quot;B -&quot;), &quot;C&quot;,&quot;&quot;))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>StatusP with sorting</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Status_BgColor__c</fullName>
        <externalId>false</externalId>
        <formula>IF (CONTAINS (TEXT (Select_Status__c), &quot;G -&quot;), &apos;background-color: green;&apos;, 
IF (CONTAINS (TEXT(Select_Status__c), &quot;Y -&quot;), &apos;background-color: yellow;&apos;, 
IF (CONTAINS (TEXT (Select_Status__c), &quot;R1 -&quot;), &apos;background-color: red;&apos;, 
IF (CONTAINS (TEXT (Select_Status__c), &quot;R2 -&quot;), &apos;background-color: red;&apos;, 
IF (CONTAINS (TEXT (Select_Status__c), &quot;R3 -&quot;), &apos;background-color: red;&apos;, 
IF (CONTAINS (TEXT (Select_Status__c), &quot;B -&quot;), &apos;background-color: blue;&apos;,
IF (CONTAINS (TEXT (Select_Status__c), &quot;M -&quot;), &apos;background-color: black;&apos;,&quot;&quot;)))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Status BgColor</label>
