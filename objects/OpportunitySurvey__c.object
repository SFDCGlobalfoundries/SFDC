    </searchLayouts>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>OpportunitySurveyZeroCheck</fullName>
        <active>true</active>
        <description>Ensure at least one survey question is marked as non-zero for Lost , Survey and Dropped</description>
        <errorConditionFormula>OR(
AND( ISPICKVAL(SurveyFor__c, &apos;Lost&apos;) ,Customer_Relationship__c == 0,Slow_GF_Responsiveness__c == 0,Poor_Perception_Confidence_of_GF__c== 0 ,Wafer_or_Die_Pricing__c == 0,Non_wafer_Pricing_NRE__c==0,Turnkey_Support__c==0,Capacity_Limitation__c ==0,MPW_Prototyping_Gap__c==0,Design_Partner_Gap__c==0,IP_gap__c == 0,Process_gap_device_performanc__c == 0,Incomplete_PDK_Modeling_Support__c ==0,Inadequate_technology_Process_maturit__c == 0,Quality_Issue__c == 0,GF_is_not_the_incumbent__c ==0)
, 
AND(ISPICKVAL(SurveyFor__c, &apos;Dropped&apos;),Pricing_Margin_too_Low__c == 0,PLC_rejection__c == 0 ,No_Capacity_to_Support__c==0,No_Technology_Match__c == 0,Customer_Project_Cancelled__c == 0, Process_EOL__c == 0,Device_EOL__c == 0,Unsupported_Market_Segment__c == 0,Customer_Stability__c == 0,Failed_ASICS_or_SiPh_grading__c == 0)
, 
AND(ISPICKVAL(SurveyFor__c, &apos;Void&apos;),Duplicate_Opportunity__c==0,Entered_in_Error__c==0) 
)</errorConditionFormula>
        <errorMessage>Ensure at least one survey question is marked as non-zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rating_3_Or_4_Mandatory_Comments_Check</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(ISPICKVAL(SurveyFor__c, &apos;Lost&apos;), 
OR(
AND(OR(Customer_Relationship__c ==3, Customer_Relationship__c==4),  ISBLANK(Customer_Relationship_Text__c)), 
AND( OR(Slow_GF_Responsiveness__c==3,Slow_GF_Responsiveness__c==4),ISBLANK(Slow_GF_Responsiveness_Text__c)), 
AND( OR(Poor_Perception_Confidence_of_GF__c==3,Poor_Perception_Confidence_of_GF__c==4),ISBLANK(Poor_Perception_Confidence_of_GF_Text__c)),
AND( OR(Wafer_or_Die_Pricing__c==3,Wafer_or_Die_Pricing__c==4),ISBLANK(Wafer_or_Die_Pricing_Text__c)),
AND( OR(Non_wafer_Pricing_NRE__c==3,Non_wafer_Pricing_NRE__c==4),ISBLANK(Non_wafer_Pricing_NRE_Text__c)),
AND( OR(Turnkey_Support__c==3,Turnkey_Support__c==4),ISBLANK(Turnkey_Support_Text__c)),
AND( OR(Capacity_Limitation__c==3,Capacity_Limitation__c==4),ISBLANK(Capacity_Limitation_Text__c)),
AND( OR(MPW_Prototyping_Gap__c==3,MPW_Prototyping_Gap__c==4),ISBLANK(MPW_Prototyping_Gap_Text__c)),
AND( OR(Design_Partner_Gap__c==3,Design_Partner_Gap__c==4),ISBLANK(Design_Partner_Gap_Text__c)),
AND( OR(IP_gap__c==3,IP_gap__c==4),ISBLANK(IP_Gap_Text__c)),
AND( OR(Process_gap_device_performanc__c==3,Process_gap_device_performanc__c==4),ISBLANK(Process_Gap_Device_or_Performance_Text__c)),
AND( OR(Incomplete_PDK_Modeling_Support__c==3,Incomplete_PDK_Modeling_Support__c==4),ISBLANK(Incomplete_PDK_Modeling_Support_Text__c)),
AND( OR(Inadequate_technology_Process_maturit__c==3,Inadequate_technology_Process_maturit__c==4),ISBLANK(Inadequate_Technology_Process_Text__c)),
AND( OR(Quality_Issue__c==3,Quality_Issue__c==4),ISBLANK(Quality_Issue_Text__c)),
AND( OR(GF_is_not_the_incumbent__c==3,GF_is_not_the_incumbent__c==4),ISBLANK(GF_is_not_the_incumbent_text__c))
) 
),
AND(ISPICKVAL(SurveyFor__c, &apos;Dropped&apos;), 
OR(
AND(OR(Pricing_Margin_too_Low__c ==3, Pricing_Margin_too_Low__c==4),  ISBLANK(Pricing_or_Margin_too_Low_Text__c)), 
AND( OR(PLC_rejection__c==3,PLC_rejection__c==4),ISBLANK(PLC_Rejection_Text__c)), 
AND( OR(No_Capacity_to_Support__c==3,No_Capacity_to_Support__c==4),ISBLANK(No_Capacity_to_Support_Text__c)),
AND( OR(No_Technology_Match__c==3,No_Technology_Match__c==4),ISBLANK(No_Technology_Match_Text__c)),
AND( OR(Customer_Project_Cancelled__c==3,Customer_Project_Cancelled__c==4),ISBLANK(Customer_Project_Lost_or_Cancelled_Text__c)),
AND( OR(Process_EOL__c==3,Process_EOL__c==4),ISBLANK(Process_EOL_Text__c)),
AND( OR(Device_EOL__c==3,Device_EOL__c==4),ISBLANK(Device_EOL_Text__c)),
AND( OR(Unsupported_Market_Segment__c==3,Unsupported_Market_Segment__c==4),ISBLANK(Unsupported_Market_Segment_Text__c)),
AND( OR(Customer_Stability__c==3,Customer_Stability__c==4),ISBLANK(Customer_Stability_Text__c)),
AND( OR(Failed_ASICS_or_SiPh_grading__c==3,Failed_ASICS_or_SiPh_grading__c==4),ISBLANK(Failed_ASICS_or_SiPh_grading_Text__c))
) 
),
AND(ISPICKVAL(SurveyFor__c, &apos;Void&apos;), 
OR(
AND(OR(Duplicate_Opportunity__c ==3, Duplicate_Opportunity__c ==4),  ISBLANK(Duplicate_Opportunity_Text__c)), 
AND( OR(Entered_in_Error__c==3,Entered_in_Error__c==4),ISBLANK(Entered_in_Error_Text__c))
) 
)
)</errorConditionFormula>
        <errorMessage>Please enter comments if rating is given 3 or 4</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
