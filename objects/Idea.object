    <validationRules>
        <fullName>Make_Categories_a_required_input</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Categories)</errorConditionFormula>
        <errorMessage>Please select one or more Categories, this input is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Make_Description_required</fullName>
        <active>true</active>
        <description>see also https://help.salesforce.com/apex/HTViewSolution?id=000004240&amp;language=en_US</description>
        <errorConditionFormula>LEN(Body)=0</errorConditionFormula>
        <errorMessage>Please enter a description of the Idea. This input is required.</errorMessage>
    </validationRules>
</CustomObject>
