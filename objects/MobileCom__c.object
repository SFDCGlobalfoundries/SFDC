    <validationRules>
        <fullName>Active_date_validation</fullName>
        <active>true</active>
        <description>Active date should not be less than or greater than today</description>
        <errorConditionFormula>IF(ISNEW(),IF(   OR(Activation_Date__c &lt;  TODAY(),Activation_Date__c &gt;  TODAY())  , true,false),false)</errorConditionFormula>
        <errorDisplayField>Activation_Date__c</errorDisplayField>
        <errorMessage>Activation date should not be less than or greater than  today date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Inactive_date_greater_than_active_date</fullName>
        <active>true</active>
        <description>Inactivation date must be greater than activation date</description>
        <errorConditionFormula>IF( Inactivation_Date__c &lt;= Activation_Date__c,true,false)</errorConditionFormula>
        <errorDisplayField>Inactivation_Date__c</errorDisplayField>
        <errorMessage>Inactivation date must be greater than activation date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restricting_to_edit_MobileCom_number</fullName>
        <active>true</active>
        <description>This validation rule restricts user from editing the existing MobileCom number</description>
        <errorConditionFormula>ISCHANGED( MobileCom_Number__c )</errorConditionFormula>
        <errorDisplayField>MobileCom_Number__c</errorDisplayField>
        <errorMessage>You cannot change the existing MobileCom number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>mobileComNumbervalidation</fullName>
        <active>true</active>
        <description>Mobile number should be 9 chars and not more than 10 chars</description>
        <errorConditionFormula>OR(LEN( MobileCom_Number__c) &lt; 9 , LEN( MobileCom_Number__c) &gt; 10 )</errorConditionFormula>
        <errorDisplayField>MobileCom_Number__c</errorDisplayField>
        <errorMessage>Mobile number should be 9 digits</errorMessage>
    </validationRules>
