    <validationRules>
        <fullName>Team_Room_Account_is_Mandatory</fullName>
        <active>false</active>
        <errorConditionFormula>AND 
( 
OR(RecordType.DeveloperName == &apos;External_Single_party&apos;,
RecordType.DeveloperName == &apos;External_Multi_Party&apos;) ,
 ISBLANK(Primary_Account__c) )</errorConditionFormula>
        <errorMessage>Primary account cann&apos;t be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Team_Room_Description_not_editable</fullName>
        <active>false</active>
        <description>FV2:Team_Room_Description should only be editable by admin profiles</description>
        <errorConditionFormula>AND 
( 
RecordType.DeveloperName &lt;&gt; &apos;Pre_FV2_Teamroom&apos;, 
NOT(ISNEW()), 
ISCHANGED( Team_Room_Description__c ), 
NOT 
( 
$Profile.Name =&apos;Customer Portal Admin&apos; || 
$Profile.Name =&apos;Enablement Partner Portal Admin&apos;|| 
$Profile.Name =&apos;GF Global Admin&apos;|| 
$Profile.Name =&apos;GF Regional Admin&apos;|| 
$Profile.Name =&apos;GF System Admin&apos;|| 
$Profile.Name =&apos;GF Training Admin&apos; || 
$Profile.Name =&apos;Reseller Portal Admin&apos; || 
$Profile.Name =&apos;Supplier Portal Admin&apos; || 
$Profile.Name =&apos;Overage Customer Portal Admin&apos; || 
$Profile.Name =&apos;Overage Enablement Partner Portal Admin&apos; || 
$Profile.Name =&apos;Overage Reseller Portal Admin&apos; || 
$Profile.Name =&apos;Overage Supplier Portal Admin&apos; || 
$Profile.Name =&apos;System Administrator&apos; 
) 
)</errorConditionFormula>
        <errorDisplayField>Team_Room_Description__c</errorDisplayField>
        <errorMessage>Team Room Description is editable only by admin profiles</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Team_Room_name_not_editable</fullName>
        <active>false</active>
        <description>FV2:Team Room Name should only be editable by admin profiles</description>
        <errorConditionFormula>AND 
( 
RecordType.DeveloperName &lt;&gt; &apos;Pre_FV2_Teamroom&apos;, 
NOT(ISNEW()), 
ISCHANGED(Name), 
NOT 
( 
$Profile.Name =&apos;Customer Portal Admin&apos; || 
$Profile.Name =&apos;Enablement Partner Portal Admin&apos;|| 
$Profile.Name =&apos;GF Global Admin&apos;|| 
