var currentUserId =&apos;{!User.Id}&apos;;
var canclick;
var lstgroupmember= sforce.connection.query(
		&quot;SELECT GroupId from GroupMember where UserOrGroupId=&apos;&quot; +currentUserId + &quot;&apos;&quot;);


if(lstgroupmember!= null)
{
 var records1 = lstgroupmember.getArray(&quot;records&quot;);
  for (var i=0; i&lt; records1.length; i++) 
  {
    if(records1[i].GroupId == &apos;{!$Label.Executive_Update_Admins}&apos;)
    {
     canclick=&apos;true&apos;;
     break;
    }
    else
   {
    canclick=&apos;false&apos;;
   }
  }

}

   if(canclick!=&apos;true&apos;)
  {
   alert(&apos;You do not have access.Only Executive Update Admins can Update the Previous  Revenue values&apos;);
  }
  else
  {
  var selectedRecords = {!GETRECORDIDS( $ObjectType.Opportunity)}; 
