             result = sforce.connection.create([table]); 
             alert(&apos;Previous Opportunity Revenue values will be updated&apos;);
             window.location.reload(); //to reload the window         
          }else{
             alert(&apos;Previous Opportunity Revenue values will be updated&apos;);
             window.location.reload(); //to reload the window  
          }
      }      
  }
  }</url>
