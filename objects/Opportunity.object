  else{
      //check for existing pending records
      var opptyIds = selectedRecords[0];
      for(var i=1; i&lt;selectedRecords.length; i++)
          opptyIds = opptyIds +&apos;,&apos;+selectedRecords[i];
      
      var stagingExtRecords = sforce.connection.query(&quot;SELECT Id,Name,Opportunity_Ids__c from Oppty_Staging_Table__c where Status__c=&apos;Pending&apos; and Time_diff__c &lt;= 1.0&quot;);
      if(stagingExtRecords != null){
          var records1 = stagingExtRecords.getArray(&quot;records&quot;);
          var counter = 0;
          for(var i=0;i&lt;records1.length;i++){
              if(records1[i].Opportunity_Ids__c == opptyIds){
                  counter++;
              }
          }
          
          if(counter == 0){
             var table= new sforce.SObject(&apos;Oppty_Staging_Table__c&apos;);
             table.Name=&apos;run batch&apos;;
             table.Opportunity_Ids__c = opptyIds;
