    <validationRules>
        <fullName>Check_for_1st_Close_Out_Record</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    ISCHANGED( DWP_Status__c ),
    TEXT(DWP_Status__c) = &quot;Pending&quot;,
     Close_Out_Approvals__c = 0
)</errorConditionFormula>
        <errorMessage>You need to create a DWP Close Out request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Construction_Coordinator_not_blank</fullName>
        <active>false</active>
        <errorConditionFormula>ISBLANK(Construction_Coordinator__c)</errorConditionFormula>
        <errorDisplayField>Construction_Coordinator__c</errorDisplayField>
        <errorMessage>Make sure the Construction Coordinator is filled in.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Cannot_Be_Before_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>End_Date__c &lt; Start_Date__c</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>The End Date must be greater than or equal to the Start Date. Please check your entries.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Cannot_exceed_30_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(Start_Date__c)),Start_Date__c &lt;= End_Date__c - 31)</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>For a 30-Day Permit, the End Date cannot be more than 30 days past the Start Date. Please check your entries.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Location_not_blank</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Location__c)</errorConditionFormula>
        <errorDisplayField>Location__c</errorDisplayField>
        <errorMessage>Location may not be blank. Please update your selection.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_most_fields_after_draft</fullName>
        <active>true</active>
        <description>Prevents changes to form data after Draft status (except for Start and End Date).</description>
        <errorConditionFormula>AND(
ISCHANGED(Chemicals_used_doing_dirty_work__c) ||
ISCHANGED(Cleaning_Plan_after_the_work_is_finished__c)||
ISCHANGED(Column__c)||
ISCHANGED(Construction_Coordinator_old__c)||
ISCHANGED(Contamination_Control__c)||
ISCHANGED(Location__c)||
ISCHANGED(Method__c)||
ISCHANGED(Plan_to_Eliminate_Contamination__c)||
ISCHANGED(Safety__c)||
ISCHANGED(Tool_ID__c)||
ISCHANGED(Type_of_Work__c),

NOT(TEXT(DWP_Status__c) = &quot;0.0 Draft&quot;)
)</errorConditionFormula>
        <errorMessage>NOTE: All entries except for Start Date and End Date may only be changed for DWPs in status &quot;0.0 Draft&quot;.

Click the Cancel button to discard your changes and return to the detail page.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Method_not_missing_vac_and_wipe</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(INCLUDES(Method__c, &quot;[iii] Vacuum&quot;)) ||
NOT(INCLUDES(Method__c, &quot;[v] Wipe Down&quot;))</errorConditionFormula>
        <errorDisplayField>Method__c</errorDisplayField>
        <errorMessage>Method must contain Vacuum and Wipe Down. Please update your selection.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rep_Name_not_blank</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Company_Contact__c)</errorConditionFormula>
        <errorDisplayField>Company_Contact__c</errorDisplayField>
        <errorMessage>Make sure the Company Contact field is filled in.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Safety_not_missing_ipa</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(INCLUDES(Safety__c, &quot;H: IPA Use&quot;))</errorConditionFormula>
        <errorDisplayField>Safety__c</errorDisplayField>
        <errorMessage>Safety must contain IPA Use. Please update your selection.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Tool_ID_not_blank</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Tool_ID__c)</errorConditionFormula>
        <errorDisplayField>Tool_ID__c</errorDisplayField>
        <errorMessage>Make sure the Tool ID is filled in.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
