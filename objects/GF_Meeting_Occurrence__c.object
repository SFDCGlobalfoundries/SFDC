        <fullName>GFMeetingInviteeValidation</fullName>
        <active>true</active>
        <description>FV2:Validation rule to validate that Invitees cannot be added to past meeting occurrences.</description>
        <errorConditionFormula>AND(
IsNew(), DATEVALUE( Start_Time__c )&lt; TODAY() 
)</errorConditionFormula>
        <errorDisplayField>Start_Time__c</errorDisplayField>
        <errorMessage>Invitees cannot be added to past meeting occurrences.</errorMessage>
