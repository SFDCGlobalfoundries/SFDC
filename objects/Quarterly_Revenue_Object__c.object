    <webLinks>
        <fullName>Reset_Forecast</fullName>
        <availability>online</availability>
        <description>Clears the ASP/Volume value from Quarterly Revenue records.</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Reset Forecast</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/apex.js&quot;)}
if(confirm(&apos;This action will reset Volume/ASP to zero for all Quarterly Revenue records. Are you sure you want to continue?&apos;)==1){ 

    // Now calling the webservice to reset the Volume/ASP of Quarterly Revenue
     var callback = &quot;&quot; + sforce.apex.execute(&quot;QuarterlyRevenueService&quot;,&quot;resetForecast&quot;, {opptyID:&quot;{!Opportunity.Id}&quot;});

    // If reset of Volume/ASP was ok - just reload opportunity page to see the updated Volume/ASP values
    if(callback == &apos;OK&apos;){
       if(window.parent){
            window.parent.location.reload();
       }else{
            window.location.reload();
       }
    }
    else{
        alert(&apos;Error: &apos; + callback);
    }

}</url>
    </webLinks>
