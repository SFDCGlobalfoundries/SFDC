    <fields>
        <fullName>Count_Design_Win_Revenue__c</fullName>
        <externalId>false</externalId>
        <formula>if((year(Date__c)&gt;=2012), if(ISPICKVAL(Opportunity__r.Market_Segment__c, &apos;Mobility&apos;) , 
if(Counter__c&lt;= 16, &quot;TRUE&quot;, &quot;FALSE&quot; )
,if(ISPICKVAL(Opportunity__r.Market_Segment__c, &apos;Consumer&apos;) , 
if(Counter__c&lt;= 16, &quot;TRUE&quot;, &quot;FALSE&quot; )
,if(ISPICKVAL(Opportunity__r.Market_Segment__c, &apos;Industrial,Medical &amp;Scientific&apos;) , 
if(Counter__c&lt;= 24, &quot;TRUE&quot;, &quot;FALSE&quot; )
,if(ISPICKVAL(Opportunity__r.Market_Segment__c, &apos;Compute, Connect, Store (CCS)&apos;) , 
if(Counter__c&lt;= 24, &quot;TRUE&quot;, &quot;FALSE&quot; )
,if(ISPICKVAL(Opportunity__r.Market_Segment__c, &apos;Automotive&apos;) , 
if(Counter__c&lt;= 24, &quot;TRUE&quot;, &quot;FALSE&quot; )
,if(ISPICKVAL(Opportunity__r.Market_Segment__c, &apos;Aerospace &amp; Defense&apos;) , 
if(Counter__c&lt;= 32, &quot;TRUE&quot;, &quot;FALSE&quot; )
,&quot;FALSE&quot;)))))) , &quot;FALSE&quot; )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Considered this quarter as part of Design Win Revenue</inlineHelpText>
        <label>Count Design Win Revenue?</label>
