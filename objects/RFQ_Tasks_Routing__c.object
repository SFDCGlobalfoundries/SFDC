    <validationRules>
        <fullName>Allow_only_approval_task</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
  ISPICKVAL(Related_RFQ_Routing__r.Route_Base_Purpose__c, &apos;Approval&apos;), NOT(ISPICKVAL( Action__c, &apos;&apos;)), NOT(ISPICKVAL( Action__c, &apos;Approve&apos;))
)</errorConditionFormula>
        <errorMessage>This route will only include task that require assignee&apos;s approval.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Allow_only_notify_task</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
  ISPICKVAL(Related_RFQ_Routing__r.Route_Base_Purpose__c, &apos;Review&apos;), NOT(ISPICKVAL( Action__c, &apos;&apos;)), NOT(ISPICKVAL( Action__c, &apos;Comment&apos;))
)</errorConditionFormula>
        <errorMessage>This route will only include task that require assignee&apos;s comments.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approve_Status_Only_For_Approve_Action</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
 NOT(ISPICKVAL(Action__c, &apos;Approve&apos;)),  
 NOT(ISBLANK( TEXT(Status__c)))
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>You need to select &apos;Approval Status&apos; only for approval action</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approve_Status_Req_For_Approve_Action</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
 ISPICKVAL(Action__c, &apos;Approve&apos;),  
 ISBLANK( TEXT(Status__c))
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Please select &apos;Approval Status&apos;  for approval action</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Comments_Only_For_Comment_Action</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
 NOT(ISPICKVAL(Action__c, &apos;Comment&apos;)),  
 NOT(ISBLANK(Comments__c))
)</errorConditionFormula>
        <errorDisplayField>Comments__c</errorDisplayField>
        <errorMessage>You can enter &apos;Comments&apos; only  for comment action.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Comments_Req_For_Comment_Action</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
 ISPICKVAL(Action__c, &apos;Comment&apos;),  
 ISBLANK(Comments__c)
)</errorConditionFormula>
        <errorDisplayField>Comments__c</errorDisplayField>
        <errorMessage>Please enter some &apos;Comments&apos;  for comment action.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Due_Date_Greater_Than_Negotiation_Date</fullName>
        <active>false</active>
        <errorConditionFormula>AND(  Calculate_due_date_based_on_new__c  = &apos;Negotiation date&apos;,   DATEVALUE(Task_Due_Date_and_Time__c) &gt;    Negotiation_Date__c, Skip_Validation__c = false )</errorConditionFormula>
        <errorDisplayField>Task_Due_Date_and_Time__c</errorDisplayField>
        <errorMessage>&apos;Task Due Date and Time&apos;  cannot be greater than &apos;Negotiation Date&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Due_Date_Greater_Than_Response_Date</fullName>
        <active>false</active>
        <errorConditionFormula>AND(  Calculate_due_date_based_on_new__c  = &apos;Customer response date&apos;,  DATEVALUE(Task_Due_Date_and_Time__c)  &gt;    Customer_response_date__c, Skip_Validation__c = false  )</errorConditionFormula>
        <errorDisplayField>Task_Due_Date_and_Time__c</errorDisplayField>
        <errorMessage>&apos;Task Due Date and Time&apos;  cannot be greater than &apos;Customer response date&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Ente_Que_Start_Dt_Bef_Actu_Start_Dt</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK( Start_Date__c ) &amp;&amp; NOT(ISBLANK( Adjusted_Start_Date__c )))</errorConditionFormula>
        <errorMessage>You must populate &apos;Queue Start Date&apos; before entering &apos;Actual Start Date&apos;.
To populate &apos;Queue Start Date&apos; either click on &apos;Start Routing&apos; button on route OR enter &apos;Actual End Date&apos; of &apos;Predecessor Task&apos; if any.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Enter_Actu_Start_Date_Bef_End_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK( Adjusted_Start_Date__c ) &amp;&amp; NOT(ISBLANK( Actual_End_Date__c )))</errorConditionFormula>
        <errorDisplayField>Actual_End_Date__c</errorDisplayField>
        <errorMessage>User must enter &apos;Actual Start Date&apos; before entering &apos;Actual End Date&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pause_Reason_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(Task_Status__c, &apos;Pause&apos;),    ISBLANK(Pause_Reason__c) 
)</errorConditionFormula>
        <errorDisplayField>Pause_Reason__c</errorDisplayField>
        <errorMessage>Please enter &apos;Pause Reason&apos; when Task Status is &apos;Pause&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_User_Edit_when_Task_is_Complete</fullName>
        <active>true</active>
        <description>When RFQ Task is Complete, User can not edit the record.</description>
        <errorConditionFormula>AND(PRIORVALUE(Complete__c) = TRUE,
NOT( ISNEW()),
Skip_Validation__c = false
)</errorConditionFormula>
        <errorMessage>You can not edit the Task as it is Complete.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_User_Input_For_Start_Date</fullName>
        <active>false</active>
        <errorConditionFormula>OR(
AND( ISNEW() ,  ISPICKVAL(Sequence__c, &apos;All&apos;) , NOT( ISBLANK(Start_Date__c) ),  (TEXT(Order__c)!= &apos;1&apos; ),  Skip_Validation__c  = false
),
AND(NOT(ISNEW()), ISPICKVAL(Sequence__c, &apos;All&apos;),  ISCHANGED(Start_Date__c), (TEXT(Order__c)!= &apos;1&apos; ), Skip_Validation__c  = false
)
)</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>You are not allowed to enter or change &apos;Start date&apos; when sequence is set to &apos;All&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_User_ToSet_Task_Status_Complete</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Complete__c = FALSE, 
 ISPICKVAL(Task_Status__c, &apos;Complete&apos;) 
)</errorConditionFormula>
        <errorDisplayField>Task_Status__c</errorDisplayField>
        <errorMessage>&apos;Task Status&apos; cannot be set to &apos;Complete&apos; when &apos;Complete&apos; is not checked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_User_ToSet_Task_Status_Pause</fullName>
        <active>false</active>
        <errorConditionFormula>AND( NOT(ISPICKVAL(Related_RFQ_Routing__r.Status__c, &apos;Pause&apos;)) , 
 ISPICKVAL(Task_Status__c, &apos;Pause&apos;) 
)</errorConditionFormula>
        <errorDisplayField>Task_Status__c</errorDisplayField>
        <errorMessage>&apos;Task Status&apos; cannot be set to &apos;Pause&apos; when &apos;Related RFQ Route&apos; status is not paused</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_User_To_Complete_Task</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISPICKVAL(Task_Status__c, &apos;Complete&apos;),  Complete__c = TRUE), OR( ISBLANK( Adjusted_Start_Date__c ),  ISBLANK( Actual_End_Date__c )  )
)</errorConditionFormula>
        <errorMessage>User cannot not check &apos;Complete&apos; checkbox or set &apos;Task Status&apos; to &apos;Complete&apos; if  &quot;Actual Start Date&quot; or &quot;Actual End Date&quot; are blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SetDelegateToOnlyForAllowDelegation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 Allow_Delegation__c = false,
 NOT(ISBLANK(Delegated_To__c))
)</errorConditionFormula>
        <errorDisplayField>Delegated_To__c</errorDisplayField>
        <errorMessage>Select &apos;Delegated To&apos; only when &apos;Allow Delegation&apos; is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>User_Must_Enter_Start_Date</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
ISBLANK(Start_Date__c),  OR(ISPICKVAL(Sequence__c, &apos;Any&apos;), (TEXT(Order__c)== &apos;1&apos; )), Skip_Validation__c  = false
)</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>You must enter &apos;Start date&apos; when order is &apos;1&apos; or sequence is set to &apos;Any&apos;</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
