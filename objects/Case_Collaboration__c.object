    </validationRules>
    <validationRules>
        <fullName>Subscribe_or_unsubscribe_other_accounts</fullName>
        <active>true</active>
        <description>Users not allowed to subscribe or unsubscribe other accounts contacts.</description>
        <errorConditionFormula>AND( NOT(ISPICKVAL( $Profile.UserType, &apos;Standard&apos;)), $User.Parent_Account_Id__c  &lt;&gt;  Account_Contact__c)</errorConditionFormula>
        <errorMessage>You are not allowed to subscribe or unsubscribe other accounts contact.</errorMessage>
