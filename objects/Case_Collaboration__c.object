    <validationRules>
        <fullName>Contacts_subscribed_in_Internal_Case</fullName>
        <active>true</active>
        <errorConditionFormula>Case__r.Internal_or_external__c ==true &amp;&amp; Is_Subscribed__c ==true</errorConditionFormula>
