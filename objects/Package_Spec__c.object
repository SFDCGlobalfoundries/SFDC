    <validationRules>
        <fullName>Error_Package_Spec_Update</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Design_Spec__c )&amp;&amp; $Profile.Name != &apos;GF Integration&apos;</errorConditionFormula>
        <errorMessage>Cannot update the Design Spec of Package Spec</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Spec_Asso_to_non_Draft_Package</fullName>
        <active>false</active>
        <description>This validation rule prevents adding new Specs to a Package which is published to customers.</description>
        <errorConditionFormula>ISNEW() &amp;&amp;     NOT(ISPICKVAL(Package__r.Package_Release_Status__c, &apos;Draft&apos;))</errorConditionFormula>
        <errorMessage>Cannot add Design Spec to a Package already published to customers.</errorMessage>
    </validationRules>
