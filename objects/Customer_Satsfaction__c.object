IF(Q1__c&lt;&gt;0,IMAGE(&apos;/resource/OrangeCustomerSatisfaction&apos;,&apos;Orange&apos;),&apos;&apos;))),IF(AND(Q1_Score__c&lt; Merit_Index_for_customers__r.RED__c,NOT(ISBLANK(Q1_Score__c))),IMAGE(&apos;/resource/RedCustomerSatisfaction&apos;,&apos;Red&apos;),
IF(Q1_Score__c &gt;= Merit_Index_for_customers__r.GREEN__c,IMAGE(&apos;/resource/GreenCustomerSatisfaction&apos;,&apos;Green&apos;),
IF(Q1_Score__c&lt;&gt;0,IMAGE(&apos;/resource/OrangeCustomerSatisfaction&apos;,&apos;Orange&apos;),&apos;&apos;))))</formula>
        <label>Merit Index</label>
