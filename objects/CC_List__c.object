    <validationRules>
        <fullName>CCList_Contributor_UserChangeValidation</fullName>
        <active>true</active>
        <description>You cannot change the user associated with a CClist member. If you want to change the user anme then delete the previous and craete a new record.</description>
        <errorConditionFormula>AND(NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c),
$Profile.Name &lt;&gt; &quot;System Administrator&quot; ,
ISCHANGED( User__c ), NOT( ISNEW()))</errorConditionFormula>
        <errorDisplayField>User__c</errorDisplayField>
        <errorMessage>You cannot change the ccList user. Please delete this record and create a new one.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CC_ListContributor_Type_Validation</fullName>
        <active>true</active>
        <description>Type is Mandatory for Other Contributors role.</description>
        <errorConditionFormula>AND(NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c), TEXT(Roles_Responsibility__c)=&apos;Other Contributors&apos;, ISBLANK(TEXT(Type__c)))</errorConditionFormula>
        <errorDisplayField>Type__c</errorDisplayField>
        <errorMessage>Type is Mandatory for Other Contributors role.</errorMessage>
    </validationRules>
