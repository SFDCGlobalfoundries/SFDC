    <validationRules>
        <fullName>Active_date_validation</fullName>
        <active>true</active>
        <description>Active date should not be less than today</description>
        <errorConditionFormula>AND(IF(ISNEW(),IF(OR(Activation_Date__c &lt;  TODAY(),Activation_Date__c &gt;  TODAY())  , true,false),false),NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c),$Profile.Name &lt;&gt; &apos;GF Integration&apos;)</errorConditionFormula>
        <errorDisplayField>Activation_Date__c</errorDisplayField>
        <errorMessage>Activation date should not be less than today date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Audit_date_validation</fullName>
        <active>true</active>
        <description>Audit date must be in between activation and inactivation date</description>
        <errorConditionFormula>AND(OR (IF( Audit_Date__c  &lt;  Activation_Date__c , true, false) ,
 IF( Audit_Date__c  &gt;   Inactivation_Date__c , true, false) ),NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c),
$Profile.Name &lt;&gt; &apos;GF Integration&apos;)</errorConditionFormula>
        <errorDisplayField>Audit_Date__c</errorDisplayField>
        <errorMessage>Audit date must be in between activation and inactivation date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Inactive_date_greater_than_active_date</fullName>
        <active>true</active>
        <description>Inactive date should be greater than active date</description>
        <errorConditionFormula>AND(IF( Inactivation_Date__c  &lt;=  Activation_Date__c ,true,false ), NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c),
$Profile.Name &lt;&gt; &apos;GF Integration&apos;)</errorConditionFormula>
        <errorDisplayField>Inactivation_Date__c</errorDisplayField>
        <errorMessage>inactivation date must be greater than activation date</errorMessage>
    </validationRules>
