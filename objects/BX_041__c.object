        <masterLabel>New CDRS</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/28.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/28.0/apex.js&quot;)}
//sforce.connection.session = &quot;{!$Api.Session_ID}&quot;;
var profile = &apos;{!$Profile.Name}&apos;;
var IsAuthorized = false;
var deviceId = &apos;{!Device__c.Id}&apos;;
var deviceStage = &apos;{!Device__c.Stage__c}&apos;;
var deviceStatus = &apos;{!Device__c.Status__c}&apos;;
var deviceComplete = {!Device__c.Device_Complete__c};
var isInternalDevice = false;
var currentUserId = &apos;{!$User.Id}&apos;;
var deviceOwnerId = &apos;{!Device__c.OwnerId}&apos;;
var objectPermission = true;
var deviceRecTypeName = &apos;{!Device__c.RecordType}&apos;;

if(deviceRecTypeName != null &amp;&amp; (deviceRecTypeName == &apos;Internal Device&apos; || deviceRecTypeName == &apos;Internal Device Read Only&apos;)){
	isInternalDevice = true;
}
 
if(isInternalDevice){
	var describeResult = sforce.connection.describeSObject(&quot;BX_041__c&quot;);
	if(describeResult != null &amp;&amp; (describeResult.createable == &apos;false&apos; ||  describeResult.updateable == &apos;false&apos;)){
		objectPermission = false;
		IsAuthorized = false;
	}
	if(objectPermission){
		if(currentUserId == deviceOwnerId){
			   IsAuthorized = true;
		}
		if(!IsAuthorized){
			var accTeamProxyResult = sforce.connection.query(&quot;SELECT Id, User__c from Account_Team_Proxy__c WHERE &quot;+ 
												&quot; User__c = &apos;{!$User.Id}&apos; AND Account__c = &apos;{!Device__c.AccountId__c}&apos;&quot;+
												&quot; AND Team_Role__c = &apos;Account Administrator&apos;&quot;);
			if(accTeamProxyResult != null &amp;&amp; accTeamProxyResult.size &gt; 0){
				IsAuthorized = true;
			}
		} 
	}		
}

if(!isInternalDevice &amp;&amp; (profile.indexOf(&quot;Admin&quot;) != -1 || profile.indexOf(&quot;FAE/FTS&quot;) != -1 || profile.indexOf(&quot;Customer Portal&quot;) != -1)){
	IsAuthorized = true;
}

if(!IsAuthorized){
    if(!isInternalDevice){
		alert(&quot;Not right to create CDRS.&quot;);
    }else{
		if(!objectPermission){
			alert(&quot;Insuffiecient privilages. Please contact to Admin and assign &apos;Access to Internal Device Management permission set&apos;&quot;);
		}else{
			alert(&quot;Only Device Owner/Account Admin from Account Team Proxy can create CDRS.&quot;);
		}
    }
}else{
	if(deviceComplete){
		if(deviceStage == &apos;Design In&apos; &amp;&amp; deviceStatus == &apos;Active&apos;){
			var queryString = &quot;SELECT id FROM BX_041__c where Device__c = &apos;&quot; + deviceId + &quot;&apos;&quot;;
			var queryResult = sforce.connection.query(queryString);
			if(queryResult.size &gt; 0 ) {
				alert(&quot;You can&apos;t Create another CDRS record.&quot;);
			}else {
				window.open(&quot;{!URLFOR($Action.BX_041__c.New, null,null,false)}&quot;,&quot;_parent&quot;);
			}
		}else {
			alert(&quot;This Device is not allowed to create CDRS&quot;);
		}
	}else{
		alert(&quot;Required fields are missing on Device. Please edit Device and enter required fields.&quot;);
	}
}</url>
