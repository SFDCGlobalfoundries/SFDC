    <validationRules>
        <fullName>Design_Spec_Lookup_Mandatory_for_Design</fullName>
        <active>true</active>
        <description>When Record Type Name is equal to Design Specs, then Design Spec Lookup Field (Design_Spec__c) should be mandatory.</description>
        <errorConditionFormula>AND(RecordType.Name == &apos;Design Spec&apos;,Design_Spec__c == &apos;&apos;)</errorConditionFormula>
