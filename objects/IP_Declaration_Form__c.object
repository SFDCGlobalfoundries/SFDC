        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var usrRole = &apos;{!$UserRole.Name}&apos;;
var profName = &apos;{!$Profile.Name}&apos;;
var status = &apos;{!IP_Declaration_Form__c.Status__c }&apos;;
if(profName == &apos;System Administrator&apos; || ISPICKVAL($Profile.UserType ,&quot;CustomerSuccess&quot;)){
  if(status == &apos;Draft&apos; || status == &apos;Reassigned&apos; || status == &apos;Pending Resubmission&apos; || status == &apos;Recalled&apos;){
    window.location = &apos;/apex/IpDeclarationChangeOwner?id={!IP_Declaration_Form__c.Id}&apos;;
  }else{
    alert(&apos;You are only allowed to reassign when status is Draft, Reassigned, Pending Resubmission or Recalled.&apos;);
