    <fields>
        <fullName>Process_Technology__c</fullName>
        <externalId>false</externalId>
        <formula>IF( NOT(ISBLANK( Device__r.Process_Family__c )) &amp;&amp; NOT(ISPICKVAL(Device__r.Geometry_Filter__c, &apos;&apos;)) , Device__r.Process_Family__c + TEXT(Device__r.Geometry_Filter__c), 
     IF( ISBLANK( Device__r.Process_Family__c ) &amp;&amp; NOT(ISPICKVAL(Device__r.Geometry_Filter__c, &apos;&apos;)) ,  TEXT(Device__r.Geometry_Filter__c), 
          IF( NOT(ISBLANK( Device__r.Process_Family__c )) &amp;&amp; ISPICKVAL(Device__r.Geometry_Filter__c, &apos;&apos;), Device__r.Process_Family__c, &apos;NA&apos;) 
     ) 
) 

/*
 IF(logical_test, value_if_true, value_if_false) 

a= Device__r.Process_Family__c , to be replace by Assigned Process Id (PI/YE) field in Siebel
b= Device__r.Geometry__c , to be replaced by Technology in Siebel

original:
if(process != null &amp;&amp; technology != null){
			return process + &quot;, &quot; + technology;
		}else if(process == null &amp;&amp; technology != null){
			return technology;
		}else if(process != null &amp;&amp; technology == null){
			return process;
		}else {
			return &quot;NA&quot;;
		}
*/</formula>
        <label>Process Technology</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
