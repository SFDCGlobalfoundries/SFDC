    <webLinks>
        <fullName>Include_In_Next_Matching_Process</fullName>
        <availability>online</availability>
        <description>This button flags the IP Declaration for VCID matching using the batch job.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Include In Next Matching Process</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/apex.js&quot;)} 

var status = &apos;{! IP_Declaration_Form__c.Status__c }&apos;;
var ipmatchFlag = &apos;{!IP_Declaration_Form__c.Next_Matching_Process__c}&apos;;

if(ipmatchFlag==0){
if(status == &apos;Draft&apos; || status == &apos;Reassigned&apos; || status == &apos;Pending Resubmission&apos; || status == &apos;Recalled&apos;){
		// Now calling the webservice 
		var callback = &quot;&quot; + sforce.apex.execute(&quot;IPDeclarationMatching&quot;,&quot;setToNextMatchingProcess&quot;, {recordID:&quot;{! IP_Declaration_Form__c.Id }&quot;});
		if(callback == &apos;success&apos;){ 
			window.location.reload(); 
		}else{
			alert(&apos;Error: &apos; + callback);
		} 
}else{
	alert(&apos;You can only check IP Declaration Matching Process in Draft/Reassigned/Pending Resubmission/Recalled status.&apos;);
}
}</url>
    </webLinks>
