    <validationRules>
        <fullName>Certified_Check_ValidEndDate</fullName>
        <active>true</active>
        <description>Certified check box will be successfully check for valid end days</description>
        <errorConditionFormula>AND(OR(ISBLANK(ITAR_EndDate__c), ITAR_EndDate__c &lt; TODAY()),  ITARCertified__c = TRUE)</errorConditionFormula>
        <errorDisplayField>ITARCertified__c</errorDisplayField>
        <errorMessage>Please Enter a valid End Date, to check ITAR Certified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Future_Value</fullName>
        <active>true</active>
        <description>End Date should be either today or Future Days.</description>
        <errorConditionFormula>ITAR_EndDate__c &lt; TODAY()</errorConditionFormula>
        <errorDisplayField>ITAR_EndDate__c</errorDisplayField>
        <errorMessage>Please enter Current or Future Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ITAR_Access_To_Only_Internal_Users</fullName>
        <active>true</active>
        <description>ITAR Access to only Internal Users</description>
        <errorConditionFormula>AND(OR(DropBox_ITAR__c= TRUE, DropBox_Non_ITAR__c = TRUE , Conferencing_ITAR__c= TRUE , TeamRoom_ITAR__c= TRUE ,Remote_Mask_Viewer_ITAR__c= TRUE , Remote_Mask_Viewer_Non_ITAR__c = TRUE , ITARCertified__c = TRUE , NOT(ISBLANK(ITAR_EndDate__c ))), NOT(ISBLANK(User__r.ContactId)))</errorConditionFormula>
        <errorMessage>ITAR specific fields usage is valid only to Internal USER.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_End_Date_for_Certified_Users</fullName>
        <active>true</active>
        <description>End Date is mandatory if ITAR Certified is checked</description>
        <errorConditionFormula>AND(
NOT(ITARCertified__c),
NOT(ISNULL(ITAR_EndDate__c)))</errorConditionFormula>
        <errorDisplayField>ITAR_EndDate__c</errorDisplayField>
        <errorMessage>Please check ITAR Certified in order to fill ITAR End Date</errorMessage>
    </validationRules>
    <validationRules>
