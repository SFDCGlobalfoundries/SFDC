    <validationRules>
        <fullName>RMA_RI_Adjusted_Price</fullName>
        <active>true</active>
        <description>Adjusted Unit Price should not be greater than Unit Price.
NJain-27Jul16-Added the condition to skip RMA with Reason Code = 81.</description>
        <errorConditionFormula>AND(
!ISPICKVAL(RMA__r.ReasonCode__c,&apos;R81&apos;),
Wafer_Die_Unit_Price__c &lt;= Adjusted_Unit_Price__c,
ISPICKVAL(RMA__r.RMA_By__c,&apos;Wafer&apos;),
NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c),
$Profile.Name &lt;&gt; &apos;GF Integration&apos;
)</errorConditionFormula>
        <errorDisplayField>Adjusted_Unit_Price__c</errorDisplayField>
        <errorMessage>Value should be less that Unit Selling Price</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RMA_RI_Adjusted_Price_Die</fullName>
        <active>true</active>
        <description>Adjusted Unit Price should not be greater than Die Selling Price.
NJain-27Jul-16-Added the condition to skip RMA with Reason Code = 81.</description>
        <errorConditionFormula>AND(
!ISPICKVAL(RMA__r.ReasonCode__c,&apos;R81&apos;),
Invoice__r.Die_Selling_Price__c &lt;= Adjusted_Unit_Price__c,
ISPICKVAL(RMA__r.RMA_By__c,&apos;Die&apos;),
NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c),
$Profile.Name &lt;&gt; &apos;GF Integration&apos;
)</errorConditionFormula>
        <errorDisplayField>Adjusted_Unit_Price__c</errorDisplayField>
        <errorMessage>Adjusted Price should be less that Die Selling Price</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RMA_RI_Adjusted_Price_Module</fullName>
