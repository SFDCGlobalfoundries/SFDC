    <fields>
        <fullName>Total_Price__c</fullName>
        <externalId>false</externalId>
        <formula>/*
IF( ISPICKVAL(RMA_By__c,&apos;Wafer&apos;),
    IF(Adjusted_Unit_Price__c &gt; 0, Adjusted_Unit_Price__c, Wafer_Die_Unit_Price__c) * Wafer_Quantity__c,
    IF(Adjusted_Unit_Price__c &gt; 0, Adjusted_Unit_Price__c, Die_Selling_Price__c)* Die_Quantity__c
)*/
IF( !ISBLANK(TEXT(Existing_Form_Factor__c)),
    Adjusted_Unit_Price__c *  Existing_Form_Factor_Quantity__c, 
    IF( ISPICKVAL(RMA_By__c,&apos;Wafer&apos;),
		IF(Adjusted_Unit_Price__c &gt; 0, Adjusted_Unit_Price__c, Wafer_Die_Unit_Price__c) * Wafer_Quantity__c,
		IF(ISPICKVAL(RMA_By__c,&apos;Die&apos;),
			IF(Adjusted_Unit_Price__c &gt; 0, Adjusted_Unit_Price__c, Die_Selling_Price__c)* Die_Quantity__c,
			IF(Adjusted_Unit_Price__c &gt; 0, Adjusted_Unit_Price__c, Wafer_Die_Unit_Price__c)* Module_Quantity__c
		)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
