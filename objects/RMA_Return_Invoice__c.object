        <errorConditionFormula>AND(
Invoice__r.Bill_Quantity__c  &lt;  Die_Quantity__c || Die_Quantity__c &lt;1,
ISPICKVAL(RMA__r.RMA_By__c,&apos;Die&apos;),
NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c),
$Profile.Name &lt;&gt; &apos;GF Integration&apos;
)</errorConditionFormula>
        <errorDisplayField>Die_Quantity__c</errorDisplayField>
        <errorMessage>Die Quantity should be less or equal to Bill Quantity and greater than zero</errorMessage>
    </validationRules>
