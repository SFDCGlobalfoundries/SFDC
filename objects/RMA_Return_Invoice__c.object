        <fullName>Current_Die_Quantity__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
	OR( 
		RMA__r.Workflow_Stage__c == &apos;RMA Raised&apos;,
		RMA__r.Workflow_Stage__c == &apos;Rejected RMA&apos;,
		RMA__r.Workflow_Stage__c == &apos;&apos;
	),
	Die_Quantity__c,
	IF( 
            OR( RMA__r.Workflow_Stage__c == &apos;Pending for Quality Validation&apos;,
                RMA__r.Workflow_Stage__c == &apos;Pending for Fab Validation&apos;,
		RMA__r.Workflow_Stage__c == &apos;Pending for Turnkey/ Sub-con Validation&apos;,
		RMA__r.Workflow_Stage__c == &apos;Pending for Non Fab Category Validation&apos;,
		RMA__r.Workflow_Stage__c == &apos;Pending for Product Engineering Validation&apos;
	    ),
	    Die_Quantity_Fab_Validated__c,
	    IF( ISPICKVAL(RMA__r.RMA_Category__c,&apos;Failure Analysis&apos;),
                Die_Quantity__c,
                Die_Quantity_Regional_Validated__c
            )
	)
)</formula>
