    IF(Adjusted_Unit_Price__c &gt; 0, Adjusted_Unit_Price__c, Wafer_Die_Unit_Price__c) * Total_Wfr_Qty_after_validation__c,
    IF(Adjusted_Unit_Price__c &gt; 0, Adjusted_Unit_Price__c, Die_Selling_Price__c)* Die_Quantity_Fab_Validated__c
)*/
IF( !ISBLANK(TEXT(Existing_Form_Factor__c)), 
Adjusted_Unit_Price__c * Existing_Form_Factor_Quantity__c, 
IF( ISPICKVAL(RMA_By__c,&apos;Wafer&apos;),
    IF(Adjusted_Unit_Price__c &gt; 0, Adjusted_Unit_Price__c, Wafer_Die_Unit_Price__c) * Total_Wfr_Qty_after_validation__c,
    IF(ISPICKVAL(RMA_By__c,&apos;Die&apos;),
	IF(Adjusted_Unit_Price__c &gt; 0, Adjusted_Unit_Price__c, Die_Selling_Price__c)* Die_Quantity_Fab_Validated__c,
	IF(Adjusted_Unit_Price__c &gt; 0, Adjusted_Unit_Price__c, Wafer_Die_Unit_Price__c)* Module_Quantity_Fab_Validated__c
    )
)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Price (FAB Validation)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Total_Price_Final__c</fullName>
        <externalId>false</externalId>
        <formula>/*
IF( ISPICKVAL(RMA_By__c,&apos;Wafer&apos;),
    IF(Adjusted_Unit_Price__c &gt; 0, Adjusted_Unit_Price__c, Wafer_Die_Unit_Price__c) * Final_RMA_Wfr_Qty_To_Issue_CN_SO__c,
    IF(Adjusted_Unit_Price__c &gt; 0, Adjusted_Unit_Price__c, Die_Selling_Price__c)* Die_Quantity_Regional_Validated__c
)*/
IF( !ISBLANK(TEXT(Existing_Form_Factor__c)), 
Adjusted_Unit_Price__c * Existing_Form_Factor_Quantity__c,
IF( ISPICKVAL(RMA_By__c,&apos;Wafer&apos;),
    IF(Adjusted_Unit_Price__c &gt; 0, Adjusted_Unit_Price__c, Wafer_Die_Unit_Price__c) * Final_RMA_Wfr_Qty_To_Issue_CN_SO__c,
    IF(ISPICKVAL(RMA_By__c,&apos;Die&apos;),
	IF(Adjusted_Unit_Price__c &gt; 0, Adjusted_Unit_Price__c, Die_Selling_Price__c)* Die_Quantity_Regional_Validated__c,
	IF(Adjusted_Unit_Price__c &gt; 0, Adjusted_Unit_Price__c, Wafer_Die_Unit_Price__c)* Module_Quantity_Regional_Validated__c
    )
)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
