    <fields>
        <fullName>Wafer_Quantity__c</fullName>
        <externalId>false</externalId>
        <formula>IF( !ISBLANK(TEXT(Existing_Form_Factor__c)),
	Existing_Form_Factor_Quantity__c,
	IF(	ISPICKVAL(RMA_By__c,&apos;Wafer&apos;),
		IF(NOT(ISPICKVAL(Fab_Group__c,&apos;FAB 9&apos;)) &amp;&amp; NOT(ISPICKVAL(Fab_Group__c,&apos;FAB 10&apos;)) &amp;&amp; NOT(ISPICKVAL(Fab_Group__c,&apos;ALTIS&apos;)),
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;1&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;2&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;3&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;4&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;5&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;6&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;7&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;8&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;9&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;10&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;11&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;12&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;13&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;14&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;15&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;16&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;17&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;18&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;19&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;20&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;21&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;22&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;23&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;24&quot;), 1,0) + 
			IF(INCLUDES( Wafer_Id_CSR_Submission__c, &quot;25&quot;), 1,0),
			Wafer_Qty_Fab_9_10_CSR_Submission__c
		),
		IF(ISPICKVAL(RMA_By__c,&apos;Die&apos;),
			Die_Quantity__c,
			Module_Quantity__c
		)
	)
)</formula>
