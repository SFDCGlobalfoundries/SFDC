    <validationRules>
        <fullName>VRDRTMSTCannotBeBlank</fullName>
        <active>false</active>
        <errorConditionFormula>ISBLANK(Mask_Set_Title__c) || ISNULL(Mask_Set_Title__c)</errorConditionFormula>
        <errorMessage>Mask Set Title cannot be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRDRTMaskLayerRevROMCodeCannotBeBlank</fullName>
        <active>false</active>
        <errorConditionFormula>ISBLANK(Mask_Layer_No__c) || ISBLANK(Mask_Rev__c) || ISBLANK(ROM_Code__c)</errorConditionFormula>
        <errorDisplayField>DRT__c</errorDisplayField>
        <errorMessage>Mask Layer No, Mask Rev, ROM Code cannot be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRDRTRetrofitMaskRevMandatory</fullName>
        <active>true</active>
        <description>To check whether the Retrofit Mask rev field has a blank value or not when the DRT is in Active status.</description>
        <errorConditionFormula>ISBLANK(Retrofit_Mask_Rev__c) &amp;&amp; (DRT_Status__c == &apos;Active&apos;)</errorConditionFormula>
        <errorMessage>Retrofit Mask Rev cannot be blank when activating a DRT.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VRDRTRevisionMandatory</fullName>
        <active>false</active>
        <description>This validation rule will check whether the Revision field has a blank value or not when the DRT is in Active status.</description>
        <errorConditionFormula>ISBLANK(Retrofit_Mask_Rev__c) &amp;&amp; ( ISPICKVAL(DRT__r.Status__c ,&apos;Active&apos;))</errorConditionFormula>
        <errorMessage>Retrofit Mask Rev is mandatory when activating a DRT</errorMessage>
    </validationRules>
