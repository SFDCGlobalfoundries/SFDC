    <fields>
        <fullName>GBC_Stage__c</fullName>
        <externalId>false</externalId>
        <formula>CASE( Stage__c ,
&quot;New&quot;, IMAGE(&quot;resource/GBC_Stage_New&quot;, &quot;&quot;, 60, 400) + &quot;Click Submit for Approval button&quot;,
&quot;Submitted&quot;,IMAGE(&quot;resource/GBC_Stage_Submitted&quot;, &quot;&quot;, 60, 150) + &quot;Submitted&quot;, 
&quot;Closed - Approved&quot;, IMAGE(&quot;resource/GBC_Stage_Approved&quot;, &quot;&quot;, 60, 150) + &quot;Approved&quot;,
&quot;Closed - Rejected&quot;, IMAGE(&quot;resource/GBC_Stage_Rejected&quot;, &quot;&quot;, 60, 150) + &quot;Closed - Rejected&quot;, 
&quot;Closed - Cancelled&quot;, IMAGE(&quot;resource/GBC_Stage_Approved&quot;, &quot;&quot;, 60, 150) + &quot;Cancelled&quot;,
&quot;&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>GBC Stage</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
