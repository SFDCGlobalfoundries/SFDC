    <validationRules>
        <fullName>PTRF_Name_Cannot_Be_Changed</fullName>
        <active>true</active>
        <description>PTRF Name cannot be changed</description>
        <errorConditionFormula>ISCHANGED(Name)</errorConditionFormula>
        <errorMessage>PTRF Number cannot be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_New_PTRF_Creation_on_Upsert</fullName>
        <active>true</active>
        <description>This validation rule prevents creation of New PTRF for upsert call from SWIFT dotNET system if PTRF Number is not matching.</description>
        <errorConditionFormula>AND( ISNEW() , CONTAINS(CreatedBy.Profile.Name, &apos;integration&apos;),NOT(ISNULL( PTRF_External_ID__c )))</errorConditionFormula>
        <errorMessage>There is no matching PTRF for the specified External ID (PTRF Number).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_On_Order_Type</fullName>
        <active>true</active>
        <description>Order Type should contain a value</description>
        <errorConditionFormula>ISBLANK( Order_Type__c )</errorConditionFormula>
        <errorDisplayField>Order_Type__c</errorDisplayField>
        <errorMessage>Order Type cannot be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_On_Reticle_Type_Picklist</fullName>
        <active>true</active>
        <description>Reticle Type a value should be selected</description>
        <errorConditionFormula>NOT(OR(ISPICKVAL( Recticle_Type__c , &quot;Single Layer Reticle (SLR)&quot;),ISPICKVAL( Recticle_Type__c , &quot;Multi Layer Reticle (MLR)&quot;),ISPICKVAL( Recticle_Type__c , &quot;Multi Node Reticle (MNR)&quot;),ISPICKVAL( Recticle_Type__c , &quot;Small Field Reticle (SFR)&quot;)))</errorConditionFormula>
        <errorDisplayField>Recticle_Type__c</errorDisplayField>
        <errorMessage>Please select a Reticle Type from the drop down.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
