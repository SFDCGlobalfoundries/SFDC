    <validationRules>
        <fullName>ROS_Cycle_EndDate_GTE_TODAY</fullName>
        <active>true</active>
        <description>Cycle End Date should be greater than or equal to TODAY.</description>
        <errorConditionFormula>AND(Cycle_End_Date__c &lt; TODAY(),
$User.HCM_Login_ID__c &lt;&gt; &apos;CRMBATCHUSR&apos;, 
$User.HCM_Login_ID__c &lt;&gt; &apos;E1006044&apos;, 
$User.HCM_Login_ID__c &lt;&gt; &apos;CRMDEVS&apos;
)</errorConditionFormula>
        <errorDisplayField>Cycle_End_Date__c</errorDisplayField>
        <errorMessage>Cycle End Date should be greater than or equal to TODAY.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ROS_Cycle_EndDate_GT_StartDate</fullName>
        <active>true</active>
        <description>Cycle End Date should be greater than Cycle Start Date.</description>
        <errorConditionFormula>AND(
Cycle_End_Date__c  &lt;= Cycle_Start_Date__c,
$User.HCM_Login_ID__c &lt;&gt; &apos;CRMBATCHUSR&apos;, 
$User.HCM_Login_ID__c &lt;&gt; &apos;E1006044&apos;, 
$User.HCM_Login_ID__c &lt;&gt; &apos;CRMDEVS&apos;
)</errorConditionFormula>
        <errorDisplayField>Cycle_End_Date__c</errorDisplayField>
        <errorMessage>Cycle End Date should be greater than Cycle Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ROS_Cycle_EndDate_UnEditable</fullName>
        <active>true</active>
        <description>Once the End Date has been populated on a particular cycle, it cannot be changed further.</description>
        <errorConditionFormula>AND(
  !ISNEW(),
  !ISBLANK(PRIORVALUE(Cycle_End_Date__c)),
  OR(
      PRIORVALUE(Cycle_End_Date__c) != Cycle_End_Date__c,
      ISBLANK(Cycle_End_Date__c)
  ),
  $User.HCM_Login_ID__c &lt;&gt; &apos;CRMBATCHUSR&apos;,
  $User.HCM_Login_ID__c &lt;&gt; &apos;E1006044&apos;,
  $User.HCM_Login_ID__c &lt;&gt; &apos;CRMDEVS&apos;
)</errorConditionFormula>
        <errorDisplayField>Cycle_End_Date__c</errorDisplayField>
        <errorMessage>You cannot edit Cycle End Date once assigned.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ROS_Cycle_StartDate_UnEditable</fullName>
        <active>true</active>
        <errorConditionFormula>AND(PRIORVALUE(Cycle_Start_Date__c) != Cycle_Start_Date__c,
$User.HCM_Login_ID__c &lt;&gt; &apos;CRMBATCHUSR&apos;, 
$User.HCM_Login_ID__c &lt;&gt; &apos;E1006044&apos;, 
$User.HCM_Login_ID__c &lt;&gt; &apos;CRMDEVS&apos;
)</errorConditionFormula>
        <errorDisplayField>Cycle_Start_Date__c</errorDisplayField>
        <errorMessage>You cannot edit the Cycle Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ROS_NextCycle_Mandatory_on_CurrentEnd</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
!ISNULL(Cycle_End_Date__c) &amp;&amp; ISPICKVAL(Next_Cycle_Name__c,&apos;&apos;),
$User.HCM_Login_ID__c &lt;&gt; &apos;CRMBATCHUSR&apos;, 
$User.HCM_Login_ID__c &lt;&gt; &apos;E1006044&apos;, 
$User.HCM_Login_ID__c &lt;&gt; &apos;CRMDEVS&apos;
)</errorConditionFormula>
        <errorDisplayField>Next_Cycle_Name__c</errorDisplayField>
        <errorMessage>Please fill in the Next Cycle name.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
