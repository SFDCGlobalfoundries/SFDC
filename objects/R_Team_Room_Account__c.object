    <validationRules>
        <fullName>R_Validate_Team_Room_Type_And_Associate</fullName>
        <active>true</active>
        <description>This validation rule is to to ensure that Account can only be associated to a Team Room if the Team Room type(TR_Type__c) is ‘External Single Party’ or ‘External Multi Party’</description>
        <errorConditionFormula>Restricted_Team_Room__r.RecordType.Name =&apos;GLOBALFOUNDRIES Internal&apos;</errorConditionFormula>
        <errorMessage>Account cannot be associated to this Team Room</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>R_Validte_One_Accnt_For_External_Single</fullName>
        <active>true</active>
        <description>More than one account cannot be associated to this Team Room</description>
        <errorConditionFormula>AND
(
Restricted_Team_Room__r.RecordType.Name =&apos;GLOBALFOUNDRIES with Single party&apos;,
Restricted_Team_Room__r.Num_Associated_Accs__c&gt;0
)</errorConditionFormula>
        <errorMessage>To validate that there is only one account associated to External single party Team room</errorMessage>
    </validationRules>
