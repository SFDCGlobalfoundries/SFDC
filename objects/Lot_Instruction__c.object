        <errorDisplayField>Submitted_Customer_Request_Date_CRD__c</errorDisplayField>
        <errorMessage>New CRD (CSR) cannot be less than today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_Expedite_Type</fullName>
        <active>false</active>
        <description>Expedite Type Mandatory for CSR to fill under Lot Instruction.</description>
        <errorConditionFormula>ISBLANK(TEXT(Expedite_Type_To_Meet_CRD_CSR__c))</errorConditionFormula>
        <errorDisplayField>Expedite_Type_To_Meet_CRD_CSR__c</errorDisplayField>
        <errorMessage>Please select Expedite Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_CSD_P_Earlier_than_Current_CSD_ML</fullName>
        <active>true</active>
        <description>Mandatory for the planners to check if the date in New CSD (P) is earlier than the current CSD.</description>
        <errorConditionFormula>AND( 
ISCHANGED(New_CSD_P__c),
/*ISPICKVAL( Lot_Request__r.Expedite_Manager_Recommendation_EM__c, &quot;Approved Under Corridor&quot;), */
ISPICKVAL( Lot_Request__r.Stage__c ,&quot;Pending Planner Response&quot;), 
New_CSD_P__c &gt; Current_CSD_ML_at_time_of_Submission__c
)</errorConditionFormula>
        <errorMessage>New CSD (P) should be earlier than the Current CSD at time of submission</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Submitted_CRD_Greater_Than_Current_CSD</fullName>
        <active>true</active>
        <description>New CRD (CSR) should always be less than Current CSD (ML).

Submitted_Customer_Request_Date_CRD__c &gt;= Current_Schedule_Date_CSD_ML__c</description>
        <errorConditionFormula>Submitted_Customer_Request_Date_CRD__c &gt;= Current_CSD_ML_at_time_of_Submission__c</errorConditionFormula>
        <errorDisplayField>Submitted_Customer_Request_Date_CRD__c</errorDisplayField>
        <errorMessage>New CRD (CSR) should always be less than Current CSD at time of submission.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Fab_on_Request_and_Lot_levels</fullName>
        <active>true</active>
        <description>Compare if the Lot Selected is from the same Fab as that of the Lot Service Request.</description>
        <errorConditionFormula>TEXT(Manufacturing_Lot__r.Fab_Group__c)  
!= TEXT(Lot_Request__r.Fab__c)</errorConditionFormula>
        <errorDisplayField>Manufacturing_Lot__c</errorDisplayField>
        <errorMessage>This Lot belongs to the different Fab as compared to the Lot Service Request.</errorMessage>
    </validationRules>
