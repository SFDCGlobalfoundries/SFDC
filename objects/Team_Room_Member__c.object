    <validationRules>
        <fullName>ExternalUserRestriction</fullName>
        <active>true</active>
        <description>FV2 : Used for restricting addition of portal users to internal team rooms.</description>
        <errorConditionFormula>AND(( 
User__r.Profile.Name =&apos;Customer Portal Admin&apos; || 
User__r.Profile.Name =&apos;Customer Portal Profile&apos;|| 
User__r.Profile.Name =&apos;Enablement Partner Portal Admin&apos;|| 
User__r.Profile.Name =&apos;Enablement Partner Portal Profile&apos;|| 
User__r.Profile.Name =&apos;Reseller Portal Admin&apos; || 
User__r.Profile.Name =&apos;Reseller Portal Profile&apos;|| 
User__r.Profile.Name =&apos;Supplier Portal Admin&apos; || 
User__r.Profile.Name =&apos;Supplier Portal Profile&apos; ||
User__r.Profile.Name =&apos;Overage Customer Portal Admin&apos; || 
User__r.Profile.Name =&apos;Overage Customer Portal Profile&apos;|| 
User__r.Profile.Name =&apos;Overage Enablement Partner Portal Admin&apos;|| 
User__r.Profile.Name =&apos;Overage Enablement Partner Portal Profile&apos;|| 
User__r.Profile.Name =&apos;Overage Reseller Portal Admin&apos; || 
User__r.Profile.Name =&apos;Overage Reseller Portal Profile&apos;|| 
User__r.Profile.Name =&apos;Overage Supplier Portal Admin&apos; || 
User__r.Profile.Name =&apos;Overage Supplier Portal Profile&apos; 
), Team_Room__r.RecordType.DeveloperName = &apos;Internal_Private&apos;||Team_Room__r.RecordType.DeveloperName =&apos;Internal_Public&apos; )</errorConditionFormula>
        <errorMessage>Internal Team Rooms can not be associated with Portal users.</errorMessage>
    </validationRules>
