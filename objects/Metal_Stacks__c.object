    <validationRules>
        <fullName>WR_MetalOptionType_Existing_Validation</fullName>
        <active>true</active>
        <description>Metal is Required for Metal Option Type Existing.</description>
        <errorConditionFormula>AND(NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c),ISPICKVAL(Metal_Option_Type__c, &apos;Existing&apos;) ,ISBLANK(Metal__c)  )</errorConditionFormula>
        <errorMessage>Metal  cannot be blank, if Metal Option Type is Existing.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WR_MetalOptionType_New_Validation</fullName>
        <active>true</active>
        <description>Metal Option and Metal LB Thickness are required for Metal Option Type New.</description>
        <errorConditionFormula>AND(NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c),ISPICKVAL(Metal_Option_Type__c, &apos;New&apos;) , OR(ISBLANK(Metal_Option__c) , ISBLANK(Metal_LB_Thickness__c)) )</errorConditionFormula>
        <errorMessage>Metal Option and Metal LB Thickness cannot be blank, if Metal Option Type is New.</errorMessage>
    </validationRules>
