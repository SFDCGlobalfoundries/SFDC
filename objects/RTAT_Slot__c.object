    <allowInChatterGroups>true</allowInChatterGroups>
    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <deploymentStatus>Deployed</deploymentStatus>
    <description>This represents the slot in the fab that the new product&apos;s lot will run it.   
It will show all the inputs needed to build the RTAT schedules
It will be linked to the lot&apos;s 3 possible schedules:
  WAFER Build Fab Schedule
  Wafer Finish Schedule
  Module Build Schedule</description>
    <enableActivities>true</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableChangeDataCapture>false</enableChangeDataCapture>
    <enableFeeds>false</enableFeeds>
    <enableHistory>false</enableHistory>
    <enableReports>true</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>ReadWrite</externalSharingModel>
    <fields>
        <fullName>Conversion_Lot_Number__c</fullName>
        <externalId>false</externalId>
        <label>Conversion Lot Number</label>
        <length>20</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ERP_Device_Deliverable_Type__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT(ERP_Device_for_Slot__r.Deliverable_Type__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>ERP Device Deliverable Type</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ERP_Device_Fab__c</fullName>
        <externalId>false</externalId>
        <formula>IF( INCLUDES( ERP_Device_for_Slot__r.Fab__c , &apos;FAB 9&apos;), &apos;FAB 9&apos;, 
     IF(INCLUDES( ERP_Device_for_Slot__r.Fab__c , &apos;FAB 10&apos;), &apos;FAB 10&apos;, 
        IF(INCLUDES( ERP_Device_for_Slot__r.Fab__c , &apos;XFAB&apos;), &apos;XFAB&apos;, &apos;FAB 1-8&apos; )
       )
   )</formula>
        <label>ERP Device Fab</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ERP_Device_Technology__c</fullName>
        <externalId>false</externalId>
        <formula>ERP_Device_for_Slot__r.Technology__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>ERP Device Technology</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ERP_Device_for_Slot__c</fullName>
        <deleteConstraint>Restrict</deleteConstraint>
        <description>The ERP Device that will provide input to create the Slot&apos;s schedules</description>
        <externalId>false</externalId>
        <label>ERP Device</label>
        <referenceTo>ERP_Device__c</referenceTo>
        <relationshipLabel>RTAT Slot</relationshipLabel>
        <relationshipName>RTAT_Slot</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Fab__c</fullName>
        <description>FAB 9= BTV, FAB 10=EFK, XFAB=ALTIS</description>
        <externalId>false</externalId>
        <label>Fab</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <controllingField>Wafer_Size__c</controllingField>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>FAB 9</fullName>
                    <default>false</default>
                    <label>FAB 9</label>
                </value>
                <value>
                    <fullName>FAB 10</fullName>
                    <default>false</default>
                    <label>FAB 10</label>
                </value>
                <value>
                    <fullName>XFAB</fullName>
                    <default>false</default>
                    <label>XFAB</label>
                </value>
            </valueSetDefinition>
            <valueSettings>
                <controllingFieldValue>200MM</controllingFieldValue>
                <valueName>FAB 9</valueName>
            </valueSettings>
            <valueSettings>
                <controllingFieldValue>200MM</controllingFieldValue>
                <valueName>XFAB</valueName>
            </valueSettings>
            <valueSettings>
                <controllingFieldValue>300MM</controllingFieldValue>
                <valueName>FAB 10</valueName>
            </valueSettings>
        </valueSet>
    </fields>
    <fields>
        <fullName>General_Comments__c</fullName>
        <description>Field for notes, comments or directions</description>
        <externalId>false</externalId>
        <label>General Comments</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Internal_Charge_Code__c</fullName>
        <externalId>false</externalId>
        <label>Internal Charge Code</label>
        <length>100</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>MPW__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Multi Part Wafer?</description>
        <externalId>false</externalId>
        <inlineHelpText>Multipart wafer</inlineHelpText>
        <label>MPW</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Mfg_Instructions__c</fullName>
        <description>Instructions for Manufacturing</description>
        <externalId>false</externalId>
        <label>Mfg Instructions</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Not_Committed_Projection_Only__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Check only if the lot is not committed and is only projected</description>
        <externalId>false</externalId>
        <inlineHelpText>Check only if the lot is not committed and is only projected.   This will be conveyed in the SAP order number section in the Customer Deliverable Summary section of the schedule output</inlineHelpText>
        <label>Not Committed - Projection Only</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>SAP_Order_Number__c</fullName>
        <description>Enter the order number from SAP</description>
        <externalId>false</externalId>
        <label>SAP Order Number</label>
        <length>20</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Slot_Status__c</fullName>
        <description>Current status of the slot</description>
        <externalId>false</externalId>
        <inlineHelpText>ACTIVE = work in progress
PUBLISHED = Complete and mailed
CANCELLED = Not to be used</inlineHelpText>
        <label>Slot Status</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>ACTIVE</fullName>
                    <default>true</default>
                    <label>ACTIVE</label>
                </value>
                <value>
                    <fullName>PUBLISHED</fullName>
                    <default>false</default>
                    <label>PUBLISHED</label>
                </value>
                <value>
                    <fullName>CANCELLED</fullName>
                    <default>false</default>
                    <label>CANCELLED</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Slot_Type__c</fullName>
        <externalId>false</externalId>
        <label>Slot Type</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>PROTO</fullName>
                    <default>false</default>
                    <label>PROTO</label>
                </value>
                <value>
                    <fullName>EUH</fullName>
                    <default>false</default>
                    <label>EUH</label>
                </value>
                <value>
                    <fullName>PROD RTAT</fullName>
                    <default>false</default>
                    <label>PROD RTAT</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Slot_Yield_Factor__c</fullName>
        <description>Slot yield factor, only applied to wafers, not chips or modules</description>
        <externalId>false</externalId>
        <label>Slot Yield Factor</label>
        <precision>2</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Technology__c</fullName>
        <externalId>false</externalId>
        <label>Technology</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetName>Technology</valueSetName>
        </valueSet>
    </fields>
    <fields>
        <fullName>Wafer_Size__c</fullName>
        <description>Size of wafers in millimeters</description>
        <externalId>false</externalId>
        <label>Wafer Size</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>200MM</fullName>
                    <default>false</default>
                    <label>200MM</label>
                </value>
                <value>
                    <fullName>300MM</fullName>
                    <default>false</default>
                    <label>300MM</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>What_If_scenario__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Check if this is a what if scenario.   If this is a real slot and schedule, leave unchecked</description>
        <externalId>false</externalId>
        <label>What If scenario</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>RTAT Slot</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>Wafer_Size__c</columns>
        <columns>ERP_Device_for_Slot__c</columns>
        <columns>UPDATEDBY_USER</columns>
        <columns>OBJECT_ID</columns>
        <columns>CREATED_DATE</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
        <language>en_US</language>
    </listViews>
    <nameField>
        <label>RTAT Slot Name</label>
        <type>Text</type>
    </nameField>
    <pluralLabel>RTAT Slots</pluralLabel>
    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>ChooseDev_OR_Tech_and_Fab</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISBLANK(ERP_Device_for_Slot__c), ISPICKVAL(Technology__c,&quot;&quot;) , ISPICKVAL(Fab__c,&quot;&quot;))</errorConditionFormula>
        <errorMessage>Either link to an ERP Device, or choose Technology and Fab</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
    <webLinks>
        <fullName>Add_Finish_Schedule</fullName>
        <availability>online</availability>
        <description>Add a wafer finishing schedule to the RTAT Slot</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Add Finish Schedule</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!RequireScript(&quot;/soap/ajax/10.0/connection.js&quot;)} 
window.open(&apos;/apex/RTATAddFinishingSchedule?varRTATSlotId={!RTAT_Slot__c.Id }&apos;);</url>
    </webLinks>
    <webLinks>
        <fullName>Add_Module_Schedule</fullName>
