    <validationRules>
        <fullName>CE_FB_Fab_Assignment_Mandatory</fullName>
        <active>false</active>
        <description>Team Role as Customer Engineer and Fab Planner should not have Fab Assignment as Null.</description>
        <errorConditionFormula>AND(OR(ISPICKVAL(Team_Role__c,&apos;Customer Engineer&apos;),ISPICKVAL(Team_Role__c,&apos;Fab Planner&apos;)),ISNULL(Fab_Assignment__c))</errorConditionFormula>
        <errorMessage>Customer Engineer and Fab Planner Users must atleast have one Fab assigned</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IBM_Account_Team_Read_Only</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
      NOT(OR($Profile.Name = &apos;System Administrator&apos;,$Profile.Name = &apos;GF System Admin&apos;,$Profile.Name = &apos;GF Integration&apos;)),
      Account__r.RecordType.Name = &apos;IBM Account&apos;,
      OR(
           ISNEW(),
           NOT(ISNEW())
      )
)</errorConditionFormula>
        <errorMessage>Account Team under IBM accounts can&apos;t be created or updated</errorMessage>
    </validationRules>
