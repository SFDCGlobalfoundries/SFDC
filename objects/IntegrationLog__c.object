    <webLinks>
        <fullName>ERP_Sync</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>ERP Sync</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>if(&quot;{!IntegrationLog__c.Object__c}&quot; == &quot;Account&quot;){
	if(&quot;{!IntegrationLog__c.ERP_Status__c}&quot; == &quot;ERROR&quot;){
		if(!{!IntegrationLog__c.Resend_to_ERP__c}){
                        var left = (screen.width/2)-(600/2);
                        var top = (screen.height/2)-(200/2);
			window.open(&quot;/apex/ManualERPSync?id={!IntegrationLog__c.SFDC_Row_ID__c}&amp;logid={!IntegrationLog__c.Id}&quot;,&quot;_blank&quot;,&quot;width=600,height=200,top=&quot;+top+&quot;,left=&quot;+left);
		} else {
			alert(&quot;Record has already been synced to ERP!&quot;);
		}
                       
	} else if(&quot;{!IntegrationLog__c.ERP_Status__c}&quot; == &quot;SUCCESS&quot;){
		alert(&quot;Record has already been synced to ERP!&quot;);
	} else {
		 alert(&quot;This record can&apos;t be synced to ERP!&quot;);
	}
} else {
	alert(&quot;This record can&apos;t be synced to ERP!&quot;);
}</url>
    </webLinks>
