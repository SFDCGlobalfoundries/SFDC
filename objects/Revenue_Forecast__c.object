    <webLinks>
        <fullName>Reset_Forecast</fullName>
        <availability>online</availability>
        <description>Clears the Average Sales Price/Quantity value from Revenue Forecast records.</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Reset Forecast</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/apex.js&quot;)}
if(confirm(&apos;This action will reset Average Sales Price/Quantity to zero for all Revenue Forecast records. Are you sure you want to continue?&apos;)==1){ 

// Now calling the webservice to reset the Average Sales Price/Quantity to zero for all Revenue Forecast
var callback = &quot;&quot; + sforce.apex.execute(&quot;RevenueForecastService&quot;,&quot;resetForecast&quot;, {gmplId:&quot;{!GMPL_BX009__c.Id}&quot;});

// If reset of Average Sales Price/Quantity was ok - just reload GMPL page to see the updated Average Sales Price/Quantity values
if(callback == &apos;OK&apos;){
if(window.parent){
window.parent.location.reload();
}else{
window.location.reload();
}
}
else{
alert(&apos;Error: &apos; + callback);
}

}</url>
