    <fields>
        <fullName>CSR_Data_Check__c</fullName>
        <externalId>false</externalId>
        <formula>IF (Customer_Requested_Wafer_Qty__c = 0,IMAGE(&quot;resource/LotRequestBad&quot;, &quot;&lt;&gt;&quot;, 20,20)+ &quot;Enter Customer Requested Wafer Quantity &quot; ,&quot;&quot;) &amp;
IF (Customer_Requested_Lot_Qty__c &gt; Lots_CSR_Submitted__c,IMAGE(&quot;resource/LotRequestBad&quot;, &quot;&lt;&gt;&quot;, 20,20)+ &quot;Lot Instructions submitted is less than Customer Requested Lot Quantity&quot;,&quot;&quot;) &amp; 
IF (Customer_Requested_Wafer_Qty__c &gt;  Wafers_CSR_Submitted__c ,IMAGE(&quot;resource/LotRequestBad&quot;, &quot;&lt;&gt;&quot;, 20,20)+ &quot;Lot Instructions submitted is less than Customer Requested Wafer Quantity&quot;,&quot;&quot;) &amp; 
IF ( DPML_Table__c = &quot;&quot;, IMAGE(&quot;resource/LotRequestBad&quot;, &quot;&lt;&gt;&quot;, 20,20)+&quot;Enter Fab Cycle Time Table&quot; ,&quot;&quot;)&amp;
/*IF ( Lot_Request_Price_Table__c = &quot;&quot;, IMAGE(&quot;resource/LotRequestBad&quot;, &quot;&lt;&gt;&quot;, 20,20) + &quot;Enter Lot Request Price Table&quot; , &quot;&quot;)&amp; */
IF ( ISPICKVAL(After_Action_Review_of_Expedite__c, &quot;Required&quot;),IMAGE(&quot;resource/LotRequestBad&quot;, &quot;&lt;&gt;&quot;, 20,20)+ &quot;Complete After Action Review&quot;,&quot;&quot;)&amp;
IF ( ISPICKVAL(After_Action_Review_of_Expedite__c, &quot;Completed&quot;),IMAGE(&quot;resource/LotRequestGood&quot;, &quot;&lt;&gt;&quot;, 20,20)+ &quot;After Action Review Completed&quot;,&quot;&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>CSR checks that there is no errors before Submitting for Approval</inlineHelpText>
        <label>CSR Data Check</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
