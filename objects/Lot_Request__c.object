    <fields>
        <fullName>Pricing_Approval_Status__c</fullName>
        <externalId>false</externalId>
        <formula>CASE(  Pricing_Recommendation_PR__c  ,
  &quot;Approved&quot;,
    IMAGE(&quot;resource/Traffic_Light_Approved&quot;,&quot; &quot;,25,50)&amp;&quot;     Approved &quot;,
  &quot;Rejected&quot;,
    IMAGE(&quot;resource/Traffic_Light_Reject&quot;,&quot; &quot;,25,50)&amp;&quot;     Rejected&quot; ,
  &quot;Escalated&quot;,
    IMAGE(&quot;resource/Traffic_Light_Escalated&quot;,&quot; &quot;,25,50)&amp;&quot;     Escalated&quot;,
 ( CASE( Stage__c,
    &quot;New Request&quot;,
        IMAGE(&quot;resource/Traffic_Light_Not_Started&quot;,&quot; &quot;,25,50)&amp;&quot;     Not Started&quot; ,
    &quot;Acknowledged by CSR&quot;,
        IMAGE(&quot;resource/Traffic_Light_Not_Started&quot;,&quot; &quot;,25,50)&amp;&quot;     Not Started&quot;, 
    &quot;Submitted by CSR&quot;,
        IMAGE(&quot;resource/Traffic_Light_Not_Started&quot;,&quot; &quot;,25,50)&amp;&quot;     Not Started&quot; ,
    &quot;Resubmitted by CSR&quot;,
        IMAGE(&quot;resource/Traffic_Light_Not_Started&quot;,&quot; &quot;,25,50)&amp;&quot;     Not Started&quot; ,
    
      ( IF( ISPICKVAL(Does_Customer_Accept_Standard_Pricing__c,&apos;Yes&apos;), 
         (CASE (Stage__c,
            &quot;Submitted For Review by Expedite Manager&quot;, IMAGE(&quot;resource/Traffic_Light_Approval_Not_Required&quot;,&quot; &quot;,25,50) &amp;&quot;     Review Not Required&quot; ,
        &quot;Resubmitted For Review by Expedite Manager&quot;,
           IMAGE(&quot;resource/Traffic_Light_Approval_Not_Required&quot;,&quot; &quot;,25,50)&amp; &quot;     Review Not Required&quot; ,
       IMAGE(&quot;resource/Traffic_Light_Under_Review&quot;,&quot; &quot;,25,50)&amp;&quot;     Not Assessed&quot; )),
     ( CASE (Stage__c,
        &quot;Submitted For Review by Expedite Manager&quot;,
          IMAGE(&quot;resource/Traffic_Light_Review_Required&quot;,&quot; &quot;,25,50)&amp; &quot;     Review Required&quot;  ,
        &quot;Resubmitted For Review by Expedite Manager&quot;,
           IMAGE(&quot;resource/Traffic_Light_Review_Required&quot;,&quot; &quot;,25,50)&amp;&quot;Review Required     &quot; ,
IMAGE(&quot;resource/Traffic_Light_Under_Review&quot;,&quot; &quot;,25,50)&amp;&quot;     Not Assessed&quot;))
     )))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
