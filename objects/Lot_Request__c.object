        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!RequireScript(&quot;/soap/ajax/34.0/connection.js&quot;)}
{!RequireScript(&quot;/soap/ajax/34.0/apex.js&quot;)}
var recId=&apos;{!Lot_Request__c.Id}&apos;;
var date = new Date();
var cDate= new Date(&apos;{!Lot_Request__c.Date_Customer_Submitted_Request__c}&apos;);
var timeDiff = Math.abs(date.getTime() - cDate.getTime());
var diffDays = Math.ceil(timeDiff / (1000 * 3600 * 24));
if(
	&apos;{!User.Profile}&apos; != &apos;System Administrator&apos; &amp;&amp;
	&apos;{!User.Profile}&apos; != &apos;GF System Admin&apos; &amp;&amp;
	&apos;{!User.Profile}&apos; != &apos;GF CSR&apos;
	){
	alert(&apos;You are not authorized to Submit the request for Approval&apos;); 
}
else if(diffDays &gt; 1 &amp;&amp; &apos;{!Lot_Request__c.Reason_for_Delay__c}&apos; == &apos;&apos;){
	alert(&apos;Please provide the Reason for Delay&apos;); 
}
else if(&apos;{!Lot_Request__c.Customer_Requested_Lot_Qty__c}&apos; &lt; 1){
	alert(&apos;Please associate at least one Lot Instruction to proceed.&apos;); 
}
else if(&apos;{!Lot_Request__c.Stage__c}&apos; != &apos;New Request&apos; &amp;&amp; 
	&apos;{!Lot_Request__c.Stage__c}&apos; != &apos;Acknowledged by CSR&apos; &amp;&amp; 
	&apos;{!Lot_Request__c.Stage__c}&apos; != &apos;Closed - Rejected&apos; 
	){ 
	alert(&apos;You cannot Submit the Record for Approval at this stage.&apos;); 
	}
else{
	var message = sforce.apex.execute(&quot;LotApprovalProcessUtil&quot;,&quot;activeMLonLotinstr&quot;, {lsrId:recId});


   
	if(message.toString().indexOf(&apos;LotInstr&apos;) &gt;-1){
		alert(message);
	}else{
	  var url = &apos;{!URLFOR($Action.Lot_Request__c.Submit, Lot_Request__c.Id)}&apos;;
          navigateToUrl(url);
	}
	
}</url>
