        <formula>IF( (TEXT(Stage__c) = &quot;New Request&quot;) || (Text (Stage__c) = &quot;Acknowledged by CSR&quot;), 
	IMAGE(&quot;resource/Traffic_Light_Not_Started&quot;,&quot; &quot;,25,50) &amp; &quot;Not Started&quot;,
	CASE(Expedite_Manager_Recommendation_EM__c, 
		&quot;Approved Under Corridor&quot;, IMAGE(&quot;resource/Traffic_Light_Approved&quot;,&quot; &quot;,25, 50)&amp;&quot;Approved Under Corridor&quot;, 
		&quot;Approved for Planner Feedback&quot;, IMAGE(&quot;resource/Traffic_Light_Approved&quot;,&quot; &quot;,25, 50)&amp;&quot;Approved for Planner Feedback&quot;, 
		&quot;Rejected&quot;, IMAGE(&quot;resource/Traffic_Light_Rejected&quot;,&quot; &quot;, 25, 50)&amp;&quot;Rejected&quot; , 
		&quot;Approval Not Required&quot;, IMAGE(&quot;resource/Traffic_Light_Approved&quot;,&quot; &quot;,25, 50)&amp;&quot;Approval Not Required&quot;, 
		&quot;&quot;, IF(TEXT(Stage__c) = &quot;Submitted for Review by Expedite Manager&quot; ,IMAGE(&quot;resource/Traffic_Light_Under_Review&quot;,&quot; &quot;,25,50)&amp;&quot;Submitted for Review by Expedite Manager&quot;,IMAGE(&quot;resource/Traffic_Light_Not_Started&quot;,&quot; &quot;,25,50)&amp;&quot;Not Started&quot;),
		IMAGE(&quot;resource/Traffic_Light_Under_Review&quot;,&quot; &quot;,25,50)&amp;&quot;Not Assessed&quot;
	)
)</formula>
