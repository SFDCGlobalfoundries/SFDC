    <validationRules>
        <fullName>Date_check_of_Customer_Submitted_Request</fullName>
        <active>true</active>
        <description>If the Customer Submission date is greater than current date , then restrict CSR to create the request.</description>
        <errorConditionFormula>AND( 
Date_Customer_Submitted_Request__c &gt; NOW(), 
NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c), 
$Profile.Name &lt;&gt; &apos;GF Integration&apos;, 
$Profile.Name &lt;&gt; &apos;System Administrator&apos;)</errorConditionFormula>
        <errorDisplayField>Date_Customer_Submitted_Request__c</errorDisplayField>
        <errorMessage>You cannot select the future date. Please correct.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_check_of_New_CRD_CSR</fullName>
        <active>true</active>
        <description>If the New CRD (CSR) date is less than current date , then restrict CSR to create the request.</description>
        <errorConditionFormula>New_Customer_Requested_Date_CRD_CSR__c &lt; TODAY() &amp;&amp;
OR(
ISPICKVAL(Stage__c, &apos;New Request&apos;),
ISPICKVAL(Stage__c, &apos;Acknowledged by CSR&apos;),
ISPICKVAL(Stage__c, &apos;Closed - Rejected&apos;)
)</errorConditionFormula>
        <errorDisplayField>New_Customer_Requested_Date_CRD_CSR__c</errorDisplayField>
        <errorMessage>You cannot select the past date for New CRD (CSR). Please correct.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_Check_for_Able_to_Improve_CSD</fullName>
        <active>false</active>
        <description>Mandatory for the planners to set the check box for Able to Improve CSD if the EM Recommendation is &quot;Approve Under Corridor&quot; for all the Lot Instructions before the request can be approved.</description>
        <errorConditionFormula>AND(
    ISPICKVAL(Expedite_Manager_Recommendation_EM__c, &quot;Approved Under Corridor&quot;),
    ISPICKVAL(PRIORVALUE(Stage__c),&quot;Pending Planner Response&quot;),
    ISPICKVAL(Stage__c,&quot;Fully Approved&quot;),
    Lots_Planner_Able_to_Support__c &lt; Customer_Requested_Lot_Qty__c
)</errorConditionFormula>
        <errorMessage>Mandatory to check &quot;Able to Improve CSD&quot; for all the Lot Instructions before the request can be approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_Customer_Reason_for_Discount</fullName>
        <active>true</active>
        <description>If &quot;Does Customer Accept Standard Pricing ?&quot;= No, Mandatory to fill Customer&apos;s Reason for Discounting (CSR).</description>
        <errorConditionFormula>AND(
  ISPICKVAL(Does_Customer_Accept_Standard_Pricing__c,&apos;No&apos;),
  ISBLANK(Customer_s_Reason_for_Discounting_CSR__c)
)</errorConditionFormula>
        <errorDisplayField>Customer_s_Reason_for_Discounting_CSR__c</errorDisplayField>
        <errorMessage>Please provide the reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_Customer_Req_Price_Per_Lot</fullName>
        <active>false</active>
        <description>If &quot;Does Customer Accept Standard Pricing ?&quot;= No, Mandatory to fill Customer Requested Price (per Lot).</description>
        <errorConditionFormula>AND(
  ISPICKVAL(Does_Customer_Accept_Standard_Pricing__c,&apos;No&apos;),
  ISNULL(Customer_Requested_Price_per_Lot__c)
)</errorConditionFormula>
        <errorDisplayField>Customer_Requested_Price_per_Lot__c</errorDisplayField>
        <errorMessage>Please provide the value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_Customer_Req_Price_Total_Price</fullName>
        <active>true</active>
        <description>If &quot;Does Customer Accept Standard Pricing ?&quot;= No, Mandatory to fill Customer Requested Total Price (For All).</description>
        <errorConditionFormula>AND(
  ISPICKVAL(Does_Customer_Accept_Standard_Pricing__c,&apos;No&apos;),
  ISNULL(Customer_Requested_Total_Price_For_All__c)
)</errorConditionFormula>
        <errorDisplayField>Customer_Requested_Total_Price_For_All__c</errorDisplayField>
        <errorMessage>Please provide the value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_EM_Comments_upon_EM_Recom</fullName>
        <active>true</active>
        <description>Expedite Manager Comments are mandatory to be filled when the EM selects the recommendation.</description>
        <errorConditionFormula>AND(
(
  ISPICKVAL(Expedite_Manager_Recommendation_EM__c,&apos;Approved Under Corridor&apos;) ||
  ISPICKVAL(Expedite_Manager_Recommendation_EM__c,&apos;Approved for Planner Feedback&apos;) ||
  ISPICKVAL(Expedite_Manager_Recommendation_EM__c,&apos;Rejected&apos;)
),
LEN(Expedite_Manager_Comments__c) = 0
)</errorConditionFormula>
        <errorDisplayField>Expedite_Manager_Comments__c</errorDisplayField>
        <errorMessage>Please provide the Comments.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_Pricing_Discussion_Detail</fullName>
        <active>true</active>
        <description>If &quot;Does Customer Accept Standard Pricing ?&quot;= No, Mandatory to fill Pricing Discussion Detail (CSR).</description>
        <errorConditionFormula>AND(
  ISPICKVAL(Does_Customer_Accept_Standard_Pricing__c,&apos;No&apos;),
  LEN(Pricing_Discussion_Detail_CSR__c) &lt; 1
)</errorConditionFormula>
        <errorDisplayField>Pricing_Discussion_Detail_CSR__c</errorDisplayField>
        <errorMessage>Please provide the comments.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Message_check_of_Reason_for_Delay</fullName>
        <active>false</active>
        <description>If the Customer Submission date is more than 1 day, then CSR needs to provide the reason for delay</description>
        <errorConditionFormula>ISNEW() &amp;&amp;
NOW() - Date_Customer_Submitted_Request__c &gt; 1 &amp;&amp;
LEN(Reason_for_Delay__c) &lt; 1</errorConditionFormula>
        <errorDisplayField>Reason_for_Delay__c</errorDisplayField>
        <errorMessage>Please provide the Reason for Delay</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Account_Change</fullName>
        <active>true</active>
        <description>Restrict the Change of Account when the Lot Service Request is sent for Approval.</description>
        <errorConditionFormula>!ISPICKVAL(Stage__c,&apos;New Request&apos;) &amp;&amp;
!ISPICKVAL(Stage__c,&apos;Acknowledged by CSR&apos;) &amp;&amp;
ISCHANGED(Account__c)</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>Account cannot be modified during Approval of the Request.</errorMessage>
    </validationRules>
