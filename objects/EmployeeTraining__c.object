Parent_Employee_Training__r.Parent_Employee_Training__r.Parent_Employee_Training__r.Order__c, 
IF( 
NOT(ISBLANK(Parent_Employee_Training__r.Parent_Employee_Training__c)), 
Parent_Employee_Training__r.Parent_Employee_Training__r.Order__c, 
IF( 
NOT(ISBLANK(Parent_Employee_Training__c)), 
Parent_Employee_Training__r.Order__c, 
NULL 
) 
) 
) 
) 
)</formula>
