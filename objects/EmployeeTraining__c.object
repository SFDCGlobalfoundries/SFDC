        <fullName>Category_Level__c</fullName>
        <description>Formula to calculate the Hierarchy Level in the Category Hierarchy</description>
        <externalId>false</externalId>
        <formula>IF(NOT(ISBLANK(Parent_Employee_Training__r.Parent_Employee_Training__r.Parent_Employee_Training__r.Parent_Employee_Training__r.Parent_Employee_Training__c)), 6,
 IF(NOT(ISBLANK(Parent_Employee_Training__r.Parent_Employee_Training__r.Parent_Employee_Training__r.Parent_Employee_Training__c)), 5,
 IF(NOT(ISBLANK(Parent_Employee_Training__r.Parent_Employee_Training__r.Parent_Employee_Training__c)), 4,
   IF(NOT(ISBLANK(Parent_Employee_Training__r.Parent_Employee_Training__c)), 3,
    IF(NOT(ISBLANK(Parent_Employee_Training__c)), 2, 1
    )
   )
  )
 )
)</formula>
