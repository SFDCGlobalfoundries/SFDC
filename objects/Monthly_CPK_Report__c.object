    <webLinks>
        <fullName>Reopen</fullName>
        <availability>online</availability>
        <description>Re-Open the Report Folder after approval</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Reopen</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)}

var monthlyReportRec = new sforce.SObject(&quot;Monthly_CPK_Report__c&quot;);
monthlyReportRec.id = &apos;{!Monthly_CPK_Report__c.Id}&apos;;
monthlyReportRec.Approved__c = &apos;{!Monthly_CPK_Report__c.Approved__c}&apos;
var queryResult = sforce.connection.query(&quot;SELECT Id, DeveloperName FROM RecordType WHERE DeveloperName = &apos;MonthlyCPKSubmitForApproval&apos; LIMIT 1&quot;);

/*Change Stage to Draft*/
if(monthlyReportRec.Approved__c == &apos;Approved&apos; || monthlyReportRec.Approved__c == &apos;No Reports&apos;) {
	monthlyReportRec.Approved__c = &apos;Draft&apos;; 
	monthlyReportRec.RecordTypeId = queryResult.getArray(&quot;records&quot;)[0].Id;

	var result = sforce.connection.update([monthlyReportRec]);

	if (result[0].success == &apos;false&apos;) {
		if(result[0].errors.message == &quot;insufficient access rights on object id&quot;){
			alert(&quot;Only Report Owners and Approvers can Reopen the Record&quot;);
		}
	} else {
		location.reload(true); 
	}
}</url>
