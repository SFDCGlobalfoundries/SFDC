        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;https://code.jquery.com/jquery-1.10.2.js&quot;)} 
{!REQUIRESCRIPT(&quot;https://code.jquery.com/ui/1.10.4/jquery-ui.js&quot;)} 
{!REQUIRESCRIPT(&quot;/resource/modalJS&quot;)}

j$(&quot;#modalDiv&quot;).dialog(&apos;option&apos;,&apos;title&apos;,&apos;{!Monthly_CPK_Report__c.Name}&apos;).dialog(&apos;open&apos;);</url>
