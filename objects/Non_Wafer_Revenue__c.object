if(Current_Forecasted_Tapeout_Date__c &gt;= date(2011, 4, 3), &quot;2011-Q2&quot;, 
if( Current_Forecasted_Tapeout_Date__c &gt;= date(2010, 12, 26), &quot;2011-Q1&quot;, 
