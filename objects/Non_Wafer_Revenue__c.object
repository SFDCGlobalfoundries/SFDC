if(Current_Forecasted_Tapeout_Date__c &gt;= date(2012, 9, 30), &quot;2012-Q4&quot;, 
if(Current_Forecasted_Tapeout_Date__c &gt;= date(2012, 7, 1), &quot;2012-Q3&quot;, 
if( Current_Forecasted_Tapeout_Date__c &gt;= date(2012, 4, 1), &quot;2012-Q2&quot;, 
if(Current_Forecasted_Tapeout_Date__c &gt;= date(2012, 1, 1), &quot;2012-Q1&quot;, 
