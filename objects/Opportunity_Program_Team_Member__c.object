        <externalId>false</externalId>
        <formula>LEFT(
	(
		IF(INCLUDES(User__r.Fab_Assigned__c,&apos;FAB 1&apos;),&apos;FAB 1;&apos;,&apos;&apos;) +
		IF(INCLUDES(User__r.Fab_Assigned__c,&apos;FAB 2&apos;),&apos;FAB 2;&apos;,&apos;&apos;) +
		IF(INCLUDES(User__r.Fab_Assigned__c,&apos;FAB 3&apos;),&apos;FAB 3;&apos;,&apos;&apos;) +
		IF(INCLUDES(User__r.Fab_Assigned__c,&apos;FAB 3E&apos;),&apos;FAB 3E;&apos;,&apos;&apos;) +
