IF(INCLUDES(Account_Team_Proxy__r.Fab_Assignment__c,&apos;FAB 7&apos;),&apos;FAB 7;&apos;,&apos;&apos;) + 
IF(INCLUDES(Account_Team_Proxy__r.Fab_Assignment__c,&apos;FAB 8&apos;),&apos;FAB 8;&apos;,&apos;&apos;) + 
