IF(INCLUDES(Account_Team_Proxy__r.Fab_Assignment__c,&apos;FAB 3E&apos;),&apos;FAB 3E;&apos;,&apos;&apos;) + 
IF(INCLUDES(Account_Team_Proxy__r.Fab_Assignment__c,&apos;FAB 5&apos;),&apos;FAB 5;&apos;,&apos;&apos;) + 
IF(INCLUDES(Account_Team_Proxy__r.Fab_Assignment__c,&apos;FAB 6&apos;),&apos;FAB 6;&apos;,&apos;&apos;) + 
IF(INCLUDES(Account_Team_Proxy__r.Fab_Assignment__c,&apos;FAB 7&apos;),&apos;FAB 7;&apos;,&apos;&apos;) + 
IF(INCLUDES(Account_Team_Proxy__r.Fab_Assignment__c,&apos;FAB 8&apos;),&apos;FAB 8;&apos;,&apos;&apos;) + 
IF(INCLUDES(Account_Team_Proxy__r.Fab_Assignment__c,&apos;FAB 9&apos;),&apos;FAB 9;&apos;,&apos;&apos;) +
IF(INCLUDES(Account_Team_Proxy__r.Fab_Assignment__c,&apos;FAB 10&apos;),&apos;FAB 10;&apos;,&apos;&apos;) + 
IF(INCLUDES(Account_Team_Proxy__r.Fab_Assignment__c,&apos;FAB 11&apos;),&apos;FAB 11;&apos;,&apos;&apos;) + 
IF(INCLUDES(Account_Team_Proxy__r.Fab_Assignment__c,&apos;ALTIS&apos;),&apos;ALTIS;&apos;,&apos;&apos;) 
), 
( 
LEN( 
( 
IF(INCLUDES(Account_Team_Proxy__r.Fab_Assignment__c,&apos;FAB 1&apos;),&apos;FAB 1;&apos;,&apos;&apos;) + 
IF(INCLUDES(Account_Team_Proxy__r.Fab_Assignment__c,&apos;FAB 2&apos;),&apos;FAB 2;&apos;,&apos;&apos;) + 
