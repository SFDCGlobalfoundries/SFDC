    <validationRules>
        <fullName>Adjusted_Demand_and_PO_Coverage_Missing</fullName>
        <active>true</active>
        <description>Adjusted Demand and PO Coverage input must be  at least 1 entry and can be 0</description>
        <errorConditionFormula>AND(ISNULL(Qtr_0__c), ISNULL(Qtr_0_PO_Coverage__c), ISNULL(Qtr_1__c), ISNULL(Qtr_1_PO_Coverage__c), ISNULL(Qtr_2__c), ISNULL(Qtr_2_PO_Coverage__c), ISNULL(Qtr_3__c), ISNULL(Qtr_3_PO_Coverage__c)) &amp;&amp; ISPICKVAL(Status__c, &quot;Draft&quot;)</errorConditionFormula>
        <errorDisplayField>Qtr_3__c</errorDisplayField>
        <errorMessage>The above Qtr 0,1,2,3 Adjusted Demand and PO Coverage input must have at least 1 entry and can be 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Balance_Quantity_Validation</fullName>
        <active>true</active>
        <description>Balance_Quantity_Validation</description>
        <errorConditionFormula>IF(Balance_Qty__c  &lt; 0, TRUE, FALSE)</errorConditionFormula>
        <errorMessage>Balance Quantity Cannot be less than 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Business_Unit_required_for_Fab_9_10</fullName>
        <active>true</active>
        <description>Business Unit is required</description>
        <errorConditionFormula>AND(ISPICKVAL(Status__c,&apos;Draft&apos;), ISBLANK(Text(Business_Unit__c)))</errorConditionFormula>
        <errorDisplayField>Business_Unit__c</errorDisplayField>
        <errorMessage>Please select the business unit of the Risk Start Part.
Business Unit will be used to route Risk Start to respective PLM approvers.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cum_Revenue_Required_in_Fab_8</fullName>
        <active>true</active>
        <errorConditionFormula>ISNULL(Cum_Revenue__c) &amp;&amp; ISPICKVAL(Status__c, &quot;Draft&quot;) &amp;&amp;   ISPICKVAL(FAB__c, &quot;Fab 8&quot;)</errorConditionFormula>
        <errorDisplayField>Cum_Revenue__c</errorDisplayField>
        <errorMessage>Cum Revenue $ is mandatory in Fab 8 Request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Customer_Design_Change_Expected</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(Status__c,&apos;Pending Sales Account Manager Inputs&apos;), ISPICKVAL(Is_customer_design_change_expected__c,&apos;Yes&apos;),  ISBLANK(If_Yes_state_revision_layers_and_expe__c) )</errorConditionFormula>
        <errorDisplayField>If_Yes_state_revision_layers_and_expe__c</errorDisplayField>
        <errorMessage>Field &quot;If Yes, Revision Layers , Expected Time&quot; is mandatory when &quot;Is customer design change expected&quot; field value is &quot;Yes&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ED_should_not_be_less_that_PSD</fullName>
        <active>true</active>
        <description>End Date should not be ess than Plan Start Date</description>
        <errorConditionFormula>IF((End_Date__c &lt;   DATEVALUE( CreatedDate ) ) || (End_Date__c &lt;  Plan_Start_Date__c ), true, false)</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>Plan End date should not be null or should not be less than Plan Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ExpShipDate_grater_than_Exp_Po_Date</fullName>
        <active>true</active>
        <description>Expected Shipment Date should be greater than Expected PO Date</description>
        <errorConditionFormula>IF(Expected_Shipment_Date__c &lt;= Expected_PO_Date__c, true, false)</errorConditionFormula>
        <errorDisplayField>Expected_Shipment_Date__c</errorDisplayField>
        <errorMessage>Expected Shipment Date should be greater than Expected PO Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Exp_PO_date_should_not_be_past_date</fullName>
        <active>true</active>
        <description>Exp_PO_date_should_not_be_past_date</description>
        <errorConditionFormula>IF((Expected_PO_Date__c  &lt;  DATEVALUE( CreatedDate ) ) || (Expected_PO_Date__c  &lt; Plan_Start_Date__c ), true, false)</errorConditionFormula>
        <errorDisplayField>Expected_PO_Date__c</errorDisplayField>
        <errorMessage>Expected PO date should not be a past date or should not be less than Plan Start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Planning_Endorsement_is_selected_No</fullName>
        <active>true</active>
        <errorConditionFormula>IF( TEXT( Planning_Endorsement__c ) = &apos;No&apos;  &amp;&amp;  ISBLANK( Enter_Reason_if_Planning_Endorsment_No__c )  , TRUE, FALSE)</errorConditionFormula>
        <errorDisplayField>Enter_Reason_if_Planning_Endorsment_No__c</errorDisplayField>
        <errorMessage>Please key in the reason for selecting Planning Endorsement as No</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Is_Device_Qualified_Required_in_Fab_8</fullName>
        <active>false</active>
        <errorConditionFormula>ISBlank(Is_PX_Requested_Device_Qualified__c) &amp;&amp; ISPICKVAL(Status__c, &quot;Draft&quot;) &amp;&amp;   ISPICKVAL(FAB__c, &quot;Fab 8&quot;)
&amp;&amp; $Profile.Name  &lt;&gt; &apos;GF Integration&apos;</errorConditionFormula>
        <errorDisplayField>Is_PX_Requested_Device_Qualified__c</errorDisplayField>
        <errorMessage>Is PX Requested Device Qualified? is mandatory in Fab 8 Request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PX_Discounted_Status</fullName>
        <active>false</active>
        <errorConditionFormula>IF( TEXT(PX_discounted_for_past_12_mths__c) = &apos;Yes&apos;  &amp;&amp;  ISBLANK(If_Yes_state_revision_discount__c)  , TRUE, FALSE)
&amp;&amp; $Profile.Name  &lt;&gt; &apos;GF Integration&apos;</errorConditionFormula>
        <errorDisplayField>If_Yes_state_revision_discount__c</errorDisplayField>
        <errorMessage>This field is mandatory if PX Discounted for Past 12 Months is Yes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PX_Request_Revenue_Required_in_Fab_8</fullName>
        <active>true</active>
        <errorConditionFormula>ISNULL(PX_Request_Revenue__c) &amp;&amp; ISPICKVAL(Status__c, &quot;Draft&quot;) &amp;&amp;   ISPICKVAL(FAB__c, &quot;Fab 8&quot;)</errorConditionFormula>
        <errorDisplayField>PX_Request_Revenue__c</errorDisplayField>
        <errorMessage>PX Request Revenue is mandatory in Fab 8 Request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Planned_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Plan_Start_Date__c &lt;  DATEVALUE( CreatedDate) ,TRUE,FALSE)</errorConditionFormula>
        <errorDisplayField>Plan_Start_Date__c</errorDisplayField>
        <errorMessage>Plan Start Date cannot be a past date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_for_Request_is_required</fullName>
        <active>true</active>
        <description>Reason for Request is required</description>
        <errorConditionFormula>ISPICKVAL(Status__c , &quot;Draft&quot;) &amp;&amp; ISPICKVAL( Reason_for_Request__c,&quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Reason_for_Request__c</errorDisplayField>
        <errorMessage>Reason for Request is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requesting_Customer_or_Device_Missing</fullName>
        <active>true</active>
        <description>Requesting Customer or Requested Device Missing; BTS, WIP, FG must be at least 1 entry, can be 0</description>
        <errorConditionFormula>AND(ISBlank(PX_BTS1__c), ISBlank(PX_BTS3__c), ISBlank(PX_WIP_FG1__c), ISBlank(PX_WIP_FG3__c))&amp;&amp; (  RecordType.Name  =  &quot;Customer Planning&quot;)</errorConditionFormula>
        <errorDisplayField>PX_WIP_FG1__c</errorDisplayField>
        <errorMessage>Requesting Customer or Requested Device Missing BTS, WIP,+FG #. Please update PX BTS &amp; PX WIP + FG fields above. Must have at least 1 entry, can be 0.</errorMessage>
    </validationRules>
