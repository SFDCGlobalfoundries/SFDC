    <fields>
        <fullName>Age_Pricing__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ISNULL ( Submitted_Date__c )
, FLOOR( NOW() - CreatedDate ) + ( MOD( NOW()  - CreatedDate, 1 )  * 24)/100
, FLOOR( Submitted_Date__c  -  CreatedDate ) + ( MOD ( Submitted_Date__c  -  CreatedDate , 1)  *  24 )  /  100)</formula>
        <label>Age of Pricing Team Input</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Age__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISNULL( Conversion_Complete_Action_Date__c )
, FLOOR(NOW() - CreatedDate) + (MOD(NOW() - CreatedDate, 1) * 24)/100
, FLOOR(Conversion_Complete_Action_Date__c - CreatedDate) + (MOD(Conversion_Complete_Action_Date__c - CreatedDate, 1) * 24)/100)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Age Risk Start</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
