    <fields>
        <fullName>Current_Grand_Total__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
	Workflow_Stage__c == &apos;RMA Raised&apos; ||
	Workflow_Stage__c == &apos;Rejected RMA&apos; ||
	Workflow_Stage__c == &apos;&apos; ||
        Workflow_Stage__c == &apos;Pending for Quality Validation&apos;,
	Grand_Total_Price__c,
	IF( /*Workflow_Stage__c == &apos;Pending for Quality Validation&apos; ||*/
            Workflow_Stage__c == &apos;Pending for Fab Validation&apos; || 
	    Workflow_Stage__c == &apos;Pending for Turnkey/ Sub-con Validation&apos; || 
	    Workflow_Stage__c == &apos;Pending for Non Fab Category Validation&apos; ||
            Workflow_Stage__c == &apos;Pending for Product Engineering Validation&apos;,
	    Grand_Total_FAB_Validation__c,
            IF(
		OR(
		ISPICKVAL(RMA_Category__c,&apos;Failure Analysis&apos;),
		ISPICKVAL(RMA_Category__c,&apos;Technical/ Turnkey&apos;) &amp;&amp; ISPICKVAL(ReasonCode__c,&apos;R50&apos;)
		),
		Grand_Total_Price__c,
		Grand_Total_Final__c
	    )	
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Current Grand Total</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
