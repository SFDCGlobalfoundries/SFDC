    <validationRules>
        <fullName>CSR_Queue_Validation</fullName>
        <active>true</active>
        <description>This validation rule checks if Queue in time is greater than Queue out time.</description>
        <errorConditionFormula>AND(
Queue_in_Time_for_CSR_Submission__c &gt; Queue_out_Time_for_CSR_Submission__c,
NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c),
$Profile.Name &lt;&gt; &apos;GF Integration&apos;
)</errorConditionFormula>
        <errorMessage>Queue-in Time for CSR Submission cannot be greater than Queue-out Time for CSR Submission</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fab_Queue_Validation</fullName>
        <active>true</active>
        <description>This validation rule checks if Queue in time is greater than Queue out time.</description>
        <errorConditionFormula>AND(
Queue_in_Time_for_Fab_Validation__c &gt; Queue_out_Time_for_Fab_Validation__c,
NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c),
$Profile.Name &lt;&gt; &apos;GF Integration&apos;
)</errorConditionFormula>
        <errorMessage>Queue-in Time for Fab Validation cannot be greater than	Queue-out Time for Fab Validation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Finance_Approval_Queue_Validation</fullName>
        <active>true</active>
        <description>This validation rule checks if Queue in time is greater than Queue out time.</description>
        <errorConditionFormula>AND(
Queue_in_Time_for_Finance_Approval__c&gt; Queue_out_Time_for_Finance_Approval__c,
NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c),
$Profile.Name &lt;&gt; &apos;GF Integration&apos;
)</errorConditionFormula>
        <errorMessage>Queue-in Time for Finance Approval cannot be greater than Queue-out Time for Finance Approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RMA_FailureAnalysis_CompType_NoComp</fullName>
        <active>true</active>
        <description>Compensation Type for RMA Category = &apos;Failure Analysis&apos; can only be &apos;No Compensation&apos;</description>
        <errorConditionFormula>(ISCHANGED(RMA_Category__c) || ISCHANGED(Compensation_Type__c) ) &amp;&amp; 
ISPICKVAL(RMA_Category__c,&apos;Failure Analysis&apos;) &amp;&amp;
!ISPICKVAL(RMA_Category__c,&apos;&apos;) &amp;&amp; 
!ISPICKVAL(Compensation_Type__c,&apos;No Compensation&apos;)</errorConditionFormula>
        <errorDisplayField>Compensation_Type__c</errorDisplayField>
        <errorMessage>Compensation Type should be &apos;No Compensation&apos; for &apos;Failure Analysis&apos; category.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Regional_Approval_Queue_Validation</fullName>
        <active>true</active>
        <description>This validation rule checks if Queue in time is greater than Queue out time.</description>
        <errorConditionFormula>AND(
Queue_in_Time_for_Regional_Approval__c&gt; Queue_out_Time_for_Regional_Approval__c,
NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c),
$Profile.Name &lt;&gt; &apos;GF Integration&apos;
)</errorConditionFormula>
        <errorMessage>Queue-in Time for Regional Approval cannot be greater than Queue-out Time for Regional Approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_Customer_Requested_date</fullName>
        <active>true</active>
        <description>This validation rule is used to prevent user from entering future date.</description>
        <errorConditionFormula>AND(
Customer_Request_Date__c &gt; NOW(),
NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c),
$Profile.Name &lt;&gt; &apos;GF Integration&apos;
)</errorConditionFormula>
        <errorMessage>Customer requested date cannot be future date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Wafer_Acknowledgement_Queue_Validation</fullName>
        <active>true</active>
        <description>This validation rule checks if Queue in time is greater than Queue out time.</description>
        <errorConditionFormula>AND(
Queue_in_Time_for_Wafer_Acknowledgement__c &gt; Queue_out_Time_for_Wafer_Acknowledgement__c,
NOT($Setup.IntegrationUsers__c.Skip_Validation_Rule__c),
$Profile.Name &lt;&gt; &apos;GF Integration&apos;
)</errorConditionFormula>
        <errorDisplayField>Access_Impact__c</errorDisplayField>
        <errorMessage>Queue-in Time for Wafer Acknowledgement cannot be greater than Queue-out Time for Wafer Acknowledgement</errorMessage>
    </validationRules>
