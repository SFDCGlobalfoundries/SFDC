    <validationRules>
        <fullName>Lot_Prefix_validation</fullName>
        <active>true</active>
        <description>Lot prefix name should not contain &apos;O&apos; or &apos;0&apos;</description>
        <errorConditionFormula>OR(CONTAINS(Name,&apos;0&apos;),CONTAINS(Name,&apos;o&apos;),CONTAINS(Name,&apos;O&apos;))</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Lot Prefix should not contain &apos;0&apos; or &apos;o&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Lot_Prefix_Name</fullName>
        <active>true</active>
        <description>Imposes a 5 character limit to Lot Prefix Name.</description>
        <errorConditionFormula>LEN(Name) &gt; 5</errorConditionFormula>
        <errorMessage>Lot Prefix Name can only have up to five (5) characters.</errorMessage>
    </validationRules>
