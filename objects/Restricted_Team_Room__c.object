    <validationRules>
        <fullName>R_Team_Room_Account_is_Mandatory</fullName>
        <active>false</active>
        <errorConditionFormula>AND 
( 
OR(RecordType.DeveloperName == &apos;GLOBALFOUNDRIES_with_Single_party&apos;, 
RecordType.DeveloperName == &apos;GLOBALFOUNDRIES_with_Multi_Party&apos;) , 
ISBLANK( R_Primary_Account__c ) )</errorConditionFormula>
        <errorMessage>Primary account cannot be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>R_Team_Room_Description_not_editable</fullName>
        <active>true</active>
        <errorConditionFormula>AND 
( 
RecordType.DeveloperName &lt;&gt; &apos;GLOBALFOUNDRIES_Internal&apos;, 
NOT(ISNEW()), 
ISCHANGED(  R_Team_Room_Description__c ), 
NOT 
( 
$Profile.Name =&apos;GF Global Admin&apos;|| 
$Profile.Name =&apos;GF Regional Admin&apos;|| 
$Profile.Name =&apos;GF System Admin&apos;|| 
$Profile.Name =&apos;GF Training Admin&apos; || 
$Profile.Name =&apos;System Administrator&apos; 
) 
)</errorConditionFormula>
        <errorMessage>Team Room Description is editable only by admin profiles</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>R_Team_Room_name_not_editable</fullName>
        <active>true</active>
        <errorConditionFormula>AND 
( 
RecordType.DeveloperName &lt;&gt; &apos;GLOBALFOUNDRIES_Internal&apos;, 
NOT(ISNEW()), 
ISCHANGED(Name), 
NOT 
( 
$Profile.Name =&apos;Customer Portal Admin&apos; || 
$Profile.Name =&apos;Enablement Partner Portal Admin&apos;|| 
$Profile.Name =&apos;GF Global Admin&apos;|| 
$Profile.Name =&apos;GF Regional Admin&apos;|| 
$Profile.Name =&apos;GF System Admin&apos;|| 
$Profile.Name =&apos;GF Training Admin&apos; || 
$Profile.Name =&apos;Reseller Portal Admin&apos; || 
$Profile.Name =&apos;Supplier Portal Admin&apos; || 
$Profile.Name =&apos;System Administrator&apos; 
) 
)</errorConditionFormula>
        <errorMessage>Team Room Name is only editable by admin profiles</errorMessage>
    </validationRules>
