    <validationRules>
        <fullName>Account_Unauthorized</fullName>
        <active>true</active>
        <description>This account is been marked as unauthorized, kindly check with GTC team for further clarification.</description>
        <errorConditionFormula>Contact.Account.Unauthorized__c = true</errorConditionFormula>
        <errorMessage>This account is been marked as unauthorized, kindly check with GTC team for further clarification.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_active_users_for_Inactive_account</fullName>
        <active>true</active>
        <description>FV2: validation rule to restrict the activation of users for inactive account</description>
        <errorConditionFormula>ISPICKVAL(Contact.Account.Stage__c ,&apos;Inactive&apos;) &amp;&amp; IsActive =True</errorConditionFormula>
        <errorMessage>Can not activate a user for inactive account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Department_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>IsActive  &amp;&amp;  !ISNULL(ContactId) &amp;&amp;   !ISBLANK(ContactId) &amp;&amp; ISNULL(Contact.Department__c) &amp;&amp;  ISPICKVAL(UserType, &apos;PowerCustomerSuccess&apos;)</errorConditionFormula>
        <errorMessage>Department (GFV Access) is mandatory for portal users. Now, click &quot;Cancel&quot;, go back to Contact Page and fill up the Department (GFV Access) attribute.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Email_Need_to_Be_Same_as_Contact_Email</fullName>
        <active>true</active>
        <errorConditionFormula>AND(!ISBLANK(Email),!ISBLANK(ContactId),NOT(CONTAINS(Contact.Account.Valid_Email_Domain__c,RIGHT(Email,LEN(Contact.Email)- FIND(&quot;@&quot;,Contact.Email)))),!ISBLANK( Contact.Account.Valid_Email_Domain__c ),!(Contains(Email,&quot;@globalfoundries.com&quot;)))</errorConditionFormula>
        <errorDisplayField>Email</errorDisplayField>
        <errorMessage>The email domain does not match the valid domain mentioned in the account</errorMessage>
    </validationRules>
