    <validationRules>
        <fullName>Actual_tapeout_date_editable_fab9_10</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISNEW(),NOT(ISNEW())), 
ISBLANK(Actual_Tapeout_Date_FAB_9_and_FAB_10__c), 
OR(TEXT( Device__r.Fab__c ) = &quot;FAB 9&quot;, 
TEXT(Device__r.Fab__c) = &quot;FAB 10&quot;))</errorConditionFormula>
        <errorDisplayField>Actual_Tapeout_Date_FAB_9_and_FAB_10__c</errorDisplayField>
        <errorMessage>Please populate Actual Tapeout Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Actual_tapeout_date_not_editable_fab1To8</fullName>
        <active>false</active>
        <errorConditionFormula>AND(OR(ISNEW(),ISCHANGED( Actual_Tapeout_Date_FAB_9_and_FAB_10__c)), 
NOT(ISBLANK(Actual_Tapeout_Date_FAB_9_and_FAB_10__c)), 

TEXT( Device__r.Fab__c ) &lt;&gt; &quot;FAB 9&quot;, 
TEXT(Device__r.Fab__c) &lt;&gt; &quot;FAB 10&quot;)</errorConditionFormula>
        <errorDisplayField>Actual_Tapeout_Date_FAB_9_and_FAB_10__c</errorDisplayField>
        <errorMessage>You can populate this actual tapeout date only from fab 9 or fab 10</errorMessage>
    </validationRules>
