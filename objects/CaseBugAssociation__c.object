        <fullName>CAD_Bug</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Copy to new CAD Bug</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/apex.js&quot;)} 
try { 
var query = sforce.connection.query(&quot;select id, name from RecordType where IsActive = true and SobjectType=&apos;Bug__c&apos; and Name =&apos;{!$Label.BugCADRecordTypeName}&apos;&quot;); 

var records = query.getArray(&apos;records&apos;); 

if(records != null){ 
var recordTypeId = records[0].Id; 
window.location.href = &apos;/apex/BugCADCreate?nooverride=1&amp;rid=&apos;+recordTypeId+&apos;&amp;caseid={!Case.Id}&amp;&amp;type=copy&apos;;
} 
} 
catch(e){ 
alert(&apos;An Error has Occured. Error:&apos; +e); 
}</url>
