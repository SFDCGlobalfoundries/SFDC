        <fullName>Status_GUI__c</fullName>
        <externalId>false</externalId>
        <formula>IF ( ISPICKVAL(Stage__c, &quot;RFQ Cancelled&quot;)  ||  ISPICKVAL(Stage__c, &quot;No Bid&quot;), Image(&quot;resource/RFQ_Tracker_Gray&quot;,&quot;&quot;,25,40),
IF ( NOT( ISBLANK( Quote_Sent_to_Customer_Date__c )), 
     Image (&quot;resource/RFQ_Tracker_blue&quot;,&quot;&quot;,25,40),
IF (Days_Remaining_to_Send_Quote__c &gt;= 3, 
     Image (&quot;resource/RFQ_Tracker_green&quot;,&quot;&quot;,25,40),
IF ((Days_Remaining_to_Send_Quote__c &lt; 3) &amp;&amp; (Days_Remaining_to_Send_Quote__c &gt; 0), 
     Image (&quot;resource/RFQ_Tracker_yellow&quot;,&quot;&quot;,25,40), 
     Image (&quot;resource/RFQ_Tracker_red&quot;,&quot;&quot;,25,40)))))</formula>
