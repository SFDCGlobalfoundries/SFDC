    <webLinks>
        <fullName>Create_Export_Control_Form</fullName>
        <availability>online</availability>
        <description>This button is used for Internal Device and added in Internal Device page layout  only.</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Create Export Control Form</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/connection.js&quot;)}
var isDeviceComplete = {!Device__c.Device_Complete__c};
var accountSubType = &apos;{!Device__c.Account_Sub_Type__c}&apos;;
var objectPermission = true;

if(accountSubType == &apos;DE - IP&apos;){
	var describeResult = sforce.connection.describeSObject(&quot;Export_Control_Form__c&quot;);
	if(describeResult != null &amp;&amp; describeResult.updateable == &apos;false&apos;){
		objectPermission = false;
	}
	if(objectPermission){
		if(isDeviceComplete){
			window.top.location = &apos;/apex/ExportControlRedirect?deviceid={!Device__c.Id}&amp;accountid={!Device__c.AccountId__c}&apos;;
		}else{
			alert(&apos;Required fields are missing on Device. Please edit Device and enter required fields.&apos;);
		}
	}else{
		alert(&quot;Insuffiecient privilages. Please contact to Admin and assign Access to Internal Device Management permission set&quot;);
	}
}else{
	alert(&apos;Export Control is not required other than DE - IP account Internal Device&apos;);
}</url>
    </webLinks>
