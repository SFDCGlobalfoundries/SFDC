    <sharingModel>Private</sharingModel>
    <validationRules>
        <fullName>CR_Number_Required</fullName>
        <active>true</active>
        <errorConditionFormula>IF(((($Profile.Name != &apos;GF System Admin&apos;) &amp;&amp; 
($Profile.Name != &apos;System Administrator&apos;)) &amp;&amp; 
((Deployment_Status_txt__c = &apos;Deployed&apos;) || 
(Deployment_Status_txt__c = &apos;Failed&apos;) || 
(Deployment_Status_txt__c = &apos;Dev Team Verification&apos;) || 
(Deployment_Status_txt__c = &apos;Completed&apos;))), true, false)</errorConditionFormula>
        <errorDisplayField>CR_Number__c</errorDisplayField>
        <errorMessage>Please enter CR Number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CR_Number_Start_With_CH</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(BEGINS( CR_Number__c , &apos;CH&apos;))</errorConditionFormula>
        <errorDisplayField>CR_Number__c</errorDisplayField>
        <errorMessage>CR Number should start with CH</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Can_not_change_Deployment_Status</fullName>
        <active>true</active>
        <errorConditionFormula>IF(((($Profile.Name != &apos;GF System Admin&apos;) &amp;&amp; ($Profile.Name != &apos;System Administrator&apos;)) &amp;&amp; ((Deployment_Status_txt__c = &apos;Deployed&apos;) || (Deployment_Status_txt__c = &apos;Failed&apos;) || (Deployment_Status_txt__c = &apos;Dev Team Verification&apos;) || (Deployment_Status_txt__c = &apos;Completed&apos;))), true, false)</errorConditionFormula>
        <errorMessage>Can not change Deployment Status to Deployed/Failed/Dev Team Verification/Completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Can_not_change_Review_Status</fullName>
        <active>true</active>
        <errorConditionFormula>IF(((($Profile.Name != &apos;GF System Admin&apos;) &amp;&amp; ($Profile.Name != &apos;System Administrator&apos;)) &amp;&amp;  ((RM_Review_Status_txt__c = &apos;Approved&apos;) || (RM_Review_Status_txt__c = &apos;Rejected&apos;) || (RM_Review_Status_txt__c = &apos;Cancelled&apos;) || (RM_Review_Status_txt__c = &apos;RM Approval&apos;))), true, false)</errorConditionFormula>
        <errorMessage>Can not change the Review status to RM Approval/Approved/Rejected/Cancelled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Code_Reviewer_Required</fullName>
        <active>true</active>
        <errorConditionFormula>IF((Is_Code_Review_Completed_txt__c = &apos;Yes&apos;) &amp;&amp;   ISBLANK(Code_Review_User__c), true, false)</errorConditionFormula>
        <errorDisplayField>Code_Review_User__c</errorDisplayField>
        <errorMessage>Please select code reviewer name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Request_Date_greater_than_Today</fullName>
        <active>false</active>
        <errorConditionFormula>DATEVALUE(Deployment_Request_Date_Time__c) &lt; TODAY()</errorConditionFormula>
        <errorDisplayField>Deployment_Request_Date_Time__c</errorDisplayField>
        <errorMessage>Requested Deployment Date should be greater than today&apos;s date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Support_KT_Date_Required</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISBLANK(Support_KT_Date__c) &amp;&amp; 
((Support_KT_Status_txt__c = &apos;Planned&apos;) || (Support_KT_Status_txt__c = &apos;Completed&apos;)) , true, false)</errorConditionFormula>
        <errorDisplayField>Support_KT_Date__c</errorDisplayField>
        <errorMessage>Please enter Completed/Planned Support KT date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TA_Reviewer_Required</fullName>
        <active>true</active>
        <errorConditionFormula>IF((Is_TA_Review_Completed_txt__c = &apos;Yes&apos;) &amp;&amp; ISBLANK( TA_Review_User__c), true, false)</errorConditionFormula>
        <errorDisplayField>TA_Review_User__c</errorDisplayField>
        <errorMessage>Please select TA reviewer name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TFA_Reviewer_Required</fullName>
        <active>true</active>
        <errorConditionFormula>IF((Is_TFA_Review_Completed_txt__c = &apos;Yes&apos;) &amp;&amp; ISBLANK(  TFA_Review_User__c ), true, false)</errorConditionFormula>
        <errorDisplayField>TFA_Review_User__c</errorDisplayField>
        <errorMessage>Please select TFA reviewer name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UAT_DR_Required</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(ISBLANK( UAT_DR__c), (Target_Environment_txt__c = &apos;Production&apos;), NOT((Request_Type_txt__c = &apos;Data Migration Only&apos;))) , true, false)</errorConditionFormula>
        <errorDisplayField>UAT_DR__c</errorDisplayField>
