        <fullName>Test_Date_Time__c</fullName>
        <externalId>false</externalId>
        <formula>(Approval_Date__c - Submitted_Date__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Test Date Time</label>
