    <fields>
        <fullName>Time_Take_to_Deploy__c</fullName>
        <externalId>false</externalId>
        <formula>Deployment_End_Date_Time__c  -    (IF( ISBLANK( SDM_Approval_Date__c ) ,  RM_Approval_Date__c , SDM_Approval_Date__c))</formula>
        <label>Time Take to Deploy</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Time_Taken_for_Approval__c</fullName>
        <externalId>false</externalId>
        <formula>IF(AND(NOT(ISBLANK(Submitted_Date__c)),NOT(ISBLANK(Approval_Date__c))), 
	CASE( MOD( DATEVALUE(Submitted_Date__c) - DATE(1900, 1, 7), 7), 
		0, IF(Approval_Date__c &gt;= (Submitted_Date__c + 1),CEILING((Approval_Date__c - (Submitted_Date__c+1))*24*60),0),
		6, IF(Approval_Date__c &gt;= (Submitted_Date__c + 2),CEILING((Approval_Date__c - (Submitted_Date__c+2))*24*60),0),
		IF(AND(Submitted_Day__c = 2,(Approval_Date__c - Submitted_Date__c) &gt;= 5), CEILING((Approval_Date__c - (Submitted_Date__c+2))*24*60),
		IF(AND(Submitted_Day__c = 3,(Approval_Date__c - Submitted_Date__c) &gt;= 4), CEILING((Approval_Date__c - (Submitted_Date__c+2))*24*60),
		IF(AND(Submitted_Day__c = 4,(Approval_Date__c - Submitted_Date__c) &gt;= 3), CEILING((Approval_Date__c - (Submitted_Date__c+2))*24*60),
		IF(AND(Submitted_Day__c = 5,(Approval_Date__c - Submitted_Date__c) &gt;= 2), CEILING((Approval_Date__c - (Submitted_Date__c+2))*24*60),
		IF(AND((Approval_Date__c - Submitted_Date__c) &lt; 2, Submitted_Time__c &gt;= 6, Submitted_Time__c &lt;= 18),IF(AND((Approval_Date__c - Submitted_Date__c) &lt; 2, Approval_Time__c &gt;= 6, Approval_Time__c &lt;= 18),0,CEILING((Approval_Date__c - (Submitted_Date__c + ((18 - Submitted_Time__c)/24)))*24*60)),((Approval_Date__c - Submitted_Date__c)*24*60))))))),
	0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Time Taken for Approval (in MIN)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
