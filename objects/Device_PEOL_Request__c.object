        <fullName>Fill_CSRs_From_Account</fullName>
        <availability>online</availability>
        <description>Copies Primary CSR and Backup CSR from Customer&apos;s Account Team Proxy. If it does not find person with this role in the Customer Team, then alert user to add them to the team.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Get CSRs From Account</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}
try {
    var peolIdString = &apos;{!Device_PEOL_Request__c.Id}&apos;
    var result = sforce.apex.execute(&quot;DevicePEOLUtility&quot;, &quot;fillCSR&quot;, {
        peolId: peolIdString
    });
    location.reload();
} catch (err) {
    alert(err.faultstring);
    console.log(err);
}</url>
    </webLinks>
