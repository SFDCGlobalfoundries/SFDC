<apex:page controller="CSDetailsController" id="page" readOnly="true" showHeader="false" Sidebar="false" contentType="application/vnd.ms-excel#CSResult.xls" >
    <!-- Schema added to add autofilter for header row in Excel sheet -->
    <html xmlns:v="urn:schemas-microsoft-com:vml" xmlns:o="urn:schemas-microsoft-com:office:office" xmlns:x="urn:schemas-microsoft-com:office:excel" xmlns="http://www.w3.org/TR/REC-html40">
        <head></head> 
        <body > 
            <apex:pageBlock id="results">
                <!-- HTML Table used to display Custom settings data  in Excel Sheet with Grid Lines -->
                <table border="1" cellpadding="1" cellspacing="0" style="width:100%;border:1px solid #000000;font-familyans-serif;font-size:10px">
                    <tr style="font-family:verdana;color:Black;font-size:12px;height=18px;width:100%" >  
                        <!-- Autofilter added for header row in Excel sheet -->
                        <td x:autofilter='all' style="background-color: #DAFFFF" ><b> <apex:outputText value="Name"  /></b> </td>
                        <td x:autofilter='all' style="background-color: #DAFFFF" ><b> <apex:outputText value="Bug Admin"  /></b> </td>
                        <td x:autofilter='all' style="background-color: #DAFFFF" ><b> <apex:outputText value="Bug Admin Name"  /></b> </td>
                        <td x:autofilter='all' style="background-color: #DAFFFF" ><b> <apex:outputText value="Bug Owner"  /></b> </td>
                        <td x:autofilter='all' style="background-color: #DAFFFF" ><b> <apex:outputText value="Bug Owner Name"  /></b> </td>
                        <td x:autofilter='all' style="background-color: #DAFFFF" ><b> <apex:outputText value="Bug BugTeam"  /></b> </td>
                        <td x:autofilter='all' style="background-color: #DAFFFF" ><b> <apex:outputText value="BugTeam Members"  /></b> </td>
                        <td x:autofilter='all' style="background-color: #DAFFFF" ><b> <apex:outputText value="Bug Component"  /></b> </td>
                        <td x:autofilter='all' style="background-color: #DAFFFF" ><b> <apex:outputText value="Product Affected"  /></b> </td>
                        <td x:autofilter='all' style="background-color: #DAFFFF" ><b> <apex:outputText value="Additional BugTeam"  /></b> </td>
                        <td x:autofilter='all' style="background-color: #DAFFFF" ><b> <apex:outputText value="Additional BugTeam Members"  /></b> </td>
                    </tr> 
                    <apex:repeat value="{!csObjexportList}" var="cSet" id="theRepeat"> 
                        <tr border="1" style="font-family:verdana;color:Black;font-size:12px;width:100%" > 
                            <td ><apex:outputText value="{!cSet.Name}"/></td>
                            <td ><apex:outputText value="{!cSet.Bug_Admin__c}"/></td>
                            <td ><apex:outputText value="{!groupmap[cSet.Bug_Admin__c]}"/></td>
                            <td ><apex:outputText value="{!cSet.Bug_Owner__c}"/></td>
                            <td ><apex:outputText value="{!groupmap[cSet.Bug_Owner__c]}"/></td>
                            <td ><apex:outputText value="{!cSet.Bug_BugTeam__c}"/></td>
                            <td ><apex:outputText value="{!groupmap[cSet.Id]}"/></td>
                            <td ><apex:outputText value="{!cSet.Bug_Component__c}"/></td>
                            <td ><apex:outputText value="{!cSet.Product_affected__c}"/></td>
                             <td ><apex:outputText value="{!cSet.Additional_Bug_Team__c}"/></td>
                            <td ><apex:outputText value="{!addGroupmap[cSet.Id]}"/></td>
                        </tr> 
                    </apex:repeat> 
                </table>
                <br/> <br/> <br/> <br/> <br/>

                <apex:outputtext > Confidential Information - Do Not Distribute</apex:outputtext>
                <br/>
                <apex:outputtext >Generated By: {!$User.FirstName}  {!$User.LastName}  {!Now()} </apex:outputtext>
                <br/>
                <apex:outputtext value="GLOBALFOUNDRIES" />
            </apex:pageBlock>
        </body >
    </html>
</apex:page>