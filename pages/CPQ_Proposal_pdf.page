<!--
	Page API Name : CPQ_Proposal_pdf
-->
<apex:page showheader="false" sidebar="false" controller="CPQProposalPDFController" renderAs="pdf" action="{!savePDF}" standardStylesheets="false" applyBodyTag="false" > 
	<head>
		<style type="text/css" media="print">
			@page {
				@top-right{ 
					content: element(header);
				}
				@bottom-left {
					content: element(footer);
				}
			}
			div.header {
				padding: 10px;
				position: running(header);
			}
			div.footer {
				display: block;
				padding: 5px;
				position: running(footer);
			}
			.pagenumber:before {
				content: counter(page);
			}
			.pagecount:before {
				content: counter(pages);
			}
		</style>
	</head>
	
	<div class="header" style="color:#808080;text-align:right;">
		<div>
			<table align="right">
			<tr>
			  <td align="left">Configuration ID:</td>
			  <td>{!proposal.Name}</td>
			</tr>
			<tr>
			  <td align="left">Page:</td>
			  <td><span class="pagenumber"/> of <span class="pagecount"/></td>
			</tr>
			</table>
		</div>
	</div>

	<div class="footer" style="color:#808080;">
		<center>                 
			<div>GLOBALFOUNDRIES Confidential
				<br/>
				Any hard and soft copy out of the Document Management System deemed as uncontrolled.
			</div>
		</center>
	</div>

	<div class="content">
		<apex:image rendered="{!if(proposal.Purpose_of_the_Configuration__c != 'Final',true,false)}" 
					value="{!URLFOR($Resource.DraftWatermark)}" width="100%" height="100%"
					style="position: fixed;background-repeat:repeat;left: 0cm; top:0cm; z-index:-999; opacity=10;">
		</apex:image>
		<br/>

		<table width="100%" style="font-size:14px;" align="center">
			<tr>
				<td>
					<apex:image value="{!$Resource.GFLogo}" height="130px;"/>
					<p>
						<b>
							Contact your GLOBALFOUNDRIES Customer Engineering or Field Engineering representative for 
							question or comments about this Proposal.  
						</b>
					</p>
					<p>
						<b>  
							Copyright Â© 2012 by GLOBALFOUNDRIES 
						</b>
					</p>
					<p style="font-size:12px;">        
						The information disclosed herein is the property of GLOBALFOUNDRIES. GLOBALFOUNDRIES reserves all 
						proprietary, design, manufacturing, reproduction, use, and sales right thereto, and to any article or 
						process utilizing such information, except to the extent that rights are expressly granted to others.
					</p>
				</td>
			</tr>
		</table>
		
		<!--

						FIRST SECTION  -  CUSTOMER INFORMATION - to be Displayed always

		-->
		
		<div style="page-break-inside: avoid;">
			<table align="center" width="100%" cellspacing="0" cellpadding="4" border="0" style="font-size:18px;">
				<tr>
					<td colspan="2">
						<b>Customer Information</b>
					</td>
				</tr>
			</table>
		
			<table align="center" width="100%" style="margin-top:20px;margin-bottom:20px;" cellspacing="0" cellpadding="4" border="1">
				<tr>
					<td width="40%">Customer</td>
					<td>{!proposal.Apttus_Proposal__Account__r.name}</td>   
				</tr>
				<tr>
					<td>Configuration Name</td>
					<td>{!proposal.Apttus_Proposal__Proposal_Name__c}</td>   
				</tr>
				<tr>
					<td>Process Description</td>
					<td>{!productLine.Apttus_Proposal__Description__c}</td>   
				</tr>
				<tr>
					<td>Purpose of the Configuration</td>
					<td>{!proposal.Purpose_of_the_Configuration__c}</td>   
				</tr>
				<tr>
					<td>Configuration ID</td>
					<td>Product Config - {!proposal.Apttus_Proposal__Proposal_Name__c}</td>   
				</tr>
				<tr>
					<td>Process Technology Name</td>
					<td>{!proposal.Process_Technology_Name__c}</td>   
				</tr>
				<tr>
					<td>Author</td>
					<td>{!proposal.Owner.Name}</td>   
				</tr>
				<tr>
					<td>Contact</td>
					<td>{!proposal.Apttus_Proposal__Primary_Contact__r.Firstname & ' ' & proposal.Apttus_Proposal__Primary_Contact__r.Lastname}</td>   
				</tr>
				<tr>
					<td>Contact Email</td>
					<td>{!proposal.Apttus_Proposal__Primary_Contact__r.Email}</td>   
				</tr>
				<tr>
					<td>Device Name</td>
					<td>{!proposal.Device__r.Name}</td>   
				</tr>
				<tr>
					<td>DM Version in use</td>
					<td>{!proposal.PDK_Version__c}</td>   
				</tr>
				<tr>
					<td>Application</td>
					<td>{!proposal.Application__c}</td>   
				</tr>
				<tr>
					<td>Forecast Tape out Date</td>
					<td>
						<apex:outputText value="{0,date,MM/dd/yyyy}">
							<apex:param value="{!proposal.Forecast_Tape_out_Date__c}"/>
						</apex:outputText>
					</td>   
				</tr>
				<tr>
					<td>VV Large via Size Option</td>
					<td>{!proposal.VV_Large_Via_Size_option__c}</td>   
				</tr>
				<tr>
					<td>Fab Name</td>
					<td>{!proposal.Fab_Name__c}</td>   
				</tr>
				<tr>
					<td>Fab Location</td>
					<td>{!proposal.Fab_Location__c}</td>   
				</tr> 
			</table>
		</div>

		<!--

						
						SECTION(S)  - Product Attribute Groups 
		
		-->
		<div style="page-break-inside: avoid;">
			<!-- added by shiva start -->
			<apex:outputpanel layout="none">
				<apex:repeat value="{!objProposalAttributesController.ProdGrpSect}" var="grp">
					<apex:variable var="i" value="{!1}"/> 
					<apex:outputpanel layout="none">
						<div>
							<table align="center"  width="100%" style="margin-top:20px;margin-bottom:20px;" cellspacing="0" cellpadding="4" border="1">
								<tr>
									<td colspan="2"><b>{!grp}</b></td>
								</tr>
								<apex:repeat value="{!objProposalAttributesController.attrMap[grp]}" var="c">
									<apex:outputpanel layout="none">
										<tr>
											<td  width="40%">{!c.Attvalue}</td>
											<td>{!c.Attlabel}</td>
										</tr>
									</apex:outputpanel>
									<apex:variable var="i" value="{!i}"/>
								</apex:repeat>
								<br/>
							</table> 
						</div> 
					</apex:outputpanel>
				</apex:repeat>
			</apex:outputpanel>
			<!-- added by shiva end -->
		</div>
		
		<!--
						
						SECTION  -  METAL STACK & PACKAGING DETAILS
						
						Displays -  list of "Apttus_Proposal__Proposal_Line_Item__c" with Option_Product_Family_Static__c = 'Packaging' OR 'packaging' order by CategoryHierarchy.Left ASC
						
