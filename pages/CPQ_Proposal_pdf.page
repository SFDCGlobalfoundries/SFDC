						<td colspan='2' style="background-color:#f3f3ec"><b>Metal Stack &amp; Packaging Details</b> </td>
					</tr>
					<tr>
						<td>Metal Option</td>
						<td>{!proposal.APTPS_Metal_Options__c}</td>   
					</tr>
					
					<apex:repeat value="{!lstPacakgingLstLineItem}" var="objpackge">
						<tr>
							<td>{!objpackge.Option_Section_Name_Static__c}</td>
							<td>{!objpackge.Option_Name_Static__c}</td>
						</tr>
					</apex:repeat>
					
					<apex:repeat value="{!lstAdvancedPackagingResponses}" var="AdvancedPackagingResponse">
						<tr>
							<td>{!AdvancedPackagingResponse.Configurator_Plus_Question__r.Question_Name__c}</td>
							<td>{!AdvancedPackagingResponse.Response__c}</td>
						</tr>
