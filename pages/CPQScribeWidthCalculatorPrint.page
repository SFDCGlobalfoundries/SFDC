    <script>
    function calculateTotal(PreXID, PreYID, PostXID, PostYID, AreaPreID, AreaPostID ) {
        
        var PreX = document.getElementById(PreXID).value;
        var PreY = document.getElementById(PreYID).value;  
        if(PreX > 25.80 ){
            alert('Maximum die size pre shrink X (mm) = 25.80');
        }
        if(PreY > 30.83 ){
            alert('Maximum die size pre shrink Y (mm) = 30.83');
        }
        var AreaPre =  PreX * PreY  ; 
        var AreaPost = PreX * PreY * 0.9 * 0.9 ; 
        var PostX = PreX * 0.9 ; 
        var PostY = PreY * 0.9 ; 
        document.getElementById(AreaPreID).innerHTML = AreaPre.toFixed(2);
        document.getElementById(PostXID).innerHTML = PostX.toFixed(2);
        document.getElementById(PostYID).innerHTML = PostY.toFixed(2);
        document.getElementById(AreaPostID).innerHTML = AreaPost.toFixed(2);
    }
    
    function doScribeWidthCalculation() {
      $("#preXErrorIMG").hide();
      $("#preYErrorIMG").hide();
      if(parseFloat($.trim($('[id="pg:frm:pb:PreX"]').val())) > 25.80) {
        alert("Die size pre-shrink X cannot be more than 25.80");
        $("#preXErrorIMG").show();
        return;
      }
      if(parseFloat($.trim($('[id="pg:frm:pb:PreY"]').val())) > 30.83) {
        alert("Die size pre-shrink Y cannot be more than 30.83");
        $("#preYErrorIMG").show();
        return;
      }
      $("#postX").html((parseFloat($.trim($('#preX').html())) * 0.9).toFixed(2));
      $("#postY").html((parseFloat($.trim($('#preY').html())) * 0.9).toFixed(2));
      $("#areaPre").html((parseFloat($.trim($('#preX').html())) * parseFloat($.trim($('#preY').html()))).toFixed(2));
      $("#areaPost").html((parseFloat($.trim($("#postX").html())) * parseFloat($.trim($("#postY").html()))).toFixed(2));
      table1();
      
    }
    function table1() { 
       var minHorizontal123  = 0 ;
