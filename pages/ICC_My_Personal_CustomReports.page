<apex:page controller="ICC_My_Personal_CustomReports" sidebar="false" showHeader="false">
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockTable value="{!lstMyPersonalCustomReports}" var="rep">
                <apex:column headerValue="Report Name">                    
                    <a href="{!$Site.BaseUrl}/{!rep.Id}" target="_blank">{!rep.Name}</a><br/>
                </apex:column>
            </apex:pageBlockTable>
        </apex:pageBlock>
    </apex:form>
</apex:page>