<apex:page standardController="Work_Order__c" extensions="WorkOrderLotDetails" action="{!getLotDetails}">
    <apex:form style="width:100%">
    <apex:pageBlock >
        <!--<apex:pageBlockSection >-->
        <apex:pageBlockTable value="{!wolw}" var="rl" id="woltable" style="width:100%">
            
            <apex:column title="Rework Lot Numbers" headerValue="Rework Lot #" style="width:150px">
                <apex:outputText value="{!rl.reworkLot}"/>     
            </apex:column>
            <apex:column title="Original Lot Numbers" headerValue="Original Lot #" style="width:150px">
                <apex:outputText value="{!rl.OriginalLot}"/>     
            </apex:column>
            <apex:column title="Qual Code" headerValue="Qual Code" style="width:150px">
                <apex:outputText value="{!rl.qualCode}"/>     
            </apex:column>
            <apex:column title="Lot Qty" headerValue="Lot Qty" style="width:150px">
                <apex:outputText value="{!rl.lotQty}"/>     
            </apex:column>
        </apex:pageBlockTable>
        <!--<apex:commandButton value="Edit/AddNew" action="{!editOrAddNew}"/>-->
        <!--</apex:pageBlockSection>-->
    </apex:pageBlock> 
    
    </apex:form> 
</apex:page>