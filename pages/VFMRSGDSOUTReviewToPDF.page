<apex:page standardController="MRS__c" extensions="MRSCustomerGDSOUTReviewExtn"  showHeader="false" sideBar="false" standardStylesheets="false" 
contentType="application/pdf#MRSGDSOUTReview.pdf" renderAs="pdf" cache="true">
<!--<apex:page standardController="DRT__c" extensions="MRSCustomerGDSOUTReviewExtn"  showHeader="false" sideBar="false" standardStylesheets="false" 
cache="true">-->
    <apex:form >
        <table>
            <apex:outputLabel rendered="{!customerOrFoundry}">
                <tr align="center"><td colspan="5"><h2><u>GDSOUT Review - Customer</u></h2></td></tr>
            </apex:outputLabel>
            <apex:outputLabel rendered="{!!customerOrFoundry}">
                <tr align="center"><td colspan="5"><h2><u>GDSOUT Review - Foundry</u></h2></td></tr>
            </apex:outputLabel>
        </table>
        <br/>
        <br/>
        <table border="1" cellspacing="5">
            <apex:outputLabel rendered="{!isCustomerListActive}">
                <tr>
                    <th><b>Customer Name:</b></th>
                    <td >{!customerNameAfterSave}</td>
                </tr>
            </apex:outputLabel>
            <apex:outputLabel rendered="{!!isCustomerListActive}">
                <tr>
                    <th><b>Customer Name:</b></th>
                    <td>{!customerName}</td>
                </tr>
            </apex:outputLabel>
            <tr>
                <th><b>Mask Set Title:</b></th>
                <td >{!maskSetTitleMap[selectedMSTId]}</td>
            </tr>
            <apex:outputLabel rendered="{!IF(selectedLayerId != null,true,false)}">
                <tr>
                    <th><b>Mask Layer:</b></th>
                    <td >{!selectedLayerId}</td>
                </tr>
            </apex:outputLabel>
            
            <apex:outputLabel rendered="{!IF(selectedLayerStatus != null,true,false)}">
                <tr>
                    <th><b>Layer Status:</b></th>
                    <td >{!selectedLayerStatus}</td>
                </tr>
            </apex:outputLabel>
            
            <apex:outputLabel rendered="{!IF(selectedChipId != null,true,false)}">
                <tr>
                    <th><b>Chip Name:</b></th>
                    <td >{!selectedChipId}</td>
                </tr>
            </apex:outputLabel>
            
            <apex:outputLabel rendered="{!IF(selectedDeviceId != null,true,false)}">
                <tr>
                    <th><b>Device ID:</b></th>
                    <td >{!selectedDeviceId}</td>
                </tr>
            </apex:outputLabel>
            
            <apex:outputLabel rendered="{!IF(selectedPTRFId != null,true,false)}">
                <tr>
                    <th><b>PTRF:</b></th>
                    <td >{!selectedPTRFId}</td>
                </tr>
            </apex:outputLabel>
            
        </table>
        <br/>
        <br/>
        <table style="border:1px solid black;" cellspacing="0" cellpadding="6">
            <thead style="background:#e3e3e3;height:68px;">
                
                <tr>
                    <!--<th rowspan="3" align="left" style="border-bottom:1px solid #ccc;border-right:1px solid #ccc;">No.</th>
                    <th rowspan="3" align="center" style="border-bottom:1px solid #ccc;border-right:1px solid #ccc;">Mask Layer<br />(ROM Code)</th>
                    <th rowspan="3" align="center" style="border-bottom:1px solid #ccc;border-right:1px solid #ccc;">PTRF Number</th>
                      -->
                    <th><apex:outputLabel style="border-bottom:0px;border-right:0px solid #ccc;align:left;">No.</apex:outputLabel></th>
                    <th><apex:outputLabel style="border-bottom:0px;border-right:0px solid #ccc;align:left;">Mask Layer<br />(ROM Code)</apex:outputLabel></th>
                    <th><apex:outputLabel style="border-bottom:0px;border-right:0px solid #ccc;align:left;">PTRF Number</apex:outputLabel></th>
                    <th><apex:outputLabel rendered="{!isTechGeoActive}" style="border-bottom:0px;border-right:0px solid #ccc;align:left;">
                       Tech Geo 
                    </apex:outputLabel> 
                   </th>
                   <apex:repeat value="{!chipNameSet}" var="chip">
                        <th style="border-bottom:0px;border-right:0px solid #ccc;align:center">{!chip}</th>
                   </apex:repeat> 
                           
                </tr>
                      
            </thead>
            <tbody>
                <apex:repeat value="{!otherdisplayTableList}" var="display">
                
                          
                        <tr>
                        
                        <td align="left" style="border-right:0px solid #ccc;border-bottom:0px;">{!display.slNo}</td>
                        <td align="center" style="border-right:0px solid #ccc;border-bottom:0px;">{!display.maskLayerROMCode}</td> 
                        <td align="center" style="border-right:0px solid #ccc;border-bottom:0px;">{!display.ptrf}</td>
                        
                        <!--
                        <apex:outputLabel>
                        <td align="center" style="border-right:1px solid #ccc;border-bottom:1px solid #ccc;">{!display.TechGeo}</td>
                        </apex:outputLabel>
                        <apex:outputLabel>
                        <td align="center" style="border-right:1px solid #ccc;border-bottom:1px solid #ccc;">{!uniqueStatusMap[key].Customer_GDSOUT_Review__c}</td>
                        </apex:outputLabel>
                        <apex:outputLabel>
                        <td align="center" style="border-right:1px solid #ccc;border-bottom:1px solid #ccc;">{!uniqueStatusMap[key].Foundry_GDSOUT_Review__c}</td>
                        </apex:outputLabel>
                        -->
                        <td>
                        <apex:outputLabel style="border-right:0px;border-bottom:0px;align:left" rendered="{!isTechGeoActive}">
                        {!display.TechGeo} 
                        </apex:outputLabel>
                        </td>
              
                        <apex:repeat value="{!chipNameSet}" var="chip">
                  
                            <apex:variable var="key" value="{!display.maskLayerROMCode+':'}{!chip}{!':'+display.ptrf}"/>
                            <td>
                            <apex:outputLabel style="border-right:0px solid #ccc;border-bottom:0px;align:center" rendered="{!customerOrFoundry}">
                                {!uniqueStatusMapChanged[key].Customer_GDSOUT_Review__c}
                            </apex:outputLabel>
                            
                            
                            <apex:outputLabel style="border-right:0px solid #ccc;border-bottom:0px;align:center" rendered="{!!customerOrFoundry}">
                                {!uniqueStatusMapChanged[key].Foundry_GDSOUT_Review__c}
                            </apex:outputLabel>
                            </td>
                       </apex:repeat> 
                       
                    </tr>
                 
                 
                 
                  
                   
                 </apex:repeat>
            </tbody>
        </table>
    </apex:form>
</apex:page>