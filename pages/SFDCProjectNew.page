<apex:page standardController="SFDC_Project__c" showchat="false">
<script>
function showthis(obj){
    alert(obj);
    alert(obj.value);
}
</script>
  <apex:form >
    <apex:pageBlock title="SFDC Project">
      <apex:pageBlockButtons >
          <apex:commandButton value="Save" action="{!Save}"/>
          <apex:commandButton value="Cancel" action="{!Cancel}"/>
      </apex:pageBlockButtons>
      <apex:messages />
      <apex:pageBlockSection columns="1" >
        <apex:inputField value="{!SFDC_Project__c.Name}"/>
        <apex:inputField required="true" value="{!SFDC_Project__c.Project_Status_Indicator__c}"/>
        <apex:inputField required="true" value="{!SFDC_Project__c.Priority_Sequence__c}"/>
        <apex:inputField value="{!SFDC_Project__c.Description__c}"/>
        
      </apex:pageBlockSection>
      <apex:pageBlockSection columns="1">
         <apex:inputField value="{!SFDC_Project__c.Sponsoring_Organization__c}">
             <apex:actionSupport event="onchange" rerender="otherSponsorOrg"/>        
         </apex:inputField>
         <apex:outputPanel id="otherSponsorOrg">
             <apex:outputLabel style="font-weight;bold;" value="Other (Sponsoring Organization):" rendered="{!SFDC_Project__c.Sponsoring_Organization__c == 'Other'}" /> <apex:inputField rendered="{!SFDC_Project__c.Sponsoring_Organization__c == 'Other'}" value="{!SFDC_Project__c.Other_Sponsoring_Organization__c}"/>
         </apex:outputPanel>
         <apex:inputField value="{!SFDC_Project__c.Category__c}"/>
        <apex:inputField value="{!SFDC_Project__c.Business_Capability__c}" >
             <apex:actionSupport event="onchange" rerender="otherBizCap"/>        
        </apex:inputField>
        <apex:outputPanel id="otherBizCap">
         <apex:outputLabel style="font-weight;bold;" value="Other (Business Capability):" rendered="{!SFDC_Project__c.Business_Capability__c == 'Other'}" /> <apex:inputField rendered="{!SFDC_Project__c.Business_Capability__c == 'Other'}" value="{!SFDC_Project__c.Business_Capability_Other__c}"/>
        </apex:outputPanel>
        
      <apex:inputField label="Selected Vendor 1" value="{!SFDC_Project__c.Vendor__c}"/>
      <apex:pageBlockSectionItem ></apex:pageBlockSectionItem>
      <apex:inputField label="Selected Vendor 2" value="{!SFDC_Project__c.Vendor2__c}"/>
      <apex:pageBlockSectionItem ></apex:pageBlockSectionItem>
      <apex:inputField label="Selected Vendor 3" value="{!SFDC_Project__c.Vendor3__c}"/>
      <apex:pageBlockSectionItem ></apex:pageBlockSectionItem>
  
        
        <apex:inputField value="{!SFDC_Project__c.User_Group__c}" />
        <apex:inputField value="{!SFDC_Project__c.Other_User_Group__c}" />
        
       
        
      </apex:pageBlockSection>
      
      <apex:pageBlockSection collapsible="false" columns="1">
     <table class="detailList" cellspacing="0" cellpadding="0" border="0">
       <tbody>
        <tr></tr><tr></tr><tr></tr>
        <apex:inputField value="{!SFDC_Project__c.Urgency__c}"/>
        <tr> <td class="labelCol "></td>
           <td>
           <div style="border: 1px solid black;width:300px;">
           <div style="text-align:left;font-weight:bold;">Project Urgency Legend</div>
           <ul>
               <li>Immediate (3-6 Months)</li>
               <li>Mid-Term (6 -12 Months)</li>
               <li>Long Term (12 month or beyond)</li>
           </ul>
           </div>
        </td>
        <td class="labelCol empty"></td><td class="dataCol empty"></td>
        </tr>
        <apex:inputField value="{!SFDC_Project__c.Impact__c}"/>
        <tr> <td class="labelCol last "></td>
           <td>
           <div style="border: 1px solid black;width:300px;">
           <div style="text-align:left;font-weight:bold;">Project Impact on Revenue Legend</div>
           <ul>
               <li>High - million or more</li>
               <li>Medium - 250k or more</li>
               <li>Low - less than 250k</li>
           </ul>
           </div>
        </td>
        <td class="labelCol empty"></td><td class="dataCol empty"></td>
        </tr>
        </tbody>
     </table>
        
        
        
   </apex:pageBlockSection>
    </apex:pageBlock>
  </apex:form>
</apex:page>