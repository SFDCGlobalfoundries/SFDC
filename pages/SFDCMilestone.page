<apex:page standardController="SFDC_Project_Milestone__c" extensions="SFDCMilestoneExt" tabStyle="SFDC_Project__c" sidebar="false">
  <apex:form >
    <apex:pageBlock title="Milestone" >
       <apex:pageBlockButtons >
         <apex:CommandButton action="{!Edit}" value="Edit" />
         <apex:CommandButton action="{!Delete}" value="Delete" />
      </apex:pageBlockButtons>
    <apex:pageBlockSection collapsible="false" columns="2">
     <tr><td class="labelCol">Stage Name</td><td class="dataCol"><a href="/{!stage.Id}" >{!stage.Name__c}</a></td><td></td><td></td></tr>
      <apex:outputField value="{!SFDC_Project_Milestone__c.Name}"/>
      <apex:outputField value="{!SFDC_Project_Milestone__c.Completed__c}"/>
      <apex:outputLabel ></apex:outputLabel>
      <apex:outputField value="{!SFDC_Project_Milestone__c.Start_Date__c}"/>
      <apex:outputField value="{!SFDC_Project_Milestone__c.End_Date__c}"/>
      <apex:outputField value="{!SFDC_Project_Milestone__c.Predecessor_Milestone__c}"/>
      <apex:outputField value="{!SFDC_Project_Milestone__c.Description__c}"/>
      <apex:outputField value="{!SFDC_Project_Milestone__c.Chart_Color__c}"/>
      <apex:outputField label="Created By" value="{!SFDC_Project_Milestone__c.CreatedBy.Name}"/>
      <apex:outputField label="Last Modified By" value="{!SFDC_Project_Milestone__c.LastModifiedBy.Name}"/>
   </apex:pageBlockSection>

    
    
    </apex:pageBlock>
  </apex:form>
  <apex:relatedList list="Tasks__r" />
  
  <apex:relatedList subject="{!SFDC_Project_Milestone__c}" list="NotesAndAttachments" />
</apex:page>