<apex:page standardController="RFQ__c" tabStyle="IP_Request__c" extensions="GetIPRRecordType">
    <apex:includeScript value="{!URLFOR($Resource.jquery,'js/jquery-1.8.3.min.js')}" />
        <script>
            function cancel() {           
                window.location = "/a6s/o";       
            }
        </script>
        <apex:form >
            <apex:pageBlock title="New Request">
                <apex:pageBlockSection title="Select New Request Type and Record Type">
                    <apex:selectList value="{!selectedrequesttype}" multiselect="false" size="1" required="true" label="RequestType">
                        <apex:selectOptions value="{!RequestType}"/>
                    </apex:selectList>
                    <apex:selectList value="{!selectedrecordtype}" multiselect="false" size="1" required="true" label="RecordType">
                        <apex:selectOptions value="{!RecordType}"/>
                    </apex:selectList>
               </apex:pageBlockSection>
               <apex:pageBlockButtons location="bottom">
                   <apex:commandButton value="Proceed" onclick="check();"  action="{!CreateIPR}"/> 
                   <input type="button" value="Cancel" onclick="cancel();" class="btn" />
               </apex:pageBlockButtons>
            </apex:pageBlock>
         </apex:form> 
</apex:page>