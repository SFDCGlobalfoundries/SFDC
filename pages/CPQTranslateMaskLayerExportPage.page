<!--
    Author: Anirban Roy
    Description: This serves as the excel export page for translated/matched result for GDS and GR masks with CPL masks
                 in the Multi Source Tapeout.
    History: 
        ARoy    10152014    - page creation. 
-->
<apex:page controller="CPQTranslateMaskLayerController" contenttype="application/vnd.ms-excel#GDS_GR_Layer_Translation.xls">                              
    <apex:pageBlock title="GDS Layer : GDS Layer Translation">
           <table width="100%" id="headerTable" class="list">
               <thead class="rich-table-thead">
                   <tr class="headerRow">
                        <th class="headerRow" scope="col" colspan="1" style="border: thin solid black;">
                        </th>
                        <apex:outputPanel rendered="{!NOT(isExternalUser)}">
                            <th class="headerRow" scope="col" colspan="7" style="border: thin solid black;">
                                <div>GDS</div>
                            </th>
                        </apex:outputPanel>
                        <apex:outputPanel rendered="{!isExternalUser}">
                            <th class="headerRow" scope="col" colspan="3" style="border: thin solid black;">
                                <div>GDS</div>
                            </th>
                        </apex:outputPanel>
                        <th class="headerRow" scope="col" colspan="1" style="border: thin solid black;">
                            <div>Configuration</div>
                        </th>
                        <th class="headerRow" scope="col" colspan="1" style="border: thin solid black;">
                        </th>
                   </tr>
               </thead>
               <thead class="rich-table-thead">
                   <tr class="headerRow">
                        <th class="headerRow" scope="col" colspan="1" style="border: thin solid black;">
                        </th>  
                        <th class="headerRow" scope="col" colspan="3" style="border: thin solid black;">
                            <div>Customer Layers</div>
                        </th>
                        
                        <apex:outputPanel rendered="{!NOT(isExternalUser)}">
                            <th class="headerRow" scope="col" colspan="3" style="display:none;border: thin solid black;">
                                <div>GLOBALFOUNDRIES Design Layer</div>
                            </th>
                            <th class="headerRow" scope="col" colspan="1" style="border: thin solid black;">
                                <div>Translated Masks</div>
                            </th>
                        </apex:outputPanel>
                        
                        <th class="headerRow" scope="col" colspan="1" style="border: thin solid black;">
                            <div>Masks</div>
                        </th>
                        <th class="headerRow" scope="col" colspan="1" style="border: thin solid black;">
                        </th>                                    
                   </tr>
               </thead>
               <thead class="rich-table-thead">
                   <tr class="headerRow">
                        <th class="headerRow" scope="col" colspan="1" style="border: thin solid black;">
                            <div>Record Number</div>
                        </th>  
                        <th class="headerRow" scope="col" colspan="1" style="border: thin solid black;">
                            <div>MKT_NAME</div>
                        </th>
                        <th class="headerRow" scope="col" colspan="1" style="border: thin solid black;">
                            <div>MKT_LAYER</div>
                        </th>
                        <th class="headerRow" scope="col" colspan="1" style="border: thin solid black;">
                            <div>MKT_DT</div>
                        </th>                                    
                        
                        <apex:outputPanel rendered="{!NOT(isExternalUser)}">
                            <th class="headerRow" scope="col" colspan="1" style="border: thin solid black;">
                                <div>Names</div>
                            </th>
                            <th class="headerRow" scope="col" colspan="1" style="border: thin solid black;">
                                <div>Design Layer No</div>
                            </th>
                            <th class="headerRow" scope="col" colspan="1" style="border: thin solid black;">
                                <div>Data Type</div>
                            </th>                                                                        
                            <th class="headerRow" scope="col" colspan="1" style="border: thin solid black;">
                                <div>Mask</div>
                            </th>
                        </apex:outputPanel>
                        
                        <th class="headerRow" scope="col" colspan="1" style="border: thin solid black;">
                            <div>Mask</div>
                        </th>
                        <th class="headerRow" scope="col" colspan="1" style="border: thin solid black;">
                            <div>Status</div>
                        </th>
                   </tr>
               </thead>                               
               <apex:repeat value="{!fullGenericObjMap}" var="key">
                   <apex:repeat value="{!fullGenericObjMap[key]}" var="calcMaskLayer" id="calcMaskLayerList">      
                       <tr class="dataRow odd">
                           <td class="dataCell" colspan="1" style="border: thin solid black;">
                               <apex:outputText value="{!calcMaskLayer.rowCount}"/>
                           </td>                                                                      
                                                      
                           <td class="dataCell" colspan="1" style="border: thin solid black;">
                               <apex:outputText value="{!calcMaskLayer.gdsLayerName}"/>
                           </td>                                                                                                                                            
                       
                           <td class="dataCell" colspan="1" style="border: thin solid black;">
                               <apex:outputText value="{!calcMaskLayer.gdsLayerNumber}"/>
                           </td>
                           
                           <td class="dataCell" colspan="1" style="border: thin solid black;">
                               <apex:outputText value="{!calcMaskLayer.gdsLayerDT}"/>
                           </td>
                           
                           <apex:outputPanel rendered="{!NOT(isExternalUser)}">
                               <td class="dataCell" colspan="1" style="border: thin solid black;">
                                   <apex:outputText value="{!calcMaskLayer.grLayerName}"/>
                               </td>                                                                  
                           
                               <td class="dataCell" colspan="1" style="border: thin solid black;">
                                   <apex:outputText value="{!calcMaskLayer.grLayerNumber}"/>
                               </td>
                               
                               <td class="dataCell" colspan="1" style="border: thin solid black;">
                                   <apex:outputText value="{!calcMaskLayer.grLayerDT}"/>
                               </td>
                           
                               <td class="dataCell" colspan="1" style="border: thin solid black;">
                                   <apex:outputText value="{!calcMaskLayer.grMaskLayer}"/>
                               </td>
                           </apex:outputPanel>
                           
                           <td class="dataCell" colspan="1" style="border: thin solid black;">
                               <apex:outputText value="{!calcMaskLayer.cplMaskLayer}"/>
                           </td>
                           
                           <td class="dataCell" colspan="1" style="border: thin solid black;">
                               <apex:outputText value="{!calcMaskLayer.status}"/>
                           </td>
                       </tr>
                   </apex:repeat>
                </apex:repeat> 
            </table>                                                                                    
    </apex:pageBlock>                                  
</apex:page>