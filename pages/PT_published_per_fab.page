<apex:page controller="PTCatalogPublishController" >
 <head>
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script type="text/javascript">
      google.load("visualization", "1", {packages:["corechart"]});
      google.setOnLoadCallback(drawChart);
      function drawChart() {

        var data = google.visualization.arrayToDataTable([
          ['Catalog Publication','Count'], 
         
          /*
          
           ['Published',   60],
          ['Not Published', 40]
          
          */
          <apex:repeat value="{!lstPTPublishedPerFab}" var="st">
           ['{!st.status}',{!st.count}], 
          </apex:repeat>
        ]);

        var options = {
          title: "Configurator enabled Process technologies / fab (based on TO)",
          legend : {position: 'bottom', textStyle: {color: 'blue', fontSize: 10}},
          is3D : true
        };

        var chart = new google.visualization.PieChart(document.getElementById('piechart'));
        google.visualization.events.addListener(chart, 'select', function() {window.parent.location.href = '/{!$Label.Process_Technology_publish_report_id}?pv0={!MONTH(scribeWidthSetting.Actual_tapeout_date_for_dashboard__c)}/{!DAY(scribeWidthSetting.Actual_tapeout_date_for_dashboard__c)}/{!YEAR(scribeWidthSetting.Actual_tapeout_date_for_dashboard__c)}';});
        chart.draw(data, options);
      }
    </script>
  </head>
  <body>
    <div id="piechart" style="width: 280px; height: 300px;"></div>
  </body>
</apex:page>