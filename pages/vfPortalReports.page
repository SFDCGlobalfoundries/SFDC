<apex:page controller="clsPortalReports" >
<apex:form >
<html>
<head>
<style type="text/css" media="screen">
        body,
        html {
            width: 100%;
            height: 100%;
            overflow: hidden;
        }
         * {
            padding: 0;
            margin: 0;
        }
               </style>
              </head>
<body>
<apex:pageBlock tabStyle="Report" title="Analytical Reports">
   <apex:pageblocksection >
    <apex:selectRadio style="width: 80%" value="{!reportName}" onclick="radioButtonOneClick()">
            <apex:selectOption itemLabel="{!frequencyLoginName}" itemValue="{!frequencyLoginName}"/>
            <apex:selectOption itemLabel="Inactive Users" itemValue="{!InactiveUsersName}"/>
           <!-- <apex:selectOption itemLabel="{!TypeDocumentSubscribedName}" itemValue="{!TypeDocumentSubscribedName }"/> -->
           <!-- <apex:selectOption itemLabel="{!TopDownloadsName}" itemValue="{!TopDownloadsName}"/> -->        
           
           <!-- <apex:selectOption itemLabel="{!documentDownloadName}" itemValue="{!documentDownloadName}"/>  -->                 
    </apex:selectRadio>
    <apex:outputLink value="{!$Label.QlikViewURL}" title="{!$Label.QlikViewHelpText}" target="_blank">{!$Label.QlikViewLink}</apex:outputLink>
    </apex:pageblocksection>
</apex:pageBlock>  
<apex:actionFunction action="{!onclickRadioButton}" name="radioButtonOneClick" rerender="out"/>  

<apex:outputPanel id="out" rendered="true">
<apex:iframe src="{!url}" scrolling="true" height="400px"/>
</apex:outputPanel> 
          
</body>
</html>
</apex:form>
</apex:page>