<apex:page controller="PDK_OutlookController" showHeader="false" showChat="false">
    <apex:pageBlock mode="maindetail">
        <apex:pageBlockTable value="{!cdnMap}" var="cv">
            <apex:column headerValue="Title">
                <apex:outputLink target="_blank" value="/sfc/servlet.shepherd/version/download/{!cdnMap[cv].ContentVersion.id}?asPdf=false">
                {!cdnMap[cv].ContentVersion.Title}</apex:outputLink>
            </apex:column>
            <apex:column value="{!cdnMap[cv].ContentVersion.Description}" headerValue="Description" />
            <apex:column headerValue="Publication Date"  value="{!cntMap[cv].ContentModifiedDate}" />{!cntMap[cv].ID}
        </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>