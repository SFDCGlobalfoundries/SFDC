    <script>
        function checkforAll(documentId){
            if(document.getElementById(documentId)){
                var options = document.getElementById(documentId).options;
                if(options.length ==0 ){
                    var x = document.getElementById(documentId);
                    var option = document.createElement("option");
                    option.text = "All";
                    x.add(option);
                }
            }
        }
        
        window.onload = function(){
            checkforAllComplete();
        }
        
