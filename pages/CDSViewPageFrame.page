    //added for checking the idr date field and to launch the designexecution page -by Abhishek 
    function checkIDRDateValue(cdsId, idrdate, optionrecordid, cdsstatus){
        var IDRdatevar = idrdate;
        //alert('&&&&' + {!Custom_Design__c.CDS_Status__c});
        //alert('****' +cdsstatus);
        //var IDRdatevar = '{!Custom_Design__c.IDR_actual_date__c}';
        if(!IDRdatevar){
            alert('You must populate "CDS Complete (Ready for IDR)" Field' );
            return false;
        }
        else{
            if(!optionrecordid){
                alert('You must populate option control value' );
                return false;
            }
            else{
                if(cdsstatus == 'Ready for IDR' || cdsstatus == 'In DE' || cdsstatus  == 'Complete' ){
                    var DEurl = '/apex/DesignExecutionLaunch?cdsId='+cdsId;
                    window.open(DEurl,"DesignPhaseForm","menubar=0, toolbar=1, resizable=1, width=1200, height=750");
                    return false;
                }
                else {
                    alert('CDS Status should be "Ready for IDR" to launch Design Execution');
                    return false;                    
                }
            }
        }
    }
      
    
    //added for checking the idr date field and to launch the designexecution page -by Varsha 
    function checkIDRDateForTechReview(){
        var IDRdatevar = '{!customDesign.IDR_actual_date__c}';
        var cdsStatus = '{!customDesign.CDS_Status__c}';
        var optionrecordid = '{!customDesign.DIW_Option_Control_Number__c}';
        if(!optionrecordid){
            alert('You must populate option control value' );
            return false;
        }
        if(!IDRdatevar || cdsStatus == 'Draft'){
            alert('You must populate IDR Actual date Field before using this button and status of CDS should be Ready for IDR, In DE or Complete.' );
            return false;
        }
        else{  
            goToTechReview();
            return false;
        }
    }
