<apex:page id="roshelppage" standardController="ROS_Help__c" extensions="ROShelppagecontroller" sidebar="false" tabStyle="ROS_Help_Page__tab" title="GlobalFoundries - ROS Help Page">
    <script>
    </script>
    <center>
        <apex:form id="form1">
            <apex:pageBlock id="pb1" title="ROS Help" mode="edit">
                <apex:pageBlockButtons id="pbb1" location="top">
                    <apex:commandButton value="Edit" action="{!editHelp}" rendered="{!IF(AND(showEdit,recordId == null),true,false)}"/>
                    <apex:commandButton value="Save" action="{!saveHelp}" rendered="{!IF(recordId != null,true,false)}"/>
                </apex:pageBlockButtons>
                <apex:pageblockSection id="pbs1" columns="1">
                    <apex:inputField id="if1" value="{!rosHelp.Help_Content__c}" rendered="{!IF(recordId != null,true,false)}"/>
                    <apex:outputField id="of1" value="{!rosHelp.Help_Content__c}" rendered="{!If(rosHelp.Help_Content__c != '',true,false)}"/>
                </apex:pageblockSection>                
            </apex:pageBlock>
        </apex:form>
    </center>
