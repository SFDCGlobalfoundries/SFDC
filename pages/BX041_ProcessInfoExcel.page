<apex:page standardController="BX_041__c" extensions="BX041_processInfoPDF" sidebar="False" contentType="application/vnd.ms-excel#ProcessInformation.xls" showHeader="false" standardStylesheets="false">
    <apex:form >
        <apex:pageBlock title="New CDRS">                       
        <apex:pageBlockSection collapsible="true" title="Information" columns="1">
            <apex:outputField value="{!bx041.Device__c}"/>
            <apex:outputField value="{!bx041.BX_041_Status__c}"/>
            <apex:outputField value="{!bx041.CRMDID_Fromula__c}"/>
            <apex:outputField value="{!bx041.Opportunity_Program__c}"/>             
        </apex:pageBlockSection>   
        <apex:pageblockSection collapsible="true" title="Process Information" >
            <apex:dataTable value="{!ListWrapper}" var="wrap">
                <apex:column value="{!wrap.question}" headervalue="Questions"/>
                <apex:column value="{!wrap.value}" headerValue="Customer Request"/>
                <apex:column value="{!wrap.FEinfo}" headerValue="FE"/>
                <apex:column value="{!wrap.PiyeInfo}" headerValue="PIYE"/>
                <apex:column value="{!wrap.comments}" headervalue="Customer Comments"/>
            </apex:dataTable>    
        </apex:pageblockSection>
        <apex:pageblockSection collapsible="true" title="CDRS IP" >
            <apex:dataTable value="{!bx041IpList}" var="bx041Ip">
                <!--
                <apex:column headerValue="BX041 IP">
                    <apex:outputText value="{!bx041Ip.name}" />
                </apex:column>  -->
                <apex:column headerValue="IP Design Kit">
                    <apex:outputField value="{!bx041Ip.IP_Master__c}" />
                </apex:column>
                <apex:column headerValue="IP Type">
                    <apex:outputField value="{!bx041Ip.IP_Type__c}" />                        
                </apex:column>                        
                <apex:column headerValue="Latest Version">
                    <apex:outputField value="{!bx041Ip.Version__c}" /> 
                </apex:column>
                <apex:column headerValue="IP Category">
                    <apex:outputField value="{!bx041Ip.IP_Catg__c}" /> 
                </apex:column>
                <apex:column headerValue="Library Name">
                    <apex:outputField value="{!bx041Ip.Lib_Nm__c}" /> 
                </apex:column>
                <apex:column headerValue="IP Device List" >
                        <apex:outputField value="{!bx041Ip.IP_Device_List__c}"/> 
                    </apex:column>
                <apex:column headerValue="IP Vendor">
                    <apex:outputField value="{!bx041Ip.Vendor__c}" /> 
                </apex:column>
                <apex:column headerValue="Customer Requirements">
                    <apex:outputField value="{!bx041Ip.Cust_Rqmt__c}" /> 
                </apex:column>
                <!--  
                <apex:column headerValue="Additional requirements">
                    <apex:outputField value="{!bx041Ip.Addl_Rqmt__c}" /> 
                </apex:column>
                <apex:column headerValue="Remarks" >                                 
                     <apex:outputText value="{!bx041Ip.Remarks__c}" /> 
                </apex:column>     -->
            </apex:dataTable>    
        </apex:pageblockSection>        
        </apex:pageBlock>
    </apex:form>
</apex:page>