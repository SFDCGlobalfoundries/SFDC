<apex:page StandardController="RFQ__c" extensions="GetIPSizingOptionsRec">
    <apex:form >
        <apex:pageBlock >
           <!-- <apex:pageBlockTable value="{!ipsizingoptions}" var="rec"> -->
             <apex:pageBlockTable value="{!ipsizingoptions}" var="rec">
                 <apex:column >
                     <apex:outputLink value="/{!rec.Id}" target="blank">{!rec.Name}</apex:outputLink>
                 </apex:column>
                   <apex:column value="{!rec.Evaluator__c}"/>
                   <apex:column value="{!rec.Total_Development_Cost__c}"/>
                   <apex:column value="{!rec.Internal_Development_Cost__c}"/>
                   <apex:column value="{!rec.Internal_Support_Cost__c}"/>
                   <apex:column value="{!rec.Material__c}"/>
                   <apex:column value="{!rec.X3rd_Party_License_Porting_Support_Fees__c}"/>
                   <apex:column value="{!rec.X3rd_Party_Use_Fees__c}"/>
                   <apex:column value="{!rec.X3rd_Party_Royalty__c}"/>
                   <apex:column value="{!rec.Brand_Adjustment__c}"/>
                   <apex:column value="{!rec.Cost_Comments__c}"/>
            </apex:pageBlockTable>
        </apex:pageBlock>
    </apex:form> 
 </apex:page>