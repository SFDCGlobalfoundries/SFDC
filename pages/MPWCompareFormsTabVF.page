<apex:page id="MPWCompareFormsTabVF" standardcontroller="MPW_Reservation__c" extensions="MPWCompareFormsTabController" showHeader="true" sidebar="true" cache="false">
    
    <apex:sectionHeader title="Compare Forms" subtitle="Compare" help="https://help.salesforce.com/htviewhelpdoc?id=co_edit.htm&siteLang=en_US"/>
    <apex:pageMessages id="errorSection" /> 
    
    <apex:form id="form">
        <apex:pageBlock >
            <apex:pageBlockButtons location="bottom">
                 <apex:commandButton value="Compare" action="{!retrieveMPWFormData}"/>
            </apex:pageBlockButtons>
            
            <apex:pageBlockSection id="compare" columns="2" title="Compare Forms" collapsible="false">               
                <apex:pageBlockSectionItem >
                    <apex:outputLabel value="Form 1" />
                    <apex:inputText value="{!mpwForm1}"/>
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem >
                    <apex:outputLabel value="Form 2" />
                    <apex:inputText value="{!mpwForm2}"/>
                </apex:pageBlockSectionItem>
            </apex:pageBlockSection>
        </apex:pageBlock>
        
        <apex:pageBlock rendered="{!compareValue}" id="compareMPWForm">
             <apex:outputLabel value="Category - {!mpwForm1} : " style="font-size: 100%; font-weight: bold;"/>
             <apex:outputText value="{!mpwForm1Category}" />            
             <br />
             <apex:outputLabel value="Category - {!mpwForm2} : " style="font-size: 100%; font-weight: bold;"/>
             <apex:outputText value="{!mpwForm2Category}" style="{!IF(mpwForm1Category != mpwForm2Category, 'background-color: #00FFFF;', 'background-color: white;')}"/>
             <br />
             <br />
             <br />
             <apex:pageBlockSection id="processInfo" title="Process Information" collapsible="false">
                <apex:pageBlockTable id="mpwComForms2" value="{!MPWTrainMaintenanceDetailList}" rendered="{!IF(MPWTrainMaintenanceDetailList.size > 0, true, false)}" var="c">
                       <apex:column value="{!c.question}">
                            <apex:facet name="header">ModuleQuestion</apex:facet>
                        </apex:column> 
                        <apex:column value="{!c.answer1}">
                            <apex:facet name="header">{!mpwForm1}</apex:facet>
                        </apex:column>                       
                        <apex:column value="{!c.answer2}" style="{!IF(c.answer2 != c.answer1, 'background-color: #00FFFF;', 'background-color: white;')}">
                            <apex:facet name="header">{!mpwForm2}</apex:facet>
                        </apex:column>                    
                </apex:pageBlockTable>         
                <apex:outputText value="Nothing to compare." rendered="{!IF(OR(MPWTrainMaintenanceDetailList.size == null,MPWTrainMaintenanceDetailList.size == 0), true, false)}" />                 
            </apex:pageBlockSection>
             
             <br />
             <apex:pageBlockSection id="chipInfo" title="Chip Information" collapsible="false" columns="1">
                <apex:variable var="i" value="{!1}"/>
                    <apex:pageBlockTable value="{!MPWPrimeDieList}" var="p">
                        <apex:variable var="i" value="{!i+1}"/>
                        <apex:column headerValue="Form" rendered="{! IF((mod(i,2)) == 0, true, false)}" >
                            {!p.formName}
                        </apex:column>
                        <apex:column headerValue="Prime Die Name" rendered="{! IF((mod(i,2)) == 0, true, false)}" >
                            {!p.mpwPrimeDieTest.Prime_Die_Name__c}
                        </apex:column>
                        <apex:column headerValue="X (um)" rendered="{! IF((mod(i,2)) == 0, true, false)}" >
                            {!p.mpwPrimeDieTest.Drawn_Die_Size_X_um__c}
                        </apex:column>
                        <apex:column headerValue="Y (um)" rendered="{! IF((mod(i,2)) == 0, true, false)}" >
                            {!p.mpwPrimeDieTest.Drawn_Die_Size_Y_um__c}
                        </apex:column>  
                        <apex:column headerValue="Device Type" rendered="{! IF((mod(i,2)) == 0, true, false)}" >
                            {!p.mpwPrimeDieTest.Device_Type__c}
                        </apex:column>  
                        <apex:column headerValue="Quantity Die Form" rendered="{! IF((mod(i,2)) == 0, true, false)}" >
                            {!p.mpwPrimeDieTest.Delivery_Qty_Die_Form__c}
                        </apex:column>  
                        <apex:column headerValue="Quantity Wafer Form" rendered="{! IF((mod(i,2)) == 0, true, false)}" > 
                            {!p.mpwPrimeDieTest.Delivery_Qty_Wafer_Form__c}
                        </apex:column> 
                        <apex:column headerValue="Drawn Orientation" rendered="{! IF((mod(i,2)) == 0, true, false)}" >
                            {!p.mpwPrimeDieTest.Drawn_Orientation_Restricted_Structures__c}
                        </apex:column>  
                        <apex:column headerValue="Metal Stack" rendered="{! IF((mod(i,2)) == 0, true, false)}" > 
                            {!p.mpwPrimeDieTest.Metal_Stack__c}
                        </apex:column> 
                        <apex:column headerValue="Corner Split" rendered="{! IF((mod(i,2)) == 0, true, false)}" > 
                            {!p.mpwPrimeDieTest.Corner_Split__c}
                        </apex:column> 
                        <apex:column headerValue="POR" rendered="{! IF((mod(i,2)) == 0, true, false)}" >
                            {!p.mpwPrimeDieTest.POR__c}
                        </apex:column> 
                        <apex:column headerValue="FF" rendered="{! IF((mod(i,2)) == 0, true, false)}" > 
                            {!p.mpwPrimeDieTest.FF__c}
                        </apex:column>
                        <apex:column headerValue="SS" rendered="{! IF((mod(i,2)) == 0, true, false)}" >
                            {!p.mpwPrimeDieTest.SS__c}
                        </apex:column> 
                        <apex:column headerValue="FS" rendered="{! IF((mod(i,2)) == 0, true, false)}" >
                            {!p.mpwPrimeDieTest.FS__c}
                        </apex:column> 
                        <apex:column headerValue="SF" rendered="{! IF((mod(i,2)) == 0, true, false)}" >
                            {!p.mpwPrimeDieTest.SF__c}
                        </apex:column> 
                        <apex:column headerValue="GMPL Project" rendered="{! IF((mod(i,2)) == 0, true, false)}" >
                            {!p.mpwPrimeDieTest.GMPL_Project_Name__r.Name}
                        </apex:column>
                        <apex:column headerValue="GMPL Remarks" rendered="{! IF((mod(i,2)) == 0, true, false)}" >
                            {!p.mpwPrimeDieTest.GMPL_Purpose__c}
                        </apex:column>
                        <apex:column headerValue="Short Loop Wafer" rendered="{! IF((mod(i,2)) == 0, true, false)}" >
                            {!p.mpwPrimeDieTest.ShortLoop_Wafer__c}
                        </apex:column>
                        <apex:column headerValue="Backgrind" rendered="{! IF((mod(i,2)) == 0, true, false)}" >
                            {!p.mpwPrimeDieTest.Backgrind__c}
                        </apex:column>
                        <apex:column headerValue="Expedit Die Form" rendered="{! IF((mod(i,2)) == 0, true, false)}" >
                            {!p.mpwPrimeDieTest.Expedite_In_DIE_Form__c}
                        </apex:column>
                        <apex:column headerValue="Expedit Wafer Form" rendered="{! IF((mod(i,2)) == 0, true, false)}" >
                            {!p.mpwPrimeDieTest.Expedite_In_WAFER_Form__c}
                        </apex:column>
                        <apex:column headerValue="Turnkey Service" rendered="{! IF((mod(i,2)) == 0, true, false)}" >
                            {!p.mpwPrimeDieTest.Turnkey_Service__c}
                        </apex:column>
                        <apex:column headerValue="Form" rendered="{! IF((mod(i,2)) == 1, true, false)}" >
                            {!p.formName}
                        </apex:column>
                        <apex:column headerValue="Prime Die Name" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isPrimeDieName, 'background-color: #00FFFF;', 'background-color: white;')}" >
                            {!p.mpwPrimeDieTest.Prime_Die_Name__c}
                        </apex:column>                  
                        <apex:column headerValue="X (um)" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isDrawnX, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.Drawn_Die_Size_X_um__c}
                        </apex:column>
                        <apex:column headerValue="Y (um)" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isDrawnY, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.Drawn_Die_Size_Y_um__c}
                        </apex:column> 
                        <apex:column headerValue="Device Type" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isDeviceType, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.Device_Type__c}
                        </apex:column>
                        <apex:column headerValue="Quantity Die Form" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isQtyDie, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.Delivery_Qty_Die_Form__c}
                        </apex:column>  
                        <apex:column headerValue="Quantity Wafer Form" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isQtyWafer, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.Delivery_Qty_Wafer_Form__c}
                        </apex:column> 
                        <apex:column headerValue="Drawn Orientation" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isDrawnOrient, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.Drawn_Orientation_Restricted_Structures__c}
                        </apex:column>  
                        <apex:column headerValue="Metal Stack" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isMetalStack, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.Metal_Stack__c}
                        </apex:column> 
                        <apex:column headerValue="Corner Split" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isCornerSplit, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.Corner_Split__c}
                        </apex:column> 
                        <apex:column headerValue="POR" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isPOR, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.POR__c}
                        </apex:column> 
                        <apex:column headerValue="FF" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isFF, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.FF__c}
                        </apex:column>
                        <apex:column headerValue="SS" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isSS, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.SS__c}
                        </apex:column>
                        <apex:column headerValue="FS" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isFS, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.FS__c}
                        </apex:column>
                        <apex:column headerValue="SF" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isSF, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.SF__c}
                        </apex:column>
                        <apex:column headerValue="GMPL Project" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isGMPLProject, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.GMPL_Project_Name__r.Name}
                        </apex:column>
                        <apex:column headerValue="GMPL Remarks" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isGMPLRemarks, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.GMPL_Purpose__c} 
                        </apex:column>
                        <apex:column headerValue="Short Loop Wafer" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isShortLoop, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.ShortLoop_Wafer__c}
                        </apex:column>
                        <apex:column headerValue="Backgrind" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isBackgrind, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.Backgrind__c}
                        </apex:column>
                        <apex:column headerValue="Expedit Die Form" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isExDie, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.Expedite_In_DIE_Form__c}
                        </apex:column>
                        <apex:column headerValue="Expedit Wafer Form" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isExWafer, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.Expedite_In_WAFER_Form__c}
                        </apex:column>
                        <apex:column headerValue="Turnkey Service" rendered="{! IF((mod(i,2)) == 1, true, false)}" style="{!IF(p.isTurnkeySer, 'background-color: #00FFFF;', 'background-color: white;')}">
                            {!p.mpwPrimeDieTest.Turnkey_Service__c}
                        </apex:column>
                </apex:pageBlockTable>
                 <apex:outputText value="Nothing to compare." rendered="{!IF(OR(MPWPrimeDieList.size == null,MPWPrimeDieList.size == 0), true, false)}" />
             </apex:pageBlockSection>  
                        
        </apex:pageBlock>
    </apex:form>
    
</apex:page>