                         </div>
                         <p>&nbsp;</p>
                         <div class="getStartedLinksBlock">
                            <h3>Try one of these links to get started</h3>
                            <br /><br />
                             <apex:commandLink value="{!$Label.Configurator}" action="{!redirect}"  target="_blank" id="configLink" rendered="{!showConfigurator}"> 
                                <apex:param assignTo="{!Page}" value="configurator" name="configurator"/></apex:commandLink><br /><br />
                                <apex:commandLink value="{!$Label.Procedure}" onclick="OpenPage('procedure'); SetSeparatorHeight(); return false;" id="procedureLink" rendered="{!showProcedure}"/><br /><br />
                                <apex:commandLink value="{!$Label.IP}" action="{!redirect}"  target="_blank" id="ipLink" rendered="{!showIp}"><apex:param assignTo="{!Page}" value="ip" name="ip"/>
                                </apex:commandLink>  
                            <div id="contentPadding">
                      
                            </div>
