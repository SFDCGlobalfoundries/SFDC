                               </ul>
                            </li>
                        </ul>
                        <ul id="nav" class="leftnav">
                            <li><apex:commandLink value="Tools" onclick="OpenPage('tools');return false;" id="toolsLink" styleClass="Open" rendered="{!showTools}"/>
                               <ul class="level2">
                                  <li><apex:outputLink value="#" onclick="OpenPage('validator'); return false;" id="validator" rendered="{!showServices}">{!$Label.Validator}</apex:outputLink></li>
                                  <li><apex:outputLink value="#" onclick="OpenPage('FeatureDescriptionSearch'); return false;" id="FeatureDescriptionSearchPage" rendered="{!showFeatureDescSearch}">{!$Label.FeatureDescriptionSearch}</apex:outputLink></li>
