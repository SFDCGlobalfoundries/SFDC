                                 <apex:column headerValue="Sent Date" value="{!maillog.createdDate}"/>
                           </apex:pageBlockTable>
                           <apex:outputPanel id="maillogdisplaytxt">
                               <apex:outputText value="There are no mail logs to display" rendered="{!maillogList.size == 0}"/>
                           </apex:outputPanel>
                       </apex:pageBlockSection>
                       <apex:pageBlockSection title="RMA History" columns="1">
                           <c:RMA_HistoryComponent Object="{!RMA__c}" print="true" rendered="{!RMA__c.Id != NULL}"/>
                       </apex:pageBlockSection>
                    </apex:pageblock>
                     <apex:pageblock title="Vendor Code" id="vendor_code" rendered="{!RMA__c.Fab__c == 'FAB 7' && NOT(CONTAINS($CurrentPage.URL, 'globalfoundryview'))}" mode="maindetail">
