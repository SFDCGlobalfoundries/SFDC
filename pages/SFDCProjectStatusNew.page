<apex:page standardController="SFDC_Project_Status__c" tabStyle="SFDC_Project__c" extensions="SFDCProjectStatusNewExt" sidebar="false" showChat="false">
<apex:form >
  <apex:pageBlock title="New Project Status" mode="Edit">
    <apex:pageBlockButtons >
        <apex:commandButton action="{!Save}" value="Save"/>
        <apex:commandButton action="{!Cancel}" value="Cancel"/>
    </apex:pageBlockButtons>
    <apex:messages style="color:red;" />
    <apex:pageBlockSection columns="1">
      <apex:outputField value="{!SFDC_Project_Status__c.Project__c}"/>
      <apex:outputField value="{!SFDC_Project_Status__c.Week_Ending_Date__c}"/>
      <apex:inputField required="true" value="{!SFDC_Project_Status__c.Schedule__c}"/>
      <apex:inputField required="true" value="{!SFDC_Project_Status__c.Budget__c}"/>
      <apex:inputField required="true" value="{!SFDC_Project_Status__c.Resources__c}"/>
      <apex:inputField value="{!SFDC_Project_Status__c.Next_Stage__c}"/>
      <apex:inputField value="{!SFDC_Project_Status__c.Comments__c}"/>
    </apex:pageBlockSection>
   </apex:pageBlock>
</apex:form>
</apex:page>