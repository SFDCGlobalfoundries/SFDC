<apex:page standardController="ERP_Device__c" >
    
    <apex:pageBlock >
    <apex:pageBlockSection id="TheERPDevBlock" title="ERP Device details">
        <apex:pageBlockSectionItem >
           <apex:outputLabel value="Name" /> 
           <apex:outputText value="{!ERP_Device__c.Name}" /> 
        </apex:pageBlockSectionItem>
        <apex:pageBlockSectionItem >
           <apex:outputLabel value="Product Type" /> 
           <apex:outputText value="{!ERP_Device__c.Product_Type__c}" /> 
        </apex:pageBlockSectionItem>
        <apex:pageBlockSectionItem >
           <apex:outputLabel value="Fab" /> 
           <apex:outputText value="{!ERP_Device__c.Fab__c}" /> 
        </apex:pageBlockSectionItem>
        <apex:pageBlockSectionItem >
           <apex:outputLabel value="Technology" /> 
           <apex:outputText value="{!ERP_Device__c.Technology__c}" /> 
        </apex:pageBlockSectionItem>
        <apex:pageBlockSectionItem >
           <apex:outputLabel value="Wafer Diameter" /> 
           <apex:outputText value="{!ERP_Device__c.Wafer_Diameter__c}" /> 
        </apex:pageBlockSectionItem>
        <apex:pageBlockSectionItem >
           <apex:outputLabel value="Wafer Test Flag" /> 
           <apex:outputText value="{!ERP_Device__c.Test_wafer__c}" /> 
        </apex:pageBlockSectionItem>
    </apex:pageBlockSection>  
  </apex:pageBlock>  
    
   <flow:interview name="New_RTAT_Schedule" finishLocation="{!URLFOR( $Action.RTAT_Slot__c.Tab, $ObjectType.RTAT_Slot__c )}"/>  
       
</apex:page>