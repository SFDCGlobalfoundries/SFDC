<apex:page id="OpportunityHistory" standardController="Design_Win_Approval_History__c" extensions="OppHistory" tabStyle="Design_Win_Approval_History__c" sidebar="false">
<apex:form >
    <apex:outputPanel >
        <apex:pageblock >
        <apex:pageBlockButtons location="top">
            <apex:commandButton id="cmdBack" value="Back" action="{!Back}"/>
        </apex:pageBlockButtons>
            <apex:pageBlockSection columns="2" title="Opportunity Information" collapsible="true">
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="Account"/> 
                    <apex:outputField value="{!objOppHistory.Account__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="Market Segment"/> 
                    <apex:outputField value="{!objOppHistory.Market_Segment__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="Opportunity Name"/> 
                    <apex:outputField value="{!objOppHistory.Opportunity_Name__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="Opportunity Owner"/> 
                    <apex:outputField value="{!objOppHistory.Opportunity_Owner__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="Opportunity Sourcing Type"/> 
                    <apex:outputField value="{!objOppHistory.Opportunity_Sourcing_Type__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="Total Program Value"/> 
                    <apex:outputField value="{!objOppHistory.Total_Program_Value__c}"/>
                </apex:pageblockSectionItem>
                
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="GF Target Share %(DW)"/> 
                    <apex:outputField value="{!objOppHistory.GF_Target_Share_DW__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="GF Opportunity Size"/> 
                    <apex:outputField value="{!objOppHistory.GF_Opportunity_Size__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="Sum of Quarterly Revenue"/> 
                    <apex:outputField value="{!objOppHistory.Sum_of_Quarterly_Revenue__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="Total Lifetime Revenue"/> 
                    <apex:outputField value="{!objOppHistory.Total_Lifetime_Revenue__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="Years of Production (DW)"/> 
                    <apex:outputField value="{!objOppHistory.Years_of_Production_DW__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="Process Geometry"/> 
                    <apex:outputField value="{!objOppHistory.Process_Geometry__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="Process Platform Category (DW)"/> 
                    <apex:outputField value="{!objOppHistory.Process_Platform_Category_DW__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="Target Process Node"/> 
                    <apex:outputField value="{!objOppHistory.Target_Process_Node__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="Primary GF Fab"/> 
                    <apex:outputField value="{!objOppHistory.Primary_GF_Fab__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="Is GF Committed to Process (DW)?"/> 
                    <apex:outputField value="{!objOppHistory.Is_GF_Committed_to_Process_DW__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="Using GF PDK, DRC or SPICE Models? (DW)"/> 
                    <apex:outputField value="{!objOppHistory.Using_GF_PDK_DRC_or_SPICE_Models_DW__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="Date Design Win Approved"/> 
                    <apex:outputField value="{!objOppHistory.Date_Design_Win_Approved__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="DW Quarter Awarded"/> 
                    <apex:outputField value="{!objOppHistory.DW_Quarter_Awarded__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="Design Win Revenue"/> 
                    <apex:outputField value="{!objOppHistory.Design_Win_Revenue__c}"/>
                </apex:pageblockSectionItem>
                <apex:pageblockSectionItem >
                    <apex:outputLabel value="Awarded Amount"/> 
                    <apex:outputField value="{!objOppHistory.Awarded_Amount__c}"/>
                </apex:pageblockSectionItem>
                
                 <apex:pageblockSectionItem >
                    <apex:outputLabel value="1st Product TO Date (DW)"/> 
                    <apex:outputField value="{!objOppHistory.X1st_Product_TO_Date_DW__c}"/>
                </apex:pageblockSectionItem>
                 <apex:pageblockSectionItem >
                    <apex:outputLabel value="1st Product Production Date (DW)"/> 
                    <apex:outputField value="{!objOppHistory.X1st_Product_Production_Date_DW__c}"/>
                </apex:pageblockSectionItem>
                 <apex:pageblockSectionItem >
                    <apex:outputLabel value="Quote # (DW)"/> 
                    <apex:outputField value="{!objOppHistory.Quote_DW__c}"/>
                </apex:pageblockSectionItem>
                 <apex:pageblockSectionItem >
                    <apex:outputLabel value="Does entered ASP Align with Quote? (DW)"/> 
                    <apex:outputField value="{!objOppHistory.Does_entered_ASP_Align_with_Quote_DW__c}"/>
                </apex:pageblockSectionItem>
                
            </apex:pageBlockSection>
            </apex:pageblock>
    </apex:outputPanel>
    <apex:outputPanel >
        <apex:pageblock >
            <apex:pageBlockSection title="Quarterly Revenues" >
                <apex:pageBlockTable value="{!QuartRevRecords}" var="oQppt" styleClass="tableClass" cellpadding="4" cellspacing="4" rowClasses="odd,even">
                    <apex:column >
                            <apex:facet name="header"> QR No</apex:facet>      
                            <apex:outputField Value="{!oQppt.Name}"/>     
                    </apex:column>
                    <apex:column >
                            <apex:facet name="header"> ASP</apex:facet>      
                            <apex:outputField Value="{!oQppt.ASP__c}"/>     
                    </apex:column>
                    <apex:column >
                            <apex:facet name="header"> Fiscal Quarter</apex:facet>      
                            <apex:outputField Value="{!oQppt.Fiscal_Quarter__c}"/>     
                    </apex:column>
                    <apex:column >
                            <apex:facet name="header"> Revenue</apex:facet>      
                            <apex:outputField Value="{!oQppt.Revenue__c}"/>     
                    </apex:column>
                    <apex:column >
                            <apex:facet name="header"> Volume</apex:facet>      
                            <apex:outputField Value="{!oQppt.Volume__c}"/>     
                    </apex:column>
                </apex:pageBlockTable>
            </apex:pageBlockSection>
        </apex:pageblock>
    </apex:outputPanel>
</apex:form>
</apex:page>