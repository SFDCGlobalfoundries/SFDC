<apex:page controller="ROS_ControllerSearchReticleGFForm" action="{!loadreticleForDebitNote}">
<apex:sectionHeader title="Debit Note" subtitle="Keep Reticles"/>
<apex:form >
    <apex:pageBlock id="debitReticles">
    <apex:pageMessages />
        <apex:pageBlockButtons >
        <apex:commandButton value=" Save " action="{!saveDebitNote}" disabled="{!IsDebitNoteSaved}" reRender="debitReticles"/>
        
        <apex:commandbutton value=" Back To Search " action="{!CancelShippingAddress}"/>
        
        </apex:pageBlockButtons>
        <apex:pageBlockTable value="{!updateDebitReticle}" var="var">
            <apex:column value="{!var.Name}"/>
            <apex:column headerValue="Debit Note Reference Number">
                <apex:inputField value="{!var.Debit_Note_Number__c}"/>
            </apex:column>
        </apex:pageBlockTable>
    </apex:pageBlock>
   </apex:form> 
</apex:page>