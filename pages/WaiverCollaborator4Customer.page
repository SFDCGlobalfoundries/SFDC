<apex:page controller="WaiverCollaborator4CustomerCls" sidebar="false" showHeader="false">

	<apex:form >
	
		<apex:pageBlock >
		
			 <apex:pageBlockSection title="Design Waiver Reports" columns="1"> 
			 
                <apex:pageBlockTable value="{!WaiverList}" var="DFM">
                
                    <apex:column headerValue="Title">
                        <apex:outputLink value="/{!DFM.id}" target="_blank">{!DFM.Name}</apex:outputLink>
                    </apex:column> 
                     
                    <apex:column headerValue="PTSR Number">
                        <apex:outputText value="{!DFM.PTSR_Number__c}"/>
                    </apex:column>
                    
                    <!-- <apex:column headerValue="Workflow Status">
                        <apex:outputText value="{!DFM.Workflow_Status__c}"/>
                    </apex:column> -->
                     
                    <apex:column headerValue="PTSR Service Type">
                        <apex:outputText value="{!DFM.PTSR_Service_Type__c}"/>
                    </apex:column>
                    
                    <apex:column headerValue="DFM Service Option">
                        <apex:outputText value="{!DFM.DFMServiceOption__c}"/>
                    </apex:column>
                    
                    <apex:column headerValue="Aggregated Review Status">
                        <apex:outputText value="{!DFM.Waiver_Status__c}"/>
                    </apex:column>
                    <apex:column headerValue="Workflow Status">
                        <apex:outputText value="{!DFM.Progressbar_staus__c}"/>
                    </apex:column>
                    
                    <apex:column headerValue="Account Full Name">
                        <apex:outputText value="{!DFM.Account__r.Name}"/>
                    </apex:column>
                    
                </apex:pageBlockTable>
				
				<apex:panelGrid columns="4">
				    <apex:commandLink action="{!first}">First</apex:commandlink>
				    <apex:commandLink action="{!previous}" rendered="{!hasPrevious}">Previous</apex:commandlink>
				    <apex:commandLink action="{!next}" rendered="{!hasNext}">Next</apex:commandlink>
				    <apex:commandLink action="{!last}">Last</apex:commandlink>
			    </apex:panelGrid>
				                
            </apex:pageBlockSection>
            
        </apex:pageBlock>
        
	</apex:form>
	
</apex:page>