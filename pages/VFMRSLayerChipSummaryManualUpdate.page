    acfn_clearLayerChipData();   
}

function callLinkPage(strlink,obj,isFoundryView,customerid,custname,layerid,mst,layer,chipname,showVoid)
{
    var prefix="";
    var iconVal;
    if(isFoundryView){
        prefix='/GlobalfoundryView';
    }
    //alert(customerid);
    //alert(isVoid);
    if(showVoid==='true'){
        iconVal=[["Audit Trail",prefix+"/apex/VFMRSAuditTrail?id="+strlink+"&PageName=ManualUpdate&CustomerId="+customerid+"&customerName="+custname+"&LayerId="+layerid+"&MSTName="+mst+"&LayerNumber="+layer+"&ChipName="+chipname],["Void Chip",prefix+"/apex/VFMRSVoidChip?id="+strlink]];
        
    }else{
        iconVal=[["Audit Trail",prefix+"/apex/VFMRSAuditTrail?id="+strlink+"&PageName=ManualUpdate&CustomerId="+customerid+"&customerName="+custname+"&LayerId="+layerid+"&MSTName="+mst+"&LayerNumber="+layer+"&ChipName="+chipname]];    
        
    }
    iconMenu1(iconVal,obj);
    //alert(obj);
}
function callLinkPage2(strlink,obj,isFoundryView,customerid,customerName,layerid,MSTName,LayerNumber)
{
    var prefix="";
    if(isFoundryView){
        prefix='/GlobalfoundryView';
    }    
    var iconVal=[["Audit Trail",prefix+"/apex/VFMRSAuditTrail?PageName=ManualUpdate&id="+strlink+"&CustomerId="+customerid+"&customerName="+customerName+"&LayerId="+layerid+"&MSTName="+MSTName+"&LayerNumber="+LayerNumber]];
    iconMenu1(iconVal,obj);
}

function openExcelOrCSV(renderType){
            
            if(renderType == 'excel')
