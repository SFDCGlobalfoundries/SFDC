function resetFilter()
{
    showLoader('gf-bigcircle');
    jQuery('[id$=drpLayerStatus]').attr("value","");
    jQuery('[id$=drpDeviceId]').attr("value","");
    jQuery('[id$=drpPTRForDRT]').attr("value","");
