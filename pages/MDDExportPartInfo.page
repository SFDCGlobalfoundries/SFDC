<apex:page controller="MDD_Part_InfoController" readOnly="true" showHeader="false" Sidebar="false" cache="true" expires="1200" contenttype="application/vnd.ms-excel#{!prt.Part_Number__c}_material_declaration.xls" applyHtmlTag="true" standardStylesheets="false" >
<html xmlns:v="urn:schemas-microsoft-com:vml" xmlns:o="urn:schemas-microsoft-com:office:office" xmlns:x="urn:schemas-microsoft-com:office:excel" xmlns="http://www.w3.org/TR/REC-html40">
<style>
h2{
    border: 0px;
    font-size:14px;
}
table {
    border-collapse: collapse;
}

table, th, td {
    border: 1px solid black;
    text-align:center;
}
.pb{
    text-align:center; 
    vertical-align:middle;
}
</style>
<apex:form >
        <apex:pageBlock >
            <table>
            <tr><td colspan="8">
            GLOBALFOUNDRIES makes no promise, commitment, warranty, affirmation, description, or certification with respect to the information it provides in this declaration.  Rather, GLOBALFOUNDRIES provides 
            the information in this declaration on an "AS IS" basis, without any express or implied warranty of any kind. GLOBALFOUNDRIES may have provided a customer with an GLOBALFOUNDRIES RoHS Certificate for 
            the part covered by this declaration.  GLOBALFOUNDRIES sole and exclusive obligations, and a customers sole and exclusive remedies, with respect to the substance content of such part are 
            as expressly set forth in that document.</td>
            </tr>
            </table>
        </apex:pageBlock>
        <apex:pageBlock title="General Information" rendered="{!showPrt}" >
            <apex:pageBlockTable value="{!prt}" columns="8" var="p" style="Width=64%" border="1px"  >
                <apex:column style="Width=8%" styleClass="pb" >
                    <apex:facet name="header">
                        Part Number
                    </apex:facet>
                    <apex:outputField value="{!p.Part_Number__c}" styleClass="pb"  />
                </apex:column>
                <apex:column value="{!p.Part_Type__c} , {!p.Lead_Level__c}" style="Width=8%" styleClass="pb">
                    <apex:facet name="header">
                        Part type (Wafer or <br/>
                        module, and leaded, lead-<br/>
                        Free or lead-reduced)
                    </apex:facet>
                </apex:column>
                <apex:column value="{!p.Package_Type__c}" style="Width=8%;align=centre;" styleClass="pb">
                    <apex:facet name="header">
                        Part-family <br/>
                        package type
                    </apex:facet>
                </apex:column>
                <apex:column headerValue="Part-family" value="{!p.Part_Family__c}" style="Width=8%;text-align:centre" styleClass="pb"/>
                <apex:column headerValue="Maximum package size for part-family (mm x mm)" value="{!p.Package_x__c} X {!p.Package_y__c}" style="Width=8%" styleClass="pb">
                    <apex:facet name="header">
                        Maximum package size<br/>
                        for part-family (mm x mm)
                    </apex:facet>
                </apex:column>
                <apex:column headerValue="Maximum chip size for part-family (mm x mm)" value="{!p.Chip_size_x__c} X {!p.Chip_Size_y__c}" style="Width=8%" styleClass="pb">
                    <apex:facet name="header">
                        Maximum chip size <br/>
                        for part-family (mm x mm)
                    </apex:facet>
                </apex:column>
                <apex:column headerValue="Maximum weight for part-family (g)" value="{!p.Family_Weight__c}" style="Width=8%" styleClass="pb">
                    <apex:facet name="header">
                        Maximum weight for <br/>
                        part-family (g)
                    </apex:facet>
                </apex:column>
                <apex:column headerValue="Wafer size" value="{!p.Wafer_Size__c}" style="Width=8%" styleClass="pb"/>
            </apex:pageBlockTable>
        </apex:pageBlock>  
        <apex:pageBlock title="Exemptions" rendered="{!showExemp}">
            <apex:repeat value="{!Exemp}" var="e">
                <apex:outputField value="{!e.Name}" /><br></br>
            </apex:repeat>
        </apex:pageBlock>
        <apex:pageBlock title="Manufacturing Information" rendered="{!showPrt}">
            <apex:pageBlockTable value="{!prt}" columns="6" var="m" style="Width=66%" border="1px" >
                <apex:column value="{!m.Plating_grid_material__c}" style="Width=11%" styleClass="pb">
                    <apex:facet name="header">
                        Terminal plating / grid <br/>
                        array material
                    </apex:facet>
                </apex:column>
                <apex:column value="{!m.Base_Alloy__c}" style="Width=11%" styleClass="pb">
                    <apex:facet name="header">
                        Terminal <br/>
                        base alloy
                    </apex:facet>
                </apex:column>
                <apex:column value="{!m.MSL_Rating__c}" style="Width=11%" styleClass="pb">
                    <apex:facet name="header">
                        J-STD-020- <br/>
                        MSL rating
                    </apex:facet>
                </apex:column>
                <apex:column value="{!m.Peak_process_Body_Temp__c}" style="Width=11%" styleClass="pb">
                    <apex:facet name="header">
                        Peak process body <br/>
                        temperature
                    </apex:facet>
                </apex:column>
                <apex:column value="{!m.Max_Reflow_Time__c}" style="Width=11%" styleClass="pb">
                    <apex:facet name="header">
                        Maximum time at peak <br/>
                        temperature
                    </apex:facet>
                </apex:column>
                <apex:column value="{!m.Number_of_reflow_Cycles__c}" style="Width=11%" styleClass="pb">
                    <apex:facet name="header">
                        Number of reflow <br/>
                        cycles
                    </apex:facet>
                </apex:column>
            </apex:pageBlockTable>
        </apex:pageBlock>
        <apex:pageBlock title="Substance information">
            <table class="tbl">
                <tr class="tr">
                    <th class="th">Homogeneous material</th>
                    <th class="th">Substance type</th>
                    <th class="th">Substance group</th>
                    <th class="th">Substance name</th>
                    <th class="th">CAS number</th>
                    <th class="th">PPM of substance</th>
                    <th class="th">Percentage (by weight) of substance</th>
                    <th class="th">Substance weight (mg)</th>
                </tr>
            <apex:repeat value="{!wrap}" var="w">
                <apex:repeat value="{!w.homo}" var="h">
                    <tr class="tr">
                        <td class="td">{!h.homoInfo.Homogenous_material__c}</td>
                        <td class="td">{!h.homoInfo.Substance_type__c}</td>
                        <td class="td">{!h.homoInfo.Substance_group__c}</td>
                        <td class="td">{!h.homoInfo.Substance_name__c}</td>
                        <td class="td">{!h.homoInfo.CAS_Number__c}</td>
                        <td class="td"><apex:outputtext value="&nbsp;{!h.homoInfo.PPM_of_substance__c}&nbsp;" escape="false"/></td>
                        <td class="td"><apex:outputfield value="{!h.homoInfo.Concentration_Percent__c}"/></td>
                        <td class="td" style="float:right;"><apex:outputtext value="&nbsp;{!h.weight}&nbsp;" escape="false"/></td>
                    </tr>
                </apex:repeat>
                <tr>
                    <td colspan="7" class="td" align="right">
                        <b>Weight of {!w.homo[0].homoinfo.Homogenous_material__c}</b>
                    </td>
                    <td class="td">
                        <b><apex:outputtext value="&nbsp;{!w.totalWeight}&nbsp;" escape="false"/></b>
                    </td>
                </tr>
            </apex:repeat>
            </table>
        </apex:pageBlock>
    </apex:form>
</html>
</apex:page>