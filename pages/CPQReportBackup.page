<!--
    Author: Anirban Roy
    Description: This serves as the report link page for CPQ MLGPLUS Backup.
    History: 
        ARoy    04082015    - page creation. 
-->
<apex:page standardController="CPQ_MLGPLUS__c" extensions="CPQReportBackupController" >
    <apex:sectionHeader title="CPQ MLGPLUS" subtitle="Backup Report"/>
    <apex:pageBlock title="CPQ Report Links">
        <apex:pageBlockTable value="{!reportNameLinkMap}" var="rnl">
            <apex:column >
                <apex:facet name="header">Report Name</apex:facet>
                        {!rnl}
            </apex:column>
            <apex:column >
                <apex:facet name="header">Action</apex:facet>
                        <apex:outputLink value="{!reportNameLinkMap[rnl]}" target="_blank">Link to Report</apex:outputLink>                       
            </apex:column>
        </apex:pageBlockTable>
    </apex:pageBlock>       
</apex:page>