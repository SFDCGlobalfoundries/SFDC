<apex:page standardController="CMOS_BU_IP_Request__c" extensions="IPReqCopyController" sidebar="false" showHeader="false">
    <apex:form >
        <apex:pagemessages ></apex:pagemessages>
    	<apex:pageBlock title="Copy IP Request">
        	<apex:pageBlockSection title="Select IP Type & IP Sub Type">
                <apex:inputField value="{!CMOS_BU_IP_Request__c.IPType__c}"/>
                <apex:inputField value="{!CMOS_BU_IP_Request__c.IP_Subtype__c}"/>
            </apex:pageBlockSection>
            <apex:pageBlockButtons >
            	<apex:commandButton action="{!copy}" title="Copy" value="Copy"/>
            </apex:pageBlockButtons>
        </apex:pageBlock>
    </apex:form>
</apex:page>