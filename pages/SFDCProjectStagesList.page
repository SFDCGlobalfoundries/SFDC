<apex:page standardController="SFDC_Project__c" extensions="SFDCProjectStagesControllerExt"  showChat="false">



<apex:pageBlock >
       
       
       <apex:pageBlockSection columns="1" >
       <div style='text-align:center;padding-bottom:3px;'>
       <form action="/apex/SFDCProjectStages?id={!SFDC_Project__c.Id}" method="get" target="_parent">
          <input type="hidden" name="id" value="{!SFDC_Project__c.Id}"/>
          <input type="submit" value="Manage Stages"/>
       </form>
       </div>
       
        <apex:pageBlockTable value="{!stages}" var="s">
            <apex:column headerValue="Stage Name"><a href="/{!s.id}" target="_parent">{!s.Name__c}</a></apex:column>
            <apex:column value="{!s.Start_Date__c}"/>
            <apex:column value="{!s.End_Date__c}"/>
            <apex:column headerValue="Completed" value="{!s.Completed__c}"/>
            <apex:column headerValue="Skipped" value="{!s.Skip__c}"/>
        </apex:pageBlockTable> 
        </apex:pageBlockSection>
  </apex:pageBlock>
  
  
</apex:page>