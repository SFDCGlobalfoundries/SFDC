<apex:page standardController="Customer_Requirement__c" extensions="BnP_OptionDeliverableTypeCtrl" sidebar="false" >
    <c:BnP_OptionHeader RcId="{!Customer_Requirement__c.Id}"></c:BnP_OptionHeader>
    <apex:form id="theTabPanel">
        <html>
            <style type="text/css">
                td.activeTab 
                {
                    background-image:none; 
                    font-size:100%;
                    font-weight: bold;
                    border: 1px solid #003eff;
                    background: #007fff;
                    color: #ffffff;
                    padding-top: 5px;
                    padding-bottom: 5px;
                }
                td.inactiveTab 
                { 
                    background-color: #f6f6f6;
                    color:blue; 
                    background-image:none
                    background: #f6f6f6;
                    border: 1px solid #c5c5c5;
                    color: #454545;
                    padding-top: 4px;
                    padding-bottom: 5px;
                }
                th.labelCol
                {
                  font-size:100%;
                }
        </style>
        
        
            <apex:pageBlock title="Deliverable Type Inputs">
                <apex:pageMessages id="DTMessage" escape="false"></apex:pageMessages>                
                 <apex:pageBlockSection >
                    <apex:selectList id="TableSelect" value="{!selectDeliverabletype}" multiselect="false" size="1" title="Select Deliverable Type" disabled="{!DeliverableTypedisable}">
                        <b>Deliverable Type</b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; 
                        <apex:selectOptions value="{!DeliverableType}"/>
                        <apex:actionSupport event="onchange" rerender="Panel,DTMessage" action="{!DeliverableTypeMessage}"/>
                    </apex:selectList>
                     <apex:commandLink value="Export to Excel" action="{!doClick}" rendered="{!ShowExport}" style="Float:right;"/>
                </apex:pageBlockSection>
                 

                <apex:outputPanel id="Panel">
                    <apex:tabPanel value="{!Selectedcurenttab}" onclick="setSelectedTabOnController()" id="DeliverablePanel" tabClass="activeTab" inactiveTabClass="inactiveTab" contentStyle="font-size: 12px;" 
rendered="{!selectDeliverabletype == 'Service' || selectDeliverabletype == 'Wafer' ||  selectDeliverabletype == 'Chip' || selectDeliverabletype == 'Module' || selectDeliverabletype == 'Design'}">
                        <!-- This Tab For Service-->
                        <apex:tab label="Service"  switchType="client" name="name1"  id="tabOne1" rendered="{!selectDeliverabletype == 'Service' || selectDeliverabletype == 'Wafer' || selectDeliverabletype == 'Chip' || selectDeliverabletype == 'Module' || selectDeliverabletype == 'Design'}" >
                          
                            <apex:pageBlock >
                                <apex:pageBlockbuttons style="align:center;">
                                
                                <apex:actionStatus id="Loader" >
                                <apex:facet name="start" >
                                <img src="/img/loading.gif" title="Please Wait..." />                    
                                </apex:facet>
                                </apex:actionStatus>
                                
                                    <apex:commandButton action="{!SaveServicedata}" rendered="{!IsSaveVisible}" status="Loader" value="Save" reRender="theTabPanel"/>
                                    <apex:commandButton action="{!EdittheSection}" rendered="{!IsEditVisible}" status="Loader" value="Edit" reRender="theTabPanel" />
                                    <apex:commandButton action="{!canceldata}"  rendered="{!IsCancelVisible}"  value="Cancel" reRender="theTabPanel" />
                                </apex:pageBlockbuttons>
                                <apex:pageBlockSection title="Service Information"  columns="2">
                                    <apex:repeat value="{!serviceinfo}" var="f">
                                        <apex:inputField value="{!Service[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!ServiceInputFieldSet}"  />
                                        <apex:outputField value="{!Service[f.fieldPath]}" rendered="{!ServiceOutPutFieldSet}" />
                                    </apex:repeat>
                                    <apex:outputField value="{!Service.Technical_Requirement__c}" styleclass="genericstyle" style="width: 200px; height: 20px" />
                                </apex:pageBlockSection>
                            </apex:pageBlock>
                           
                        </apex:tab>
                        <!-- End-->
                        <!-- This Tab For Wafer-->
                        <apex:tab label="Wafer" switchType="client" name="name2" id="tabOne2" rendered="{!selectDeliverabletype == 'Wafer' || selectDeliverabletype == 'Chip' || selectDeliverabletype == 'Module'}">
                        
                            <apex:pageBlock >
                                <apex:pageBlockbuttons style="align:center;">
                                
                                <apex:actionStatus id="Loader" >
                                <apex:facet name="start" >
                                <img src="/img/loading.gif" />                    
                                </apex:facet>
                                </apex:actionStatus>
                                
                                    <apex:commandButton action="{!SaveWaferdata}" rendered="{!IsSaveWaferVisible}" status="Loader" value="Save" reRender="theTabPanel" />
                                    <apex:commandButton action="{!EditWaferSection}" rendered="{!IsEditWaferVisible}"  status="Loader" value="Edit" reRender="theTabPanel" />
                                    <apex:commandButton action="{!cancelWaferdata}"  rendered="{!IsCancelwaferVisible}"  value="Cancel" reRender="theTabPanel"/>
                                </apex:pageBlockbuttons>
                                <apex:pageBlockSection title="Information" columns="2">
                                    <apex:outputField value="{!wafer.Technical_Requirement__c}" style="width: 200px; height: 20px"/>
                                </apex:pageBlockSection>
                                <apex:pageBlockSection title="Additional active side wafer processing" columns="2">
                                    <apex:repeat value="{!Additionalactivesidewaferprocessing}" var="f">
                                        <apex:inputField value="{!wafer[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!WaferInput}" />
                                        <apex:outputField value="{!wafer[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!WaferOutPut}" />
                                    </apex:repeat>
                                </apex:pageBlockSection>
                                <apex:pageBlockSection title="Active Side Interconnect Details" columns="2">
                                    <apex:repeat value="{!ActiveSideInterconnectDetails}" var="f">
                                        <apex:inputField value="{!wafer[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!WaferInput}" required="{!OR(f.required, f.dbrequired)}"/>
                                        <apex:outputField value="{!wafer[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!WaferOutPut}"/>
                                    </apex:repeat>
                                </apex:pageBlockSection>
                                <apex:pageBlockSection title="Active Side C4 Details" columns="2">
                                    <apex:repeat value="{!ActiveSideC4Detailswf}" var="f">
                                        <apex:inputField value="{!wafer[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!WaferInput}"/>
                                        <apex:outputField value="{!wafer[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!WaferOutPut}"/>
                                    </apex:repeat>
                                </apex:pageBlockSection>
                                <apex:pageBlockSection title="Interconnect Details" columns="2">
                                    <apex:repeat value="{!InterconnectDetails}" var="f">
                                        <apex:inputField value="{!wafer[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!WaferInput}" />
                                        <apex:outputField value="{!wafer[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!WaferOutPut}" />
                                    </apex:repeat>
                                </apex:pageBlockSection>
                                <apex:pageBlockSection title="Additional back side wafer processing" columns="2">
                                    <apex:repeat value="{!Additionalbacksidewaferprocessing}" var="f">
                                        <apex:inputField value="{!wafer[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!WaferInput}" />
                                        <apex:outputField value="{!wafer[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!WaferOutPut}"/>
                                    </apex:repeat>
                                </apex:pageBlockSection>
                                <apex:pageBlockSection title="Back Side Interconnect Details" columns="2">
                                    <apex:repeat value="{!BackSideInterconnectDetails}" var="f">
                                        <apex:inputField value="{!wafer[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!WaferInput}"/>
                                        <apex:outputField value="{!wafer[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!WaferOutPut}"/>
                                    </apex:repeat>
                                </apex:pageBlockSection>
                                <apex:pageBlockSection title="Wafer Details" columns="2">
                                    <apex:repeat value="{!WaferDetails}" var="f">
                                        <apex:inputField value="{!wafer[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!WaferInput}"/>
                                        <apex:outputField value="{!wafer[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!WaferOutPut}"/>
                                    </apex:repeat>
                                </apex:pageBlockSection>
                                <apex:pageBlockSection title="Test Details" columns="2">
                                    <apex:repeat value="{!TestDetails}" var="f">
                                        <apex:inputField value="{!wafer[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!WaferInput}" />
                                        <apex:outputField value="{!wafer[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!WaferOutPut}" />
                                    </apex:repeat>
                                </apex:pageBlockSection>
                                <apex:pageBlockSection title="Additional wafer processing" columns="2">
                                    <apex:repeat value="{!Additionalwaferprocessing}" var="f">
                                        <apex:inputField value="{!wafer[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!WaferInput}"/>
                                        <apex:outputField value="{!wafer[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!WaferOutPut}"/>
                                    </apex:repeat>
                                </apex:pageBlockSection>
                            </apex:pageBlock>
                        </apex:tab>
                        <!-- End-->
                        <!-- This Tab For Chip-->
                        <apex:tab label="Chip" switchType="client" name="name3" id="tabOne3" rendered="{!selectDeliverabletype == 'Chip' || selectDeliverabletype == 'Module' || selectDeliverabletype == 'Wafer'}">
                            <apex:pageBlock >
                                <apex:pageBlockbuttons style="align:center;">
                                
                                <apex:actionStatus id="Loader" >
                                <apex:facet name="start" >
                                <img src="/img/loading.gif" />                    
                                </apex:facet>
                                </apex:actionStatus>
                                
                                    <apex:commandButton action="{!SaveChipdata}" rendered="{!IsSaveChipVisible}" status="Loader" value="Save" reRender="theTabPanel" />
                                    <apex:commandButton action="{!EditChipSection}" rendered="{!IsEditChipVisible}" status="Loader" value="Edit" reRender="theTabPanel" />
