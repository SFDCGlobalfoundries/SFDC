                        <!-- This Tab For Design-->
                        <apex:tab label="Design" switchType="client" name="name5" id="tabOne5" rendered="{!selectDeliverabletype == 'Design' || selectDeliverabletype == 'Service' || selectDeliverabletype == 'Wafer' || selectDeliverabletype == 'Chip' || selectDeliverabletype == 'Module'}">
                            <apex:pageBlock >
                                <apex:pageBlockbuttons style="align:center;" >
                                
                                <apex:actionStatus id="Loader" >
                                <apex:facet name="start" >
                                <img src="/img/loading.gif" />                    
                                </apex:facet>
                                </apex:actionStatus>
                                
                                    <apex:commandButton action="{!SaveDesigndata}" rendered="{!IsSaveDesignVisible}" status="Loader" value="Save" reRender="theTabPanel" />
                                    <apex:commandButton action="{!EditDesignSection}" rendered="{!IsEditDesignVisible}" status="Loader" value="Edit" reRender="theTabPanel" />
                                    <apex:commandButton action="{!CancelDesigndata}"  rendered="{!IsCancelDesignVisible}" value="Cancel" reRender="theTabPanel"/>
