                                        <apex:outputField value="{!Chp[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!ChipOutput}"/>
                                    </apex:repeat>
                                </apex:pageBlockSection>
                                <apex:pageBlockSection title="Parameters & Part Number - Chip Interconnect" columns="2">
                                    <apex:repeat value="{!ChipInterconnect}" var="f">
