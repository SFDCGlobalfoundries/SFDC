                                </apex:pageBlockSection>
                                <apex:pageBlockSection title="Parameters & Part Number - Identification" columns="2">
                                    <apex:repeat value="{!ParametersPartNumberIdentification}" var="f">
                                        <apex:inputField value="{!Chp[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!ChipInput}"/>
                                        <apex:outputField value="{!Chp[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!ChipOutput}" />
                                    </apex:repeat>
                                </apex:pageBlockSection>
                                <apex:pageBlockSection title="Parameters & Part Number - Physical Dimensions" columns="2">
                                    <apex:repeat value="{!PhysicalDimensions}" var="f">
