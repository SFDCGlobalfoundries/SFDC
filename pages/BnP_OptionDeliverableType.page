                                    <apex:outputField value="{!Modul.Technical_Requirement__c}" style="width: 200px; height: 20px"/>
                                    <apex:outputField value="{!Modul.Chip_1__c}" style="width: 200px; height: 20px"/>
                                </apex:pageBlockSection>
                                <apex:pageBlockSection title="MCM" columns="2" rendered="{!isMCM}">
                                    <apex:repeat value="{!MCM}" var="f">
                                        <apex:inputField value="{!Modul[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!ModuleInput}"/>
                                        <apex:outputField value="{!Modul[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!ModuleOutput}"/>
                                    </apex:repeat>
                                </apex:pageBlockSection>
                                <!--</apex:pageBlock>-->
                                <apex:pageBlockSection title="Module Assembly - Cap/Lid" columns="2">
                                    <apex:repeat value="{!CapLid}" var="f">
                                        <apex:inputField value="{!Modul[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!ModuleInput}"/>
                                        <apex:outputField value="{!Modul[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!ModuleOutput}"/>
