                                </apex:pageBlockSection>
                                <apex:pageBlockSection title="Parameters & Part Number - General" columns="2">
                                    <apex:repeat value="{!General}" var="f">
                                        <apex:inputField value="{!Chp[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!ChipInput}" />
                                        <apex:outputField value="{!Chp[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!ChipOutput}"/>
                                    </apex:repeat>
                                </apex:pageBlockSection>
                                <apex:pageBlockSection title="Parameters & Part Number - Documentation" columns="2">
                                    <apex:repeat value="{!Documentation}" var="f">
                                        <apex:inputField value="{!Chp[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!ChipInput}" />
                                        <apex:outputField value="{!Chp[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!ChipOutput}" />
                                    </apex:repeat>
                                </apex:pageBlockSection>
                                <apex:pageBlockSection title="2D/2.5D/3D" columns="2">
                                    <apex:repeat value="{!Chip2D}" var="f">
                                        <apex:inputField value="{!Chp[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!ChipInput}" />
                                        <apex:outputField value="{!Chp[f.fieldPath]}" style="width: 200px; height: 20px" rendered="{!ChipOutput}" />
                                    </apex:repeat>
                                </apex:pageBlockSection>
                            </apex:pageBlock>
                        </apex:tab>
                        <!-- End-->
                        <!-- This Tab For Module-->
                        <apex:tab label="Module" switchType="client" name="name4" id="tabOne4" rendered="{!selectDeliverabletype == 'Module'}">
                            <apex:pageBlock >
                                <apex:pageBlockbuttons style="align:center;" >
                                
                                 <apex:actionStatus id="Loader" >
                                <apex:facet name="start" >
                                <img src="/img/loading.gif" />                    
                                </apex:facet>
                                </apex:actionStatus>
                                
                                    <apex:commandButton action="{!SaveModuledata}" rendered="{!IsSaveModuleVisible}" status="Loader" value="Save" reRender="theTabPanel" />
