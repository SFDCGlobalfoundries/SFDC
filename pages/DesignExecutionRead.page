          function ritOffSetbtn() {
            $("#ritOffSetDiv").dialog({
                modal: true,
                title: 'Change Rit Off Set',
                zIndex: 10000,
                autoOpen: true,
                width: 620,
                height: 400,	
                resizable: true,
                buttons: [{
                    text: "Save",
                    click: function() {
                        var v1 = jQuery('input[id$=ritA7LPId]').val();
                         var v2 = jQuery('input[id$=ritB7LPId]').val();
                         var v3 = jQuery('input[id$=ritA14LPPId]').val();
                         var v4 = jQuery('input[id$=ritB14LPPId]').val();
                        
                        ritOffset(v1,v2,v3,v4);
                    }
                }, {
                    text: "Cancel",
                    click: function() {
                        $(this).dialog("close");
                    }
                }]
            });
              $(this).parent().css('position', 'fixed');
        }
      function close(){
       $("#ritOffSetDiv").dialog("close")
      }
    </script>  
