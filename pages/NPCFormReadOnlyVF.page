<apex:page id="NPCFormReadOnlyVF" standardController="New_Part_Creation_Form__c" extensions="NPCFormReadOnlyController" sidebar="true" showHeader="true" tabStyle="New_Part_Creation_Form__c" action="{!checkInstance}">
