    <style type="text/css">
        .showHide {
            display: none;
        }
        
        .mainTbl {
            width: 85%;
            border: none;
            border-collapse: collapse;
        }
        
        .mainTbl td {
            padding-top: 5px;
            padding-bottom: 5px;
        }
        
        .imgclass:hover {  
            background-image: url(/img/help/helpOrbs.gif);  
            background-repeat: no-repeat;  
            width: 16px;  
            height: 15px;     
            background-position: right;  
         
        }  
        
        .imgclass {  
            background-image: url(/img/help/helpOrbs.gif);  
            background-repeat: no-repeat;  
            width: 16px;  
            height: 15px;  
        }  
        
        .helpTextStyle {
            text-decoration: none;
            position: absolute;
            width: 15em;
            z-index: 12;
            background-color: #fefdb9;
            padding: 2px 5px;
            border: 1px solid orange;
            text-align: left;
            white-space: normal;
            font-weight: normal;
            color: #000;
        }
    </style>
    
    <script>
        function showHelpText(id) {
            document.getElementById(id).style.display = "block";
            document.getElementById(id).style.opacity = 0.99;
        }
        
        function hideHelpText(id) {
            document.getElementById(id).style.display = "none";
            document.getElementById(id).style.opacity = -0.2;
        }
        
        function submitRecord(id) {
            window.location.href = '/GlobalfoundryView/apex/NPCFormApproval?id=' + id;
        }
    </script>
    
    <script>
        $(document).ready(function(){
            if($("[id$='reasonForNewPartOutput']").text() == "Others") {
                $("#requireToCreateTheNewPartPanel").show();
                $("#newCustomerNamePanel").hide();
                $("#newCustomerNamePanel2").hide();
                $("#currentForecastTapeoutDatePanel").hide();
                $("#newFabPanel").hide();
            } else if($("[id$='reasonForNewPartOutput']").text() == "CustomerNameChange") {
                $("#requireToCreateTheNewPartPanel").hide();
                $("#newCustomerNamePanel").show();
                $("#newCustomerNamePanel2").show();
                $("#currentForecastTapeoutDatePanel").hide();
                $("#newFabPanel").hide();
            } else if($("[id$='reasonForNewPartOutput']").text() == "Fab to Fab Transfer") {
                $("#requireToCreateTheNewPartPanel").hide();
                $("#newCustomerNamePanel").hide();
                $("#newCustomerNamePanel2").hide();
                if ($("[id$='tapeoutOutput']").text() == 'Yes') {        
                    $("#currentForecastTapeoutDatePanel").show();  
                } else {
                    $("#currentForecastTapeoutDatePanel").hide();
                }
                $("#newFabPanel").show();
            } else if($("[id$='reasonForNewPartOutput']").text() == "Change in PID") {
                $("#requireToCreateTheNewPartPanel").hide();
                $("#newCustomerNamePanel").hide();
                $("#newCustomerNamePanel2").hide();
                if ($("[id$='tapeoutOutput']").text() == 'Yes') {        
                    $("#currentForecastTapeoutDatePanel").show();  
                } else {
                    $("#currentForecastTapeoutDatePanel").hide();
                }
                $("#newFabPanel").hide();
            } else if($("[id$='reasonForNewPartOutput']").text() == "Retrofit") {
                $("#requireToCreateTheNewPartPanel").hide();
                $("#newCustomerNamePanel").hide();
                $("#newCustomerNamePanel2").hide();
                if ($("[id$='tapeoutOutput']").text() == 'Yes') {        
                    $("#currentForecastTapeoutDatePanel").show();  
                } else {
                    $("#currentForecastTapeoutDatePanel").hide();
                }
                $("#newFabPanel").hide();
            } else if($("[id$='reasonForNewPartOutput']").text() == "Retrofit + Add ROM Code") {
                $("#requireToCreateTheNewPartPanel").hide();
                $("#newCustomerNamePanel").hide();
                $("#newCustomerNamePanel2").hide();
                if ($("[id$='tapeoutOutput']").text() == 'Yes') {        
                    $("#currentForecastTapeoutDatePanel").show();  
                } else {
                    $("#currentForecastTapeoutDatePanel").hide();
                }
                $("#newFabPanel").hide();
            } else if($("[id$='reasonForNewPartOutput']").text() == "StartingMaterialChange") {
                $("#requireToCreateTheNewPartPanel").hide();
                $("#newCustomerNamePanel").hide();
                $("#newCustomerNamePanel2").hide();
                if ($("[id$='tapeoutOutput']").text() == 'Yes') {        
                    $("#currentForecastTapeoutDatePanel").show();  
                } else {
                    $("#currentForecastTapeoutDatePanel").hide();
                }
                $("#newFabPanel").hide();
            } else if($("[id$='reasonForNewPartOutput']").text() == "BackgrindThicknessChange") {
                $("#requireToCreateTheNewPartPanel").hide();
                $("#newCustomerNamePanel").hide();
                $("#newCustomerNamePanel2").hide();
                if ($("[id$='tapeoutOutput']").text() == 'Yes') {        
                    $("#currentForecastTapeoutDatePanel").show();  
                } else {
                    $("#currentForecastTapeoutDatePanel").hide();
                }
                $("#newFabPanel").hide();
            } else if($("[id$='reasonForNewPartOutput']").text() == "ShortLoop Flow") {
                $("#requireToCreateTheNewPartPanel").hide();
                $("#newCustomerNamePanel").hide();
                $("#newCustomerNamePanel2").hide();
                if ($("[id$='tapeoutOutput']").text() == 'Yes') {        
                    $("#currentForecastTapeoutDatePanel").show();  
                } else {
                    $("#currentForecastTapeoutDatePanel").hide();
                }
                $("#newFabPanel").hide();
            } else {
                $("#requireToCreateTheNewPartPanel").hide();
                $("#newCustomerNamePanel").hide();
                $("#newCustomerNamePanel2").hide();
                $("#currentForecastTapeoutDatePanel").hide();
                $("#newFabPanel").hide();
            }
        });
    </script>
    
