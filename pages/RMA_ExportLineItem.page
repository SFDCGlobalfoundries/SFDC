<apex:page standardController="RMA__c" extensions="RMA_Edit_Extension" readOnly="true" tabstyle="RMA__c" showHeader="false"
    sidebar="false" cache="true" contentType="application/vnd.ms-excel#RMA_LineItem.xls">
        
        <apex:dataTable value="{!lstRMAInvoices}" var="li">
            <apex:column >
                <apex:facet name="header">Name</apex:facet>
                <apex:outputText value="{!li.Name}"/>
            </apex:column>
            <apex:column >
                <apex:facet name="header">RMA Name</apex:facet>
                <apex:outputText value="{!li.RMA__r.Name}"/>
            </apex:column>
            <apex:column >
                <apex:facet name="header">Invoice #</apex:facet>
                <apex:outputText value="{!li.Invoice_Number__c}"/>
            </apex:column>
            <apex:column >
                <apex:facet name="header">Lot #</apex:facet>
                <apex:outputText value="{!li.Lot_Number__c}"/>
            </apex:column>
            <apex:column >
                <apex:facet name="header">Wafer Id(CSR Submission)</apex:facet>
                <apex:outputText value="{!li.Wafer_Id_CSR_Submission__c}"/>
            </apex:column>
            <apex:column >
                <apex:facet name="header">Wafer Id(Fab Validated)</apex:facet>
                <apex:outputText value="{!li.Wafer_Id_Fab_Validated__c}"/>
            </apex:column>
            <apex:column >
                <apex:facet name="header">Wafer Id(Regional Personnel)</apex:facet>
                <apex:outputText value="{!li.Wafer_Id_Regional_Personnel_Recommended__c}"/>
            </apex:column>
            <apex:column >
                <apex:facet name="header">Total Price</apex:facet>
                <apex:outputText value="{!li.Total_Price__c}"/>
            </apex:column>
            <apex:column >
                <apex:facet name="header">Wafer Qty</apex:facet>
                <apex:outputText value="{!li.Wafer_Quantity__c}"/>
            </apex:column>
            <apex:column >
                <apex:facet name="header">Unit Selling Price</apex:facet>
                <apex:outputText value="{!li.Wafer_Die_Unit_Price__c}"/>
            </apex:column>
            <apex:column >
                <apex:facet name="header">Adjusted Unit Price</apex:facet>
                <apex:outputText value="{!li.Adjusted_Unit_Price__c}"/>
            </apex:column>
            <apex:column >
                <apex:facet name="header">Scrap Limit</apex:facet>
                <apex:outputText value="{!li.Scrap_Limit__c}"/>
            </apex:column>
            <apex:column >
                <apex:facet name="header">Wafer Yield</apex:facet>
                <apex:outputText value="{!li.Wafer_Yield__c}"/>
            </apex:column>
        </apex:dataTable>
</apex:page>