<apex:page standardController="DRT__c" extensions="DRTDetailExtn"  showHeader="false" sideBar="false" standardStylesheets="false" cache="true">
<apex:stylesheet value="{!URLFOR($Resource.Swift_DRT,'css/jquery.loader.min.css')}"/>
<apex:stylesheet value="{!URLFOR($Resource.Swift_DRT,'css/style.css')}"/>
<apex:stylesheet value="{!URLFOR($Resource.Swift_DRT,'css/jquery-ui.css')}"/>
<style>
    #frameLayerTable {
        font-size:12px!important;
    }
</style>
<!--[if IE]>
  <link rel="stylesheet" type="text/css" href="{!URLFOR($Resource.Swift_DRT, 'css/styleIE.css')}" />
<![endif]-->
<script type="text/javascript" src="{!URLFOR($Resource.Swift_DRT,'js/jquery.min.js')}"></script>
<script src="{!URLFOR($Resource.Swift_DRT,'js/jquery-ui.min.js')}"></script>
<script type="text/javascript" src="{!URLFOR($Resource.Swift_DRT,'js/common-script.js')}"></script>
<script src="{!URLFOR($Resource.Swift_DRT,'js/jquery.loader.min.js')}"></script>
<script> var swiftCommunityUrl = "{!$Label.SWIFT_Community_URL}"; showLoader('gf-bigcircle');</script>
<apex:form id="detailDRTformId">
<!--Wrapper Div Starts-->
<div id="wrapper">
            <!--Breadcrumb Starts-->            
            <apex:outputPanel rendered="{!isBreadcrumbActive && IF(fromPage!='Search', true, false)}">
                <div class="breadcrumb" id="breadcrumbDiv">
                    <label><a href="javascript:void(0);" onclick="openMyDevices();"><u>My Devices</u></a></label> &gt;
                    <label>Tapeout</label> &gt;
                    <label>DRT</label> &gt;
                    <label><apex:outputLink value="{!$Page.VFDRTDetail}?id={!drtId}"><u>{!drtName}</u></apex:outputLink></label>
                </div>
            </apex:outputPanel>
            <apex:outputPanel rendered="{!isBreadcrumbActive && IF(fromPage=='Search', true, false)}">
                <div class="breadcrumb" id="breadcrumbDiv">
                    <label><apex:outputLink value="{!$Label.SWIFT_Community_URL+'MyDevices'}"><u>My Devices</u></apex:outputLink></label> &gt;
                    <label>Tapeout</label> &gt;
                    <label>DRT</label> &gt;
                    <label><apex:outputLink value="{!$Page.VFDRTDetail}?id={!drtId}"><u>{!drtName}</u></apex:outputLink></label>
                </div>
            </apex:outputPanel>
            <apex:outputPanel rendered="{!NOT(isBreadcrumbActive) && IF(fromPage=='Search', true, false)}">
                <div class="breadcrumb" id="breadcrumbDivInternal">
                    <label>DRT</label> &gt;
                    <label><apex:outputLink value="{!$Page.VFDRTDetail}?id={!drtId}"><u>{!drtName}</u></apex:outputLink></label>
                </div>
            </apex:outputPanel>  
            <apex:outputPanel rendered="{!NOT(isBreadcrumbActive) && IF(fromPage!='Search', true, false)}">
                <div class="breadcrumb" id="breadcrumbDivInternal">
                    <label>DRT</label> &gt;
                    <label><apex:outputLink value="{!$Page.VFDRTDetail}?id={!drtId}"><u>{!drtName}</u></apex:outputLink></label>
                </div>
            </apex:outputPanel>           
            <!--Breadcrumb Ends-->
            
            <div id="drtDetailsActive" class="right-panel-section">
                <!-- Start DRT Right side Header-->                
                <h3>{!drtName}</h3>                
                <input type="hidden" id="hiddenDrtName" value="{!drtName}"/>                
                <!-- End Right side Header-->                 
                
                <div class="quicklinks">
                    <apex:variable var="excelURL" value="{!currentUrl+'VFDRTDetailToExcel?id='+drtId}"/>
                    <apex:variable var="pdfURL" value="{!currentUrl+'VFDRTDetailToExcelOrPDF?id='+drtId}"/>
                    
                    <a href="{!excelURL}" target="_blank"><img src="{!URLFOR($Resource.Swift_DRT,'images/excel-icon.png')}" width="23" height="23" alt="Export to Excel" title="Export to Excel" /></a>
                    <a href="{!pdfURL}" target="_blank"><img src="{!URLFOR($Resource.Swift_DRT,'images/print-icon.png')}" width="23" height="23" alt="Print" title="Print" /></a> 
                    <a href="#"><img src="{!URLFOR($Resource.Swift_DRT,'images/help-icon.png')}" width="23" height="23" alt="Help" title="Help" /></a>
                </div>
                
                
                <!-- --------------- First Row Starts --------------- -->
                <div id="firstRowContentDiv" class="right-section-row right-section-row-borderbottom">
                    <div class="details-button-style"> 
                        <apex:outputPanel rendered="{!isCopyActive}">
                            <input type="button" value="Copy" id="drtCopyButton"/>
                        </apex:outputPanel>                 
                        <apex:outputPanel rendered="{!isReviseActive}">
                            <input type="button" value="Revise" id="ReviseButton" onclick="redirectRevise();"/>
                        </apex:outputPanel>    
                        <apex:outputPanel rendered="{!isUpdateActive}">
                            <input type="button" value="Update" id="UpdateButton"  onclick="redirectUpdate();"/>
                        </apex:outputPanel>
                        <apex:outputPanel rendered="{!isCancelActive}">              
                            <input type="button" value="Cancel" id="drtCancelButton"/>
                        </apex:outputPanel>
                        <apex:outputPanel rendered="{!isDeleteActive}">
                            <input type="button" value="Delete" id="drtDeleteButton"/>
                        </apex:outputPanel>
                    </div>
                    <apex:outputPanel id="errorPanel">
                            <apex:repeat var="errorMsg" value="{!lstDRTErrorMessage}" rendered="{!lstDRTErrorMessage.size>0}">
                                <div class="messages error">{!errorMsg}</div>
                                <div class="clear"/>
                            </apex:repeat>
                    </apex:outputPanel>
                    <div class="clear"/>
                </div>
                <!-- --------------- First Row Ends --------------- -->
                    
                <!-- --------------- Second Row Starts --------------- -->
               
            <div id="secondRowContentDiv" class="right-section-row">    
            <apex:pageMessages id="pageMessageId"/>            
            <!------ Details tab to be included---->
            <div class="details-tab-content" style="height:auto;">
                <apex:tabPanel tabClass="activeTab" inactiveTabClass="inactiveTab" switchType="client" immediate="true" headerClass="headerPanel">
                    <apex:tab id="detailsTab" label="Details" name="details" styleClass="detailsContent">                        
                        <div class="detailsWrapper">                        
                        <div id="firstRowContentDiv" class="right-section-row right-section-row-borderbottom">
                            <table id="detailFirstRowTbl"> 
                            <tr>
                                <td>                    
                                    <dl><dt class="bold-font fontSize-12 min-width-inputLabel">Customer Name:</dt><dd class="pading-left">{!savedCustomerName}</dd></dl>
                                </td>
                                <td>
                                    <dl><dt class="dt-style-margin bold-font fontSize-12">Status:</dt><dd class="pading-left">{!savedStatus}</dd></dl>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <dl><dt class="bold-font fontSize-12 min-width-inputLabel">Mask Set Title:</dt><dd class="pading-left">{!savedMaskSetTitle}</dd></dl>
                                </td>
                                <td>
                                    <dl><dt class="dt-style-margin bold-font fontSize-12">DRT Type:</dt><dd class="pading-left">{!savedDRTType}</dd></dl>
                                </td>
                            </tr>
                            <apex:outputPanel rendered="{!isTechGeoNull}">
                            <tr>
                                <td>
                                   <dl><dt class="bold-font fontSize-12 min-width-inputLabel">Tech Geo:</dt><dd class="pading-left"> {!savedTechGeo}</dd></dl>
                                </td>
                                <td>
                                      <dl><dt class="bold-font fontSize-12"></dt><dd class="pading-left"> &nbsp;</dd></dl>
                                </td>
                            </tr>
                           </apex:outputPanel>   
                            </table>                    
                        <div class="clear"></div>
                    </div>                    
                    <div class="right-section-row no-padding-bottom">
                        <dl id="secondcreatedrt"></dl>
                        <dl><dt class="no-margin-top bold-font fontSize-12">Description:</dt><dd class="pading-left">{!description}</dd></dl> 
                    </div>                        
                 <!--Start of Tables-->
                    <div class="right-section-row">                    
                        <div class="advance-datagrid" id="primeDRTWrapper">                        
                        <apex:outputPanel id="primeDRTOutputId"> 
                          <apex:outputpanel id="primeDRTId" rendered="{!drtPrimeType}">
