                            <div class="mask-scrooler" id="scroller" style="height:299px;">
                                <div class="dummy-cont"></div>
                            </div>
                            <div class="main-tbl-wrapper border-bottom">
                                <div class="tabl-header-wrapper">
                                <table class="gen-table main-Tbl header-tbl">
                                     <tbody>
                                     <tr>
                                         <td class="tcell-width-20 no-border-left">No.</td>
                                         <td class="tcell-width-83">Mask Set Title</td>
                                         <td class="tcell-width-141">Prime Die/<br/>Scribeline Drop-in</td>
                                     </tr>
                                     </tbody>
                                 </table> 
                                </div>
                                <div class="clear"></div>                                
                                <div class="tbl-content-wrapper min-height300">
                                    <table  id="main-tbl-content"  class="gen-table main-Tbl border-bottom" >
                                         <tbody>
                                             <apex:repeat value="{!chipWrapperList}" var="chip" >               
                                                 <tr class="tcell-height-30 {!chip.colorClass}">
                                                     <td class="tcell-width-20 text-align-centre no-border-left">{!chip.counter}</td>
                                                     <td class="tcell-width-83">{!chip.mstName}</td>
                                                     <td class="tcell-width-141">{!chip.chipName} </td>
                                                 </tr>
                                             </apex:repeat>
                                         </tbody> 
                                    </table>
                                </div>                                
                                <div class="clear"></div>
                                <div class="spacer"></div>
                            </div>
                            <div class="mask-layer-tbl-wrapper" style="width:0px;display:hidden;">                                
                                <div class="mask-tbl-hdr-cont">
                              <apex:repeat value="{!maskLayerHeaderWrapperList}" var="m"> 
                                <div class="mask-tbl-header-wrapper">
                                    <table class="gen-table mask-tbl header-tbl table-height-74 IE-table-height-72">
                                        <tbody>
                                            <tr>
                                                <td colspan="2" class="tcell-width-150">
                                                    <span class="table-lable"><strong>Mask Layer : {!m.layerNo}</strong></span>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td class="tcell_width_75 no-padding header-txt">
                                                    <input type="text"   class="tbl-txt-box {!IF(m.isEdited_retrofitMaskRev,'edited-text','')}" readonly="true" value="{!m.retroftMaskRev}"/><!-- swgp-2 prime header -->
                                                </td>
                                                <td class="tcell_width_75 no-padding header-txt">
                                                    <input type="text" class="tbl-txt-box {!IF(m.isEdited_retrofitROMCode,'edited-text','')}" readonly="true" value="{!m.retroftRomCode}"/><!-- swgp-2 prime header -->
                                                </td>
                                            </tr>
                                            <tr>
                                                <td class="tcell_width_75">Mask Rev</td>
                                                <td class="tcell_width_75">ROM Code</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                              </apex:repeat>
                              </div>
                              <div class="clear"></div> 
                            <div class="mask-tbl-btm-container min-height300 border-bottom" id="mask-tbl-btm-containerId">
                                <apex:repeat value="{!maskLayerHeaderWrapperList}" var="layer" >        
                                    <div class="mask-tbl-content-wrapper min-height300">
                                        <table class="gen-table mask-tbl border-bottom">
                                            <tbody>                            
                                                <apex:repeat value="{!chipWrapperList}" var="chip">
                                                        <apex:variable var="cell" value="{!mapDisplayedMaskLayerData[layer.layerID][chip.chipID]}"/>
                                                        <tr class="tcell-height-30 {!chip.colorClass}">
                                                            <td class="tcell_width_75 {!IF(cell['isEdited_maskRev'],'edited-text','')}">{!cell['MaskRev']}</td><!-- swgp-2 prime chip -->
                                                            <td class="tcell_width_75 {!IF(cell['isEdited_romCode'],'edited-text','')}">{!cell['RomCode']}</td><!-- swgp-2 prime chip -->
                                                         </tr>
                                             </apex:repeat>
                                             </tbody>
                                        </table>
                                    </div>
                                </apex:repeat>
                            </div>                                
                            </div>
                            </div>
                    </apex:outputpanel>
                   </apex:outputPanel>
                    <!---Start of Frame-------------->
                 <apex:outputPanel id="frameDRTOutputId">
                 <apex:outputPanel id="frameDRTId" rendered="{!drtFrameType}">
                 <div id="frameLayerTable">              
                 <table class="data-table border-1" id="frameDRTTable">
                  <thead class="thead_height_68">
                            <tr class="alt-row-bgcolor">
                                <th class="tcell-width-20">No.</th>
                                <th class="tcell-width-83">Mask Set Title</th>
                                <th class="tcell-width-83">Mask Layer</th>
                                <th class="tcell_width_80">Retrofit PTRF Mask Rev</th>
                                <th class="tcell_width_80">Retrofit PTRF ROM Code</th>
                                <th class="tcell_width_68">Mask Rev</th>
                                <th class="tcell_width_68">ROM Code</th>
                                </tr>
                        </thead>
                  <tbody>
                    <apex:repeat value="{!lstframe }"  var="frameDrt">
                       <apex:variable var="colorClass" value="{!IF(frameDrt.color,'alt-row-bgcolor','')}"/>
                       <tr class="tcell-height-35 {!colorClass}">
                            <td class="tcell-width-20 text-align-centre">{!frameDrt.counter}</td>  
                            <td class="tcell-width-83 padding-left-10">{!frameDrt.mst}</td>
                            <td class="tcell-width-83 padding-left-12">{!frameDrt.maskLayer}</td>
                            <td class="tcell_width_80 {!IF(frameDrt.isEdited_retroMaskRev,'edited-text','')}">{!frameDrt.retrofitMaskRev}</td><!-- swgp-2 frame -->
                            <td class="tcell_width_80 {!IF(frameDrt.isEdited_retroROMCode,'edited-text','')}">{!frameDrt.retrofitRomCode }</td><!-- swgp-2 frame -->
                            <td class="tcell_width_68 padding-left-12">{!frameDrt.maskRev}</td>
                            <td class="tcell_width_68 padding-left-12">{!frameDrt.romCode}</td>                    
                        </tr>
                     </apex:repeat>
                </tbody>                
            </table>
            </div>
            </apex:outputPanel> 
            </apex:outputPanel>
                    <!-----End of Frame--------------->
                    </div>
                     </div>
                      </div>
                    </apex:tab>
                    <apex:tab label="Where Use" name="where_used" styleClass="detailsContent">
                        <div style="background-color:white;">
                            <div id="firstRowContentDiv" class="right-section-row right-section-row-borderbottom">
                                    <table> 
                                        <tr>
                                            <td>                    
                                                <dl><dt class="bold-font fontSize-12 min-width-inputLabel">Customer Name:</dt><dd class="pading-left">{!savedCustomerName}</dd></dl>
                                            </td>
                                            <td>
                                                <dl><dt class="dt-style-margin bold-font fontSize-12">Status:</dt><dd class="pading-left">{!savedStatus}</dd></dl>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <dl><dt class="bold-font fontSize-12 min-width-inputLabel">Mask Set Title:</dt><dd class="pading-left">{!savedMaskSetTitle}</dd></dl>
                                            </td>
                                            <td>
                                                <dl><dt class="dt-style-margin bold-font fontSize-12">DRT Type:</dt><dd class="pading-left">{!savedDRTType}</dd></dl>
                                            </td>
                                        </tr>
                                        <apex:outputPanel rendered="{!isTechGeoNull}">
                                        <tr>
                                            <td>
                                               <dl><dt class="bold-font fontSize-12 min-width-inputLabel">Tech Geo:</dt><dd class="pading-left"> {!savedTechGeo}</dd></dl>
                                            </td>
                                            <td>
                                                  <dl><dt class="bold-font fontSize-12"></dt><dd class="pading-left"> &nbsp;</dd></dl>
                                            </td>
                                        </tr>
                                       </apex:outputPanel>
                                    </table>                    
                                <div class="clear"></div>
                            </div>
                            <div class="right-section-row">
                        <!--Start of  Where use Table section-->
                              <div id="whereUseTable" class="detail-table-style">  
                                    <table class="data-table">
                                        <thead>
                                            <tr>
                                                 <th class="tcell-width-83" rowspan="2"><span>PTRF Number</span></th>
                                                 <th class="tcell-width-120" rowspan="2"><span>Order Status</span></th>
                                                 <th class="tcell-width-83" rowspan="2"><span>Mask Set Title</span></th>
                                                 <th class="tcell-width-91" rowspan="2"><span>Device ID</span></th>
                                                 <th colspan="5"><span>Tapeout Options</span></th>
                                            </tr>
                                            <tr>
                                                  <th class="tcell-width-78"><span>Frame Tapeout</span></th>
                                                  <th class="tcell-width-78"><span>Frame Mockup</span></th>
                                                  <th class="tcell-width-78"><span>Prime Tapeout</span></th>
                                                  <th class="tcell-width-110"><span>Prime GDSOUT Review</span></th>
                                                  <th class="tcell-width-73"><span>DRC</span></th>                    
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <apex:repeat value="{!PTRFDrtAssoWrapperlst}" var="drtptrf">
                                                <apex:variable var="colorClass" value="{!IF(drtptrf.color,'alt-row-bgcolor','')}"/>
                                                <tr class="{!colorClass}"> 
                                                
                                                    <td class="tcell-width-73">
                                                        <!-- TASK-2469 -->
                                                        <!--  
                                                        <a href="{!currentUrl}VFPTRFDetail?ptrfURL={!drtptrf.ptrfId}"  target="_blank" class="tcell-width-74">{!drtptrf.ptrf}</a>
                                                        -->
                                                        <a href="{! $Label.HYPERLINK_TO_SWIFT + drtptrf.ptrf }" title="{!drtptrf.ptrf}" target="_blank">{!drtptrf.ptrf}</a>
                                                        <!-- TASK-2469 END -->
                                                    </td>
                                                    
                                                    
                                                    <td  class="tcell-width-110">{!drtptrf.orderStatus}</td>
                                                    <td class="tcell_width_80">{!drtptrf.mst}</td>
                                                    <td class="tcell-width-91">{!drtptrf.deviceId }</td>
                                                    <td class="tcell-width-74"> <apex:image alt="Checked image" styleClass="float-none" url="{!URLFOR($Resource.Swift_DRT,'images/Checked_img.png')}" width="13" height="7" rendered="{!drtptrf.frameTapeOut}"/>
                                                    </td>
                                                    <td class="tcell_width_80">
                                                    <apex:image alt="Checked image" styleClass="float-none" url="{!URLFOR($Resource.Swift_DRT,'images/Checked_img.png')}" width="13" height="7" rendered="{!drtptrf.frameMockup}"/>
                                                    </td>
                                                    <td class="tcell_width_25">
                                                    <apex:image alt="Checked image" styleClass="float-none" url="{!URLFOR($Resource.Swift_DRT,'images/Checked_img.png')}" width="13" height="7" rendered="{!drtptrf.primeTapeOut}"/>
                                                    </td>
                                                    <td class="tcell_width_112">
                                                    <apex:image alt="Checked image" styleClass="float-none" url="{!URLFOR($Resource.Swift_DRT,'images/Checked_img.png')}" width="13" height="7" rendered="{!drtptrf.primeGDSOUTReview}"/>
                                                    </td>
                                                    <td class="tcell-width-73">
                                                    <apex:image alt="Checked image" styleClass="float-none" url="{!URLFOR($Resource.Swift_DRT,'images/Checked_img.png')}" width="13" height="7" rendered="{!drtptrf.drc}"/>
                                                    </td>
                                                </tr>
                                            </apex:repeat>
                                        </tbody>
                                </table>
                          </div>
                      </div>
                      </div>
                    </apex:tab>
                    <apex:tab label="Audit Trail" name="audit_trail" styleClass="detailsContent">
                          <div style="background-color:white;">
                              <div id="firstRowContentDiv" class="right-section-row right-section-row-borderbottom">
                                    <table> 
                                        <tr>
                                            <td>                    
                                                <dl><dt class="bold-font fontSize-12 min-width-inputLabel">Customer Name:</dt><dd class="pading-left">{!savedCustomerName}</dd></dl>
                                            </td>
                                            <td>
                                                <dl><dt class="dt-style-margin bold-font fontSize-12">Status:</dt><dd class="pading-left">{!savedStatus}</dd></dl>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <dl><dt class="bold-font fontSize-12 min-width-inputLabel">Mask Set Title:</dt><dd class="pading-left">{!savedMaskSetTitle}</dd></dl>
                                            </td>
                                            <td>
                                                <dl><dt class="dt-style-margin bold-font fontSize-12">DRT Type:</dt><dd class="pading-left">{!savedDRTType}</dd></dl>
                                            </td>
                                        </tr>
                                        <apex:outputPanel rendered="{!isTechGeoNull}">
                                        <tr>
                                            <td>
                                               <dl><dt class="bold-font fontSize-12 min-width-inputLabel">Tech Geo:</dt><dd class="pading-left"> {!savedTechGeo}</dd></dl>
                                            </td>
                                            <td>
                                                  <dl><dt class="bold-font fontSize-12"></dt><dd class="pading-left"> &nbsp;</dd></dl>
                                            </td>
                                        </tr>
                                       </apex:outputPanel>
                                    </table>                    
                                <div class="clear"></div>
                            </div>
                          <div class="right-section-row">
                          <!--Start of Audit Trail Table-->
                          <div id="drtAuditTrailTable">
                                        <table class="data-table">
                                            <thead>
                                                <tr>
                                                    <th class="tcell-width-114" rowspan="2"><span>Date</span></th>
                                                    <th class="tcell-width-91" rowspan="2"><span>User</span></th>
                                                    <th class="tcell-width-200" rowspan="2"><span>Description</span></th>
                                                    <th colspan="3"><span>Action</span></th>                     
                                                </tr>
                                                <tr>
                                                  <th class="tcell-width-355"><span>Field Change</span></th>
                                                  <th><span>Old Val</span></th>
                                                  <th><span>New Val</span></th>                                        
                                                </tr>
                                            </thead>
                                            <tbody>
                                                    <apex:variable value="{!1}" var="counter"/>
                                                    <apex:repeat value="{!drtAuditTrailWrappLst}" var="drtAuditTrailWrapp">
                                                        <apex:variable var="colorClass" value="{!IF(mod(counter,2)==0,'alt-row-bgcolor','')}"/>
                                                        <tr class="{!colorClass}"> 
                                                            <td class="tcell-width-114">{!drtAuditTrailWrapp.createdDate}</td>
                                                            <td  class="tcell-width-91">{!drtAuditTrailWrapp.userName}</td>
                                                            <td class="tcell_width_94">{!drtAuditTrailWrapp.description}</td>                                        
                                                            <td class="tcell-width-91">{!drtAuditTrailWrapp.fieldChange}</td>
                                                            <td class="tcell-width-74">{!drtAuditTrailWrapp.oldValue}</td>
                                                            <td class="tcell_width_80">{!drtAuditTrailWrapp.newValue}</td>
                                                        </tr>
                                                       <apex:variable var="counter" value="{!counter+ 1}"/>
                                                   </apex:repeat>
                                             </tbody>                        
                                          </table>
                                     </div>
                                 </div>
                         </div>
                    </apex:tab>
                </apex:tabPanel>
        <!--End of Tables-->
        <br/>
   </div>
   <!------ End of Details tab ------->            
   </div>
    <!-- --------------- Second Row Ends --------------- -->
    <div class="clear"></div>                 
    </div> 
</div>
<!--End of Wrapper div-->
<script type="text/javascript" src="{!URLFOR($Resource.Swift_DRT,'js/advancedDataGrid.js')}"></script>
    
<apex:actionFunction name="redirectRevise" action="{!redirectToRevise}" reRender="errorPanel"/>
<apex:actionFunction name="redirectUpdate" action="{!redirectToUpdate}" reRender="errorPanel"/>
<apex:actionFunction name="redirectCancel" action="{!redirectToCancel}" reRender="drtDetailId,errorPanel" oncomplete="enableButton('drtCancelButton');hideLoader();"/>
<apex:actionFunction name="redirectDelete" action="{!redirectToDelete}" reRender="errorPanel" oncomplete="hideLoader();"/>
<apex:actionFunction name="redirectCopy" action="{!redirectToCopy}" reRender="errorPanel" oncomplete="enableButton('drtCopyButton');hideLoader();"/>
</apex:form>
