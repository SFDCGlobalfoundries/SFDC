<apex:page sidebar="false"  controller="ClsSupplierService" id="supplier_designpage">

    <!---------------------------------------------------     StyleSheet    -------------------------------------------------------------->
    <apex:stylesheet value="{!URLFOR($Resource.NewFV2Styles, 'NewFV2Styles/css/style.css')}" id="stylesheet_location"/> 
    <!------------------------------------------------------------------------------------------------------------------------------------>
    
    <!---------------------------------------------------     JS File   ------------------------------------------------------------------>
    <apex:includeScript value="{!URLFOR($Resource.NewFV2Styles, 'NewFV2Styles/js/jquery.min.js')}" id="jqueryminjs_location"/>
    <apex:includeScript value="{!URLFOR($Resource.NewFV2Styles, 'NewFV2Styles/js/leftmenu.js')}" id="leftmenujs_location"/>
    <!------------------------------------------------------------------------------------------------------------------------------------>
   
    <apex:form id="myform">
    <script type="text/javascript">
        function openpopup(test)
        {  
       // window.open(test,"openUrl","wi​dth=670,height=480"); }
         window.open(test); }
    </script>
        <apex:iframe src="{!OpenTextLoginServerURL}" height="1px" width="1px" />
        <div id="wrapper">
            <div id="container" class="use-sidebar">
                <div id="leftpanel">
                    <div id="leftmenu">
                        <h2 class="first">Applications</h2>
                        <ul id="nav" class="leftnav">
                            <li><apex:commandLink onclick="OpenPage('msqs');openpopup('{!$Label.Material_Selection_Qualification_SystemURL}');return false;" value="{!$Label.Material_Selection_Qualification_System}" id="msqs" rendered="{!showmsqsLink}"/></li>
                            <li><apex:commandLink onclick="OpenPage('ses');openpopup('{!$Label.Supplier_eCOA_SubmissionURL}');return false;" value="{!$Label.Supplier_eCOA_Submission}" id="ses" rendered="{!showsesLink}"/></li>
                            <li><apex:commandLink onclick="OpenPage('pra');openpopup('{!$Label.Procedure_Review_AcceptanceURL}'); return false;" value="{!$Label.Procedure_Review_Acceptance}" id="pra" rendered="{!showpraLink}"/></li>
                            <li><apex:commandLink onclick="OpenPage('wds');openpopup('{!$Label.Wafer_Disposition_SystemURL}'); return false;" value="{!$Label.Wafer_Disposition_System}" id="wds" rendered="{!showwdsLink}"/></li>
                            <li><apex:commandLink onclick="OpenPage('tir');openpopup('{!$Label.Turnkey_Incident_ReportURL}'); return false;" value="{!$Label.Turnkey_Incident_Report}" id="tir" rendered="{!showtirLink}"/></li>
                             <li><apex:commandLink onclick="OpenIframe('sal'); return false;" value="{!$Label.Specs_Acknowledgement}" id="sal" rendered="{!showASLink}"/></li>
                            <li><apex:commandLink onclick="OpenPage('simi');openpopup('{!$Label.Supplier_Incoming_Material_Incidents_IMI_URL}'); return false;" value="{!$Label.Supplier_Incoming_Material_Incidents_IMI}" id="simi" rendered="{!showsimiLink}"/></li>
                            <li><apex:commandLink onclick="OpenPage('iCN');openpopup('{!$Label.Change_Notification_URL}'); return false;" value="{!$Label.Change_Notification}" id="iCN" rendered="{!showiCNLink}"/></li>
							</ul>
                        <br/><br/>
                        <apex:outputPanel rendered="{!showprocLink}">
                         <h2 class="first">Publications</h2>
                         <ul class="leftnav">
                            <li><apex:commandLink onclick="OpenIframe('proc'); return false;" value="{!$Label.Procedure}" id="proc" rendered="{!showprocLink}"/></li>
                            <li><apex:commandLink onclick="OpenIframe('ehs'); return false;" value="{!$Label.EHS_Policy}" id="ehs" rendered="{!showehsLink}"/></li>
                           
                         </ul>
                         <br/><br/>
                         </apex:outputPanel>
                         <h2 class="first">Training Materials</h2>
                        <ul id="nav" class="leftnav">
                            <li><apex:commandLink onclick="OpenIframe('sao');return false;" value="{!$Label.SQE_Applications_Overview}" id="sao" rendered="{!showsaoLink}"/></li>
                        </ul>

                      </div><a href="#" id="separator"></a>
                 </div> 
                 <div id="content">
                   <apex:pageBlock id="mainpb">
                        <apex:outputPanel rendered="{!showwelcomesection}" id="welcomepb">
                        <apex:image url="{!URLFOR($Resource.NewFV2Styles,'NewFV2Styles/images/header-icon.png')}" width="35px" height="35px" id="caption_image"/>
                        <apex:outputLabel value="About" id="welcomelabel" style="vertical-align:bottom;font-size:18px;font-style:normal;position:absolute;padding-top:8px;"/><br/>
                        <hr color="#cbcbcb" size="1" width="100%"/>
                        <p class="disc">Make your selection from the menu to left</p>
                        <div class="menudesc">  
                            <apex:image url="{!URLFOR($Resource.NewFV2Styles,'NewFV2Styles/images/banner.jpg')}" width="261px" height="265px" id="banner_image"/>
                            <h2>Supplier Service</h2>
                            <p>Supplier Service</p>
                            <div class="clear"></div>
                        </div>
                    </apex:outputPanel>
                    
                    <apex:outputPanel rendered="{!showdetailsection}" id="detailpb">
                        <apex:image url="{!URLFOR($Resource.NewFV2Styles,'NewFV2Styles/images/qualityReportsIcon.gif')}" width="30px" height="30px" id="subject_image"/>
                        <apex:outputLabel value="{!respectiveLevel}" id="label" style="position:absolute;vertical-align:bottom;font-size:17px;font-style:normal;margin-top:10px"/>
                        <hr color="#ccced0" size="1" width="100%"/>
                        <apex:iframe width="100%" height="500px" src="{!OpenPageURL}" scrolling="true" rendered="{!showpageBlock}" id="frame" />
                    </apex:outputPanel>
                    
                    </apex:pageBlock>
                    <div class="clear"/>
              </div>
              <div class="clear"/>
            </div>
        </div>
        <apex:actionFunction action="{!redirect}" name="OpenPage" reRender="mainpb">
            <apex:param assignTo="{!Page}" value="" name="param1"/>
        </apex:actionFunction> 
        <apex:actionFunction action="{!redirect}" name="OpenIframe" reRender="label,mainpb,welcomepb,detailpb,frame">
            <apex:param assignTo="{!Page}" value="" name="param1"/>
        </apex:actionFunction> 
    </apex:form>
</apex:page>