<apex:page standardController="SFDC_Project__c" tabStyle="SFDC_Project__c" showChat="false" extensions="SFDCProjectMilestonesController">

  <apex:pageBlock >
  
      
          <apex:pageBlockTable value="{!milestones}" var="m">
            <apex:column headerValue="Stage"> <a href="/{!m.Stage__r.Id}" target='_parent'> {!m.Stage__r.Name} </a></apex:column> 
            <apex:column headerValue="Milestone"> <a href="/{!m.Id}" target='_parent'> {!m.Name} </a></apex:column> 
            <apex:column headerValue="Start Date" value="{!m.Start_Date__c}"/> 
            <apex:column headerValue="End Date" value="{!M.End_Date__c}"/> 
            <apex:column headerValue="Stage Complete" value="{!m.Stage__r.Completed__c}"/>  
            <apex:column headerValue="Milestone Complete" value="{!m.Completed__c}"/>  
        </apex:pageBlockTable>
      
      
  
  </apex:pageBlock>

 
</apex:page>