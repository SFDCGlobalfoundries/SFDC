<apex:page showHeader="false" title="{!$Label.site.in_maintenance}" cache="false">
  <apex:composition template="StdExceptionTemplate">    
    <apex:define name="error">
    <apex:image height="40%" width="50%"  url="https://c.cs5.content.force.com/servlet/servlet.ImageServer?id=015O0000000GoAp&oid=00DO000000040Wz"/>
    <br/>
    <apex:outputText value="Insufficient Privileges.You do not have the level of access necessary to perform the operation you requested"/>         
     <br/> 
     <apex:outputText styleClass="title" value="{!$Site.CurrentSiteUrl}"/> 
      <br/>
      <br/>
      <apex:outputText value="{!$Label.site.sorry_for_inconvenience_back_shortly}"/>
      <apex:outputText escape="false" value=" {!$Label.site.get_in_touch}">
      <apex:param value="mailto:{!$Site.AdminEmailAddress}"/>
      <apex:param value="{!$Label.site.email_us}"/>
    </apex:outputText>
   </apex:define>
  </apex:composition>
</apex:page>