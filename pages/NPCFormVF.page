    function validateECNDate(e){
        try {
            var dateRange = {!$Label.ECN_Date_Range};
            var errorMsg = "{!$Label.ECN_Date_Error_Message}";
            
            var enteredDate = new Date($(e).val().split('/')[2], $(e).val().split('/')[0] - 1, $(e).val().split('/')[1]);

            var today = new Date();
            today.setDate(today.getDate() + parseInt(dateRange));

            if(enteredDate <= today){
                alert(errorMsg);
                $(e).val('');
            }
        }
        catch(err) {
        
        }
    }
