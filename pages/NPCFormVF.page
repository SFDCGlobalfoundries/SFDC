        function openLookup(baseURL, width, modified, searchParam) {
            var originalbaseURL = baseURL;
            var originalwidth = width;
            var originalmodified = modified;
            var originalsearchParam = searchParam;
            var isCustomLookup = false;
            var lookupType = baseURL.substr(baseURL.length - 3, 3);
            
            if (modified == '1') {
                baseURL = baseURL + searchParam;
            }
            
            if (baseURL.indexOf("GlobalfoundryView") != -1) {
                var urlArr = baseURL.split("&");
                var txtId = '';
                
                if(urlArr.length > 2) {
                    urlArr = urlArr[1].split('=');
                    txtId = urlArr[1];
                }
                
                // Custom Lookup to be used
                if (lookupType == "001") {
                    if (baseURL.indexOf("GlobalfoundryView") != -1) {
                        baseURL = "/GlobalfoundryView/apex/NPCAccountLookupVF?txt=" + txtId;
                    } else {
                        baseURL = "/apex/NPCAccountLookupVF?txt=" + txtId;
                    }
                } else {
                    if (baseURL.indexOf("GlobalfoundryView") != -1) {
                        baseURL = "/GlobalfoundryView/apex/NPCDeviceLookupVF?txt=" + txtId;
                    } else {
                        baseURL = "/apex/NPCDeviceLookupVF?txt=" + txtId;
                    }
                }
                
                baseURL = baseURL + "&frm=" + escapeUTF("{!$Component.form1}");
                
                // Get selected account
                var acctValue;
                if (baseURL.indexOf("GlobalfoundryView") != -1) {
                    //acctValue = document.getElementById("NPCFormVF:form1:BlkEdit:account3").innerHTML;
                    acctValue = document.getElementById("NPCFormVF:form1:BlkEdit:account3Id").value;
                } else {
                    // For review with Zaih
                    acctValue = document.getElementById("NPCFormVF:form1:BlkEdit:account1").value;
                }
                
                baseURL = baseURL + "&acct=" + acctValue;
                
                if (modified == '1') {
                    baseURL = baseURL + "&lksearch=" + searchParam;
                }
                
                isCustomLookup = true;
            }
            
            if (isCustomLookup == true) {
                openPopup(baseURL, "lookup", 350, 580, "width=" + width + ",height=580, toolbar=no, status=no, directories=no, menubar=no, resizable=yes, scrollable=no", true);
            } else {
                if (modified == '1') {
                    originalbaseURL = originalbaseURL + originalsearchParam;
                }
                
                openPopup(originalbaseURL, "lookup", 350, 480, "width=" + originalwidth + ",height=480, toolbar=no, status=no, directories=no, menubar=no, resizable=yes, scrollable=no", true);
            }
        }
        
