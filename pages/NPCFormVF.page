        function validateAttachment() {
      
      
            var inputFiles = document.getElementsByTagName("input");
            for (var z=0; z<inputFiles.length; z++) {
                if (inputFiles[z].type == "file") {
                    if (inputFiles[z].files[0]) {
                        if (inputFiles[z].files[0].size > (1024 * 1024 * 5)) {
                            alert("Each file attachment should not exceed 5MB.");
                            return false;
                        }
                    }
                }
            }
            
            return true;
        }
       

