<apex:page standardController="SFDC_Project__c" tabStyle="SFDC_Project__c" showChat="false" extensions="SFDCProjectExt">

<div style="font-size:20px;padding-top:5px;">SFDC Project: <span style="font-size:20px;font-weight:bold;">{!SFDC_Project__c.name}</span></div>

<chatter:feedWithFollowers entityId="{!SFDC_Project__c.id}"/>
<div style='padding-top:10px;'></div>
<style>
   .myLabel
   {
      font-weight:bold;
      color:#4a4a56;font-family: Arial, Helvetica, sans-serif;
      font-size: 11px;
   }
</style>

<apex:form >
<apex:pageBlock title="Detail" >
   
 <apex:pageBlockButtons >
     <apex:CommandButton action="{!Edit}" value="Edit" />
     <apex:CommandButton action="{!Delete}" value="Delete" />
 </apex:pageBlockButtons>
  <apex:pageBlockSection collapsible="false" columns="2">
     <apex:outputField value="{!SFDC_Project__c.Name}"/>
     <apex:outputField label="Created By" value="{!SFDC_Project__c.CreatedBy.Name}"/>
     <apex:outputField value="{!SFDC_Project__c.Project_Status_Indicator__c}"/>
    
     <apex:outputField label="Last Modified By" value="{!SFDC_Project__c.LastModifiedBy.Name}"/>
     <apex:outputField value="{!SFDC_Project__c.Priority_Sequence__c}"/>
     <apex:pageBlockSectionItem ></apex:pageBlockSectionItem>
      <apex:outputField value="{!SFDC_Project__c.Overall_Status__c}"/>
     <apex:outputField value="{!SFDC_Project__c.Status_Date__c}"/>
     
     <apex:outputField label="Project Start Date" value="{!SFDC_Project__c.Start_Date__c}"/>
     <apex:outputField label="Project End Date" value="{!SFDC_Project__c.End_Date__c}"/>
     <apex:outputField value="{!SFDC_Project__c.Description__c}"/>
     <apex:pageBlockSectionItem />
     <apex:pageBlockSectionItem >
        <apex:outputLabel for="currStageName" styleClass="myLabel" value="Current Stage"></apex:outputLabel>
        <apex:outputText id="currStageName" value="{!CurrentStageName}"></apex:outputText>
     </apex:pageBlockSectionItem>
     
     <apex:pageBlockSectionItem >
        <apex:outputLabel for="currMilestoneName" styleClass="myLabel" value="Current Milestone"></apex:outputLabel>
        <apex:outputText id="currMilestoneName" value="{!CurrentMilestoneName}"></apex:outputText>
     </apex:pageBlockSectionItem>
     
     <apex:outputField value="{!SFDC_Project__c.Sponsoring_Organization__c}"/>
     <apex:outputPanel id="other" rendered="{!SFDC_Project__c.Sponsoring_Organization__c == 'Other'}">
        <span style="font-weight:bold;padding-right:15px;">Other (Sponsoring Organization)</span> {!SFDC_Project__c.Other_Sponsoring_Organization__c}
      </apex:outputPanel>
      
  </apex:pageBlockSection>
  
  <apex:pageBlockSection collapsible="false" columns="2">
      <apex:outputField value="{!SFDC_Project__c.Business_Capability__c}"/>
      <apex:outputPanel id="other" rendered="{!SFDC_Project__c.Business_Capability__c == 'Other'}">
        <span style="font-weight:bold;padding-right:15px;">Other (Business Capability)</span> {!SFDC_Project__c.Business_Capability_Other__c}
      </apex:outputPanel>
      
      <apex:outputField value="{!SFDC_Project__c.User_Group__c}"/>
      <apex:outputField value="{!SFDC_Project__c.Other_User_Group__c}"/>
      <apex:outputField value="{!SFDC_Project__c.Category__c}"/>
  </apex:pageBlockSection>

  <apex:pageBlockSection collapsible="false" columns="2">
      <apex:outputField value="{!SFDC_Project__c.Business_Owner__c}"/>
      <apex:outputField value="{!SFDC_Project__c.WWMS_System_Solution_Owner__c}"/>
      <apex:outputField value="{!SFDC_Project__c.IT_Owner__c}"/>
      <apex:outputField value="{!SFDC_Project__c.IT_Project_Manager__c}"/>
  </apex:pageBlockSection>
  <apex:pageBlockSection collapsible="false" columns="1">
      <apex:outputField label="Primary Selected Vendor" value="{!SFDC_Project__c.Vendor__c}"/>
      <apex:outputField label="Selected Vendor 2" value="{!SFDC_Project__c.Vendor2__c}"/>
      <apex:outputField label="Selected Vendor 3" value="{!SFDC_Project__c.Vendor3__c}"/>
      
  </apex:pageBlockSection>
  

  
  <apex:pageBlockSection collapsible="false" columns="1">
      <apex:outputField value="{!SFDC_Project__c.Urgency__c}"/>
      <apex:outputField value="{!SFDC_Project__c.Impact__c}"/>
      
    <table class="detailList" cellspacing="0" cellpadding="0" border="0">
       <tbody>
        <tr></tr><tr></tr><tr></tr>
        <tr><td class="labelCol last ">Urgency</td><td class="dataCol  last ">{!SFDC_Project__c.Urgency__c}</td>
        <td class="labelCol empty"></td><td class="dataCol empty"></td>
        </tr>
        <tr> <td class="labelCol last "></td>
           <td>
           <div style="border: 1px solid black;">
           <div style="text-align:left;font-weight:bold;">Project Urgency Legend</div>
           <ul>
               <li>Immediate (3-6 Months)</li>
               <li>Mid-Term (6 -12 Months)</li>
               <li>Long Term (12 month or beyond)</li>               
           </ul>
           </div>
        </td>
        <td class="labelCol empty"></td><td class="dataCol empty"></td>
        </tr>
        <tr><td class="labelCol last ">Impact</td><td class="dataCol  last ">{!SFDC_Project__c.Impact__c}</td>
        <td class="labelCol empty"></td><td class="dataCol empty"></td>
        </tr>
        <tr> <td class="labelCol last "></td>
           <td>
           <div style="border: 1px solid black;">
           <div style="text-align:left;font-weight:bold;">Project Impact on Revenue Legend</div>
           <ul>
               <li>High - $Million or More</li>
               <li>Medium - $250k to $Million</li>
               <li>Low - Less than $250k</li>
           </ul>
           </div>
        </td>
        <td class="labelCol empty"></td><td class="dataCol empty"></td>
        </tr>
        </tbody>
     </table>
        
        
        
   </apex:pageBlockSection>
   
  
   
   
</apex:pageBlock>




</apex:form>


<apex:outputLabel value="Gantt Chart"  style="font-weight:bold;font-size:18px;"></apex:outputLabel>
<apex:include pageName="SFDCProjectGanttChart"/>
<apex:outputLabel value="Stages" style="font-weight:bold;font-size:16px;"></apex:outputLabel>
<apex:include pageName="SFDCProjectStagesList"/>
<apex:outputLabel value="Statuses" style="font-weight:bold;font-size:16px;"></apex:outputLabel>

<apex:relatedList list="Project_Status__r" />
<apex:relatedList list="Issues__r" />

<apex:relatedList list="SFDC_Project_Members__r"/>
<apex:relatedList list="Change_Requests__r" />
<apex:relatedList list="Reviews__r" />
<apex:relatedList subject="{!SFDC_Project__c}" list="NotesAndAttachments" />
<apex:include pageName="SFDCProjectChangeHistoryList"/>
</apex:page>