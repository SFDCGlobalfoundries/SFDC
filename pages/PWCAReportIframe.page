<apex:page standardController="PWCA_Attachments__c" extensions="PWCAReportUploadFile"  id="page">
 <apex:sectionHeader title="PWCA Report" subtitle="New PWCA Report" />
 <apex:form id="theForm">  

 <apex:pageMessages />
 <apex:pageBlock title="Upload PWCA Report" id="block" >
 
 <apex:pageBlockButtons >
 <apex:commandButton value="Save" action="{!doSave}" />
 <apex:commandButton value="Cancel" action="{!doCancel}"/>
 </apex:pageBlockButtons>
  <apex:pageBlockSection title="Basic Info" collapsible="false" columns="2" id="blockSection1"> 
             <apex:pageBlockSectionItem >
                <apex:outputText value="{!$ObjectType.PWCA_publication__c.fields.Name.label}"/>
                <apex:outputText value="{!pwcaRecord.Name}" />
            </apex:pageBlockSectionItem>
        </apex:pageBlockSection>
 
 <apex:pageBlockSection columns="1" >
 <apex:iframe id="vfOpenText" src="{!srcUrl}" width="100%" height="440px" scrolling="false"/>
 </apex:pageBlockSection>
 </apex:pageBlock>
 </apex:form>
</apex:page>