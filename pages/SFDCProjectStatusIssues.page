<apex:page standardController="SFDC_Project_Status__c" extensions="SFDCProjectStatusExt" showChat="false" sidebar="false">

<apex:pageBlock >
      <apex:pageBlockTable value="{!openIssues}" var="issue">
          <apex:column headerValue="Title"><a href="/{!issue.Id}" target="_parent">{!issue.Name}</a></apex:column> 
          <apex:column value="{!issue.Type__c}"/>
          <apex:column value="{!issue.Status__c}"/>
      </apex:pageBlockTable>
</apex:pageBlock>
  
</apex:page>