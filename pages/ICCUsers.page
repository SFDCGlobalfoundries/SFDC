<apex:page controller="IccUserController">
 <!-- <apex:form >-->
  <style>
        .headerRow .TableTitle {
            font-size:115% !important;
            font-weight: bold !important;
            color: black !important; 
        }    
    </style>
  
    
    <!---------------------------------------------------     StyleSheet    -------------------------------------------------------------->
  
    <apex:stylesheet value="{!URLFOR($Resource.CommunityStyle, 'CommunityStyle/css/visualforce.css')}" id="stylesheetvisualforce"/>  
    <apex:stylesheet value="{!URLFOR($Resource.CommunityStyle, 'CommunityStyle/css/fonts.css')}" id="stylesheet_fonts"/> 
    <!------------------------------------------------------------------------------------------------------------------------------------>

    <!---------------------------------------------------     JS File   ------------------------------------------------------------------>
    <apex:includeScript value="{!URLFOR($Resource.CommunityStyle, 'CommunityStyle/js/jquery.min.js')}" id="jqueryminjs_location"/> 
    <!------------------------------------------------------------------------------------------------------------------------------------>    
    <!-- Defakto Script to make current link color orange -->
    <apex:includeScript value="{!URLFOR($Resource.CommunityStyle, 'CommunityStyle/js/linkclick.js')}" id="linkclickjs_location"/>
    <!------------------------------------------------------------------------------------------------------------------------------------>

<div id="content">
     <div class="Title2">ICC Users</div>
      <br /><br />
     <div class="leftBlock2">     
        <div class="menudesc">
        
        </div>   
        <div class="getStartedLinksBlock">
          <h3>Training on Migration from ICC to Global-FoundryView<br /><br /></h3>
         </div>
       <apex:pageBlock mode="maindetail">
        <apex:pageblockTable style="width:125%;height:100px;" value="{!myWrappers}" var="u">
            <apex:column headerValue="TITLE" headerClass="TableTitle">
                <apex:outputLink target="_blank" value="/sfc/servlet.shepherd/version/download/{!u.sId}?asPdf=false">{!u.sTitle }</apex:outputLink><br/>
            </apex:column>
            
            <apex:column headerValue="DESCRIPTION" headerClass="TableTitle">
                <apex:outputText value="{!u.sDescription}"></apex:outputText>
            </apex:column>
        </apex:pageblockTable>
     </apex:pageblock>   
     </div>
     
         <div class="rightImageBlock2">
           
            <apex:image url="{!URLFOR($Resource.CommunityStyle, 'CommunityStyle/images/gettingstarted.jpg')}" styleClass="rightImage" />
         </div>      
      
        <iframe src="/GlobalfoundryView/apex/VFOTLoginService" width="100%" frameborder="0" height="50"> </iframe>  
</div>
   
<!--</apex:form>-->
</apex:page>