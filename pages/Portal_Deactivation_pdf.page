<apex:page controller="Portal_Deactivation_pdf_Controller" contentType="application/vnd.ms-excel#PortalUserDeactivation/Notification.xls" cache="true" showHeader="false"  sidebar="false" standardStylesheets="false" applyHtmlTag="false">
        <apex:pageBlock >
            <apex:pageBlockTable value="{!lstCons}" var="c" style="border:4px solid #1797c0;padding:6px;">
                <apex:column headerValue="Contact Name" value="{!c.Name}" style="border:1px solid #1797c0;padding:6px;"/>
                <apex:column headerValue="Contact Email" value="{!c.Email}" style="border:1px solid #1797c0;padding:6px;"/>
                <apex:column headerValue="Contact Link" style="border:1px solid #1797c0;padding:6px;">
                    <apex:outputLink > {!urlStr}/{!c.id}</apex:outputLink>
                </apex:column>
            </apex:pageBlockTable>
        </apex:pageBlock>
</apex:page>