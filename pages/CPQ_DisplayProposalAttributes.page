<apex:page standardController="Apttus_Proposal__Proposal__c" extensions="CPQ_DisplayProposalAttributesController" recordSetVar="{!proposalRecordField}">
<apex:form >
<apex:pageBlock rendered="{!isTechnologyConfigured}" >
<apex:repeat value="{!ProdGrpSect}" var="grp">
<apex:pageblockSection title="{!grp}" columns="2">
<apex:repeat value="{!attrMap[grp]}" var="fieldObj">
<apex:pageblocksectionitem >
<apex:outputText value="{!fieldObj.Attvalue}"  />
<apex:outputText value="{!fieldObj.Attlabel}" />
</apex:pageblocksectionitem>
</apex:repeat>
</apex:pageblockSection>
</apex:repeat>         
</apex:pageBlock>
</apex:form>
</apex:page>