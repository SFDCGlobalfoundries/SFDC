<apex:page standardController="MRS_Layer_Chip_Association__c" extensions="MRSSendFrameDataExtn"  showHeader="false" sideBar="false" standardStylesheets="false" ReadOnly="true"
contentType="application/vnd.ms-excel#MRSSendFrameData.xls"  cache="true">

    <table>
    <tr align="center"><td colspan="5"><h2><u>Send Frame Data</u></h2></td></tr>
   </table>
    <br/>
    <table  cellspacing="5"> 
           <apex:outputLabel rendered="{!IF(selectedmst==null || selectedmst='All',false,true)}">
           <tr> 
             <td style="font-weight:bold;">Mask Set Title: </td>
             <td>{!IF(selectedmst==null || selectedmst='All','',mstname)}</td>
           </tr>
           </apex:outputLabel>
            <apex:outputLabel rendered="{!IF(selectedSendData!=null,true,false)}">
           <tr> 
             <td style="font-weight:bold;">Send Data: </td>
             <td>{!selectedSendData}</td>
           </tr>
           </apex:outputLabel>
            <apex:outputLabel rendered="{!IF(ValueCatcher!=null,true,false)}">
             <tr>
               <td style="font-weight:bold;">Layer Status: </td>
               <td>{!ValueCatcher}</td>
             </tr>
           </apex:outputLabel>
           <apex:outputLabel rendered="{!IF(selectedMaskLayer!=null,true,false)}">
             <tr>
               <td style="font-weight:bold;">Mask Layer: </td>
               <td>{!selectedMaskLayer}</td>
             </tr>
           </apex:outputLabel>
          
           
           <apex:outputLabel rendered="{!IF(selectedDeviceId!=null,true,false)}">
             <tr>
               <td style="font-weight:bold;">Device Id: </td>
               <td>{!selectedDeviceId}</td>
             </tr>
           </apex:outputLabel>
           <apex:outputLabel rendered="{!IF(selectedPTRFDRTNumber !=null,true,false)}">
             <tr>
               <td style="font-weight:bold;">PTRF Number: </td>
               <td>{!selectedPTRFDRTNumber }</td>
             </tr>
           </apex:outputLabel>
          
    </table> 
    <br/>
    <table style="border:1px solid #ccc;" cellspacing="0" cellpadding="2" border="1">
        <thead style="background:#e3e3e3;">
            <tr style="height:45px;">
               <th><apex:outputLabel style="border:0px solid #ccc; border-left:0;align:left"><span>No.</span></apex:outputLabel> </th>
               <th><apex:outputLabel style="border:0px solid #ccc; border-left:0;align:left"><span>Mask Set Title</span></apex:outputLabel> </th>
               <th><apex:outputLabel style="border:0px solid #ccc; border-left:0;align:left"><span>Mask Layer(ROM Code)</span></apex:outputLabel></th>
                
               <th> <apex:outputLabel style="border:0px solid #ccc; border-left:0;align:left"><span>PTRF Number</span></apex:outputLabel> </th>
                <th> <apex:outputLabel style="border:0px solid #ccc; border-left:0;align:left"><span>Send Frame Data Status</span></apex:outputLabel> </th>
                <apex:outputLabel rendered="{!reticleType}"> <th> <apex:outputLabel style="border:0px solid #ccc; border-left:0;align:left"><span>Tech Geo</span></apex:outputLabel> </th></apex:outputLabel>
              
               
            </tr>
        </thead>
        <tbody>
         <apex:variable value="{!1}" var="counter"/>    
           <apex:repeat value="{!mapMLCA}" var="key">
            <apex:repeat value="{!mapMLCA[key]}" var="MCLA">
                <tr>
                 <td><apex:outputLabel style="border-right:0px solid #ccc; border-left:0;align:left">{!counter}</apex:outputLabel></td>
                 <td><apex:outputLabel style="border-right:0px solid #ccc; border-left:0;align:left">{!MCLA.objMRSLCA.layer__r.Mask_Set_Title_Name__c}</apex:outputLabel> </td>
                 <td><apex:outputLabel style="border-right:0px solid #ccc; border-left:0;align:left">{!MCLA.MaskLayerRomCode}</apex:outputLabel> </td>
                 
                 <td><apex:outputLabel style="border-right:0px solid #ccc; border-left:0;align:left">{!MCLA.objMRSLCA.PTRF_Number__c}</apex:outputLabel> </td>
                 <td><apex:outputLabel style="border-right:0px solid #ccc; border-left:0;align:left">{!MCLA.objMRSLCA.Send_Frame_Data__c}</apex:outputLabel> </td>
                <apex:outputLabel rendered="{!reticleType}"> <td><apex:outputLabel style="border-right:0px solid #ccc; border-left:0;align:left">{!MCLA.objMRSLCA.Tech_Geo__c}</apex:outputLabel> </td></apex:outputLabel>
              
                </tr>
                 <apex:variable value="{!counter+1}" var="counter"/>
            </apex:repeat>
             </apex:repeat>
        </tbody>
    </table>  

</apex:page>