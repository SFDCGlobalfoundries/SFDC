<apex:page controller="TeamRoomMeetingMinutesController" tabStyle="Team_Room__c" >
  
<apex:pageBlock >

   <apex:pageBlockSection title="Details" collapsible="false">
   <apex:outputField value="{!meetingMinutes.Team_Room__r.Name}" />
   <apex:outputField value="{!meetingMinutes.Name}" />
   <apex:outputField value="{!meetingMinutes.Meeting_Date__c}" />
   <apex:outputField label="Created By" value="{!meetingMinutes.CreatedBy.Name}" />
   <apex:outputField value="{!meetingMinutes.Meeting_Type__c}" />
   <apex:outputField label="Last Modified By" value="{!meetingMinutes.LastModifiedBy.Name}" />
   
   <apex:outputField value="{!meetingMinutes.Status__c}" />
     
   </apex:pageBlockSection>

    <apex:pageBlockSection title="Notes" collapsible="false" columns="1">
        <apex:outputField value="{!meetingMinutes.Meeting_Participants__c}" />
        <apex:outputField value="{!meetingMinutes.Meeting_Notes__c}" />
        
    </apex:pageBlockSection>
    
    <apex:pageBlockSection title="Open Activities" collapsible="false" columns="1">
<apex:relatedList title="" subject="{!meetingMinutes}" list="OpenActivities" />
</apex:pageBlockSection>
<apex:pageBlockSection title="Activity History" collapsible="false" columns="1">
<apex:relatedList title="" subject="{!meetingMinutes}" list="ActivityHistories" />
</apex:pageBlockSection>
<apex:pageBlockSection title="Notes and Attachments" collapsible="false" columns="1">
<apex:relatedList title="" subject="{!meetingMinutes}" list="NotesAndAttachments" />
</apex:pageBlockSection>

    
</apex:pageBlock>  
  
  
</apex:page>