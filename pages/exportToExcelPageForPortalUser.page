<apex:page controller="GF_DynamicQueryHandler" action="{!exportToExcelForPortalUser}" readOnly="true" contentType="application/octet-stream#Report.xls">

<table border='1'>
   <tr>
   <apex:repeat value="{!lstcollumnsLabel}" var="head">
      <td><B> {!head} </B></td>
   
   </apex:repeat>
   </tr>
   <apex:repeat value="{!lstSObject}" var="col"> 
   <tr>
       <apex:repeat value="{!lstcollumns}" var="val">
          <td> {!col[val]}</td>
       </apex:repeat>
       </tr>
   </apex:repeat>
   </table>
   
</apex:page>