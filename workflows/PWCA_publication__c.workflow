        <fullName>PWCA Publish Document</fullName>
        <actions>
            <name>UpdateRecordTypeWithPublish</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>PWCA_publication__c.Reviewer_Needed__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <criteriaItems>
            <field>PWCA_publication__c.Ready_To_Publish__c</field>
            <operation>notEqual</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>PWCA_publication__c.CountOfAssCustomer__c</field>
            <operation>greaterThan</operation>
            <value>0</value>
        </criteriaItems>
        <criteriaItems>
            <field>PWCA_publication__c.CountOfAttachment__c</field>
            <operation>greaterThan</operation>
            <value>0</value>
