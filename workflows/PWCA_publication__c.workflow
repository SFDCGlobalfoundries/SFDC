    <rules>
        <fullName>PWCA Attach Document</fullName>
        <actions>
            <name>PWCA_Lock_Record</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>PWCA_Attach_Document</name>
            <type>OutboundMessage</type>
        </actions>
        <active>false</active>
        <booleanFilter>(1 AND 3) OR( 2 AND 3)</booleanFilter>
        <criteriaItems>
            <field>PWCA_publication__c.Ready_To_Publish__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>PWCA_publication__c.Reviewer_Needed__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <criteriaItems>
            <field>PWCA_publication__c.Ready_To_Publish__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
