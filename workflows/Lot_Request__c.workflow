        <fullName>LotServiceReq%3ARecordType%3AFullyApproved</fullName>
        <actions>
            <name>Update_RecordType_to_AppUE</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Lot_Request__c.Stage__c</field>
            <operation>equals</operation>
            <value>Fully Approved,Under Execution</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>LotServiceReq%3AStage%3AClosedFailToExecute</fullName>
        <actions>
            <name>LotServiceReq_TimeOfClosure_NOW</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Lot_Request_Closed_Failed_to_Execute</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND( ISPICKVAL(Stage__c, &apos;Fully Approved&apos;), No_of_Passed_Instructions__c + No_of_Missed_Instructions__c = Customer_Requested_Lot_Qty__c, No_of_Passed_Instructions__c = 0, No_of_Missed_Instructions__c = Customer_Requested_Lot_Qty__c )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>LotServiceReq%3AStage%3AClosedFullyExecuted</fullName>
        <actions>
            <name>LotServiceReq_TimeOfClosure_NOW</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Lot_Request_Closed_Fully_Executed</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND( ISPICKVAL(Stage__c, &apos;Fully Approved&apos;), No_of_Passed_Instructions__c = Customer_Requested_Lot_Qty__c )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>LotServiceReq%3AStage%3AClosedPartialExecuted</fullName>
        <actions>
            <name>LotServiceReq_TimeOfClosure_NOW</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Lot_Request_Closed_Partially_Executed</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND( ISPICKVAL(Stage__c, &apos;Fully Approved&apos;), No_of_Passed_Instructions__c + No_of_Missed_Instructions__c = Customer_Requested_Lot_Qty__c, No_of_Passed_Instructions__c &gt; 0, No_of_Missed_Instructions__c &gt; 0 )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Original Customer Requested Fabout Date</fullName>
        <actions>
            <name>Populate_Original_FaboutDate</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>IF( ISBLANK(PRIORVALUE(Current_Customer_Requested_Fabout_Date__c))&amp;&amp; !ISBLANK(Current_Customer_Requested_Fabout_Date__c),true,false)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Preapproved Expedite Fabs Written to Lot Request</fullName>
        <actions>
            <name>Expedite_Fab_List_from_Account</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>Sets the Pre Approved Expedite Fab from Account.</description>
        <formula>OR (Account__c &lt;&gt; NULL,Account__c &lt;&gt; PRIORVALUE(Account__c) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Pricing Alert to Review Lot Service Request Pricing</fullName>
        <actions>
            <name>Send_Lot_Service_Request_Communication_to_Pricing_Group_via_email</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <booleanFilter>(1 OR 2) AND 3</booleanFilter>
        <criteriaItems>
            <field>Lot_Request__c.Stage__c</field>
            <operation>equals</operation>
            <value>Submitted for Review by Expedite Manager</value>
        </criteriaItems>
        <criteriaItems>
            <field>Lot_Request__c.Stage__c</field>
            <operation>equals</operation>
            <value>Resubmitted by Expedite Manager</value>
        </criteriaItems>
        <criteriaItems>
            <field>Lot_Request__c.Does_Customer_Accept_Standard_Pricing__c</field>
            <operation>equals</operation>
            <value>No</value>
        </criteriaItems>
        <description>Alert Pricing Team that they need to take action on a lot service request due to customer not accepting standard pricing.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Set Acknowledge Execution to True</fullName>
        <actions>
            <name>Set_Acknowledge_Execution_to_True</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Lot_Request__c.Planning_Approved_Lot_with_PE_code__c</field>
            <operation>greaterOrEqual</operation>
            <value>1</value>
        </criteriaItems>
        <description>Sets Acknowledge Execution to true when a lot identified by Planning for expedite has its lot type changed to PE</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Stage to %27Fully Approved%27 after Repricing</fullName>
        <actions>
            <name>Lot_Request_Fully_Approved</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_RecordType_to_Initial</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>AND(      ISPICKVAL(PRIORVALUE(Stage__c),&quot;Submitted for Repricing&quot;),      !ISBLANK(Pricing_Approved_for_Repricing__c),       Acknowledge_Execution__c == False )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Stage to %27Under Execution%27 after Repricing</fullName>
        <actions>
            <name>Lot_Request_Fully_Approved_and_Acknowled</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_RecordType_to_Initial</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>AND(      ISPICKVAL(PRIORVALUE(Stage__c),&quot;Submitted for Repricing&quot;),      !ISBLANK(Pricing_Approved_for_Repricing__c),       Acknowledge_Execution__c == True )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
