    <fieldUpdates>
        <fullName>IP_Form_Reset_Record_Type</fullName>
        <description>This field flags the record to reset the record type to &apos;Master. Resetting is done thru object trigger.</description>
        <field>Reset_Record_Type__c</field>
        <literalValue>1</literalValue>
        <name>IP Form - Reset Record Type</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
    </fieldUpdates>
