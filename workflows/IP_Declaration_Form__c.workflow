    <rules>
        <fullName>IP Declaration - Email Pending Reconciliation</fullName>
        <active>false</active>
        <criteriaItems>
            <field>IP_Declaration_Form__c.Status__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>IP_Declaration_Pending_Reconciliation</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>IP_Declaration_Form__c.Form_Submitted_Date__c</offsetFromField>
            <timeLength>3</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>IP Declaration Email - Approved</fullName>
        <actions>
            <name>IP_Declaration_Approved</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>IP_Declaration_Form__c.Status__c</field>
            <operation>equals</operation>
            <value>Approved</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>IP Declaration Email - Change Ownership</fullName>
