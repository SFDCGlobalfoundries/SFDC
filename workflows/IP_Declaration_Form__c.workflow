    </rules>
    <rules>
        <fullName>IP Declaration Form - Update Device</fullName>
        <actions>
            <name>IPDeclaration_Update_Status_on_Device</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>OR (         ISNEW(), 	ISCHANGED(Status__c)     )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
