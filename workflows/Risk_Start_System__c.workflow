    <rules>
        <fullName>%2Fconversion Complete Date Population</fullName>
        <actions>
            <name>Conversion_Complete_Date_Population</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Risk_Start_System__c.Status__c</field>
            <operation>equals</operation>
            <value>Conversion Completed</value>
        </criteriaItems>
        <description>The Converion Complete Date is populated when then stage moves to &quot;Conversion Complete&quot;</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Conversion Progress Update of Risk Start System</fullName>
        <actions>
            <name>Conversion_InProgress_Action</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Conversion_Status_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>(1 OR 3 OR 4 OR 5 OR 6 OR 7 OR 8 OR 9 OR 10 OR 11 OR 12 OR 13) AND 2 AND 14</booleanFilter>
        <criteriaItems>
            <field>Risk_Start_System__c.PO_Arrival_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Risk_Start_System__c.Status__c</field>
            <operation>equals</operation>
            <value>Approved &amp; Pending Conversion</value>
        </criteriaItems>
        <criteriaItems>
            <field>Risk_Start_System__c.SO_QTY__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Risk_Start_System__c.Void_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Risk_Start_System__c.Scrap_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Risk_Start_System__c.Reason_for_Scrap__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Risk_Start_System__c.Reason_for_Void__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Risk_Start_System__c.Write_Off_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Risk_Start_System__c.Reason_for_Write_Off__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Risk_Start_System__c.Total_Scrap_Qty__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Risk_Start_System__c.Total_PO_Quantity__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Risk_Start_System__c.Total_Write_Off_Qty__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Risk_Start_System__c.Total_Void_Qty__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Risk_Start_System__c.Balance_Qty__c</field>
            <operation>notEqual</operation>
            <value>0</value>
        </criteriaItems>
        <description>Conversion Progress Update of Risk Start System</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Final Update of Risk Start System</fullName>
        <actions>
            <name>Risk_Start_System_Conversion_Complete</name>
            <type>Alert</type>
        </actions>
